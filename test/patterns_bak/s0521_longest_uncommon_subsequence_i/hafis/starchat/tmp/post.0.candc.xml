<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="S[dcl]">
  <rule type="fa" cat="S[X]/S[X]">
   <lf start="0" span="1" word="If" lemma="if" pos="IN" chunk="I-SBAR" entity="O" cat="(S/S)/S[dcl]" />
   <rule type="ba" cat="S[dcl]">
    <rule type="fa" cat="NP[nb]">
     <lf start="1" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <rule type="ba" cat="N">
      <rule type="fa" cat="N">
       <lf start="2" span="1" word="type_string_" lemma="type_string_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="3" span="1" word="param_a_" lemma="param_a_" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
      <rule type="conj" cat="N\N">
       <lf start="4" span="1" word="and" lemma="and" pos="CC" chunk="I-NP" entity="O" cat="conj" />
       <lf start="5" span="1" word="param_b_" lemma="param_b_" pos="NN" chunk="I-NP" entity="I-ORG" cat="N" />
      </rule>
     </rule>
    </rule>
    <rule type="fa" cat="S[dcl]\NP">
     <rule type="bx" cat="(S[dcl]\NP)/(S[adj]\NP)">
      <lf start="6" span="1" word="are" lemma="be" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
      <lf start="7" span="1" word="not" lemma="not" pos="RB" chunk="O" entity="O" cat="(S\NP)\(S\NP)" />
     </rule>
     <lf start="8" span="1" word="equal" lemma="equal" pos="JJ" chunk="I-ADJP" entity="O" cat="S[adj]\NP" />
    </rule>
   </rule>
  </rule>
  <rule type="lp" cat="S[dcl]">
   <lf start="9" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
   <rule type="rp" cat="S[dcl]">
    <rule type="ba" cat="S[dcl]">
     <rule type="fa" cat="NP[nb]">
      <lf start="10" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <rule type="fa" cat="N">
       <lf start="11" span="1" word="type_integer_" lemma="type_integer_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="12" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <rule type="ba" cat="S[dcl]\NP">
      <rule type="fa" cat="S[dcl]\NP">
       <lf start="13" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
       <rule type="fa" cat="S[adj]\NP">
        <lf start="14" span="1" word="greater_than_or_equal" lemma="greater_than_or_equal" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
        <rule type="fa" cat="PP">
         <lf start="15" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
         <rule type="lex" cat="NP">
          <lf start="16" span="1" word="1" lemma="1" pos="CD" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
      <rule type="conj" cat="(S[dcl]\NP)\(S[dcl]\NP)">
       <lf start="17" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
       <rule type="fa" cat="S[dcl]\NP">
        <lf start="18" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
        <rule type="fa" cat="S[adj]\NP">
         <lf start="19" span="1" word="less_than_or_equal" lemma="less_than_or_equal" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
         <rule type="fa" cat="PP">
          <lf start="20" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
          <rule type="ba" cat="NP">
           <rule type="fa" cat="NP[nb]">
            <lf start="21" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
            <lf start="22" span="1" word="length" lemma="length" pos="NN" chunk="I-NP" entity="O" cat="N" />
           </rule>
           <rule type="fa" cat="NP\NP">
            <lf start="23" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
            <rule type="fa" cat="NP[nb]">
             <lf start="24" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
             <rule type="fa" cat="N">
              <lf start="25" span="1" word="longer" lemma="longer" pos="JJR" chunk="I-NP" entity="O" cat="N/N" />
              <lf start="26" span="1" word="string" lemma="string" pos="NN" chunk="I-NP" entity="O" cat="N" />
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
    <lf start="27" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
   </rule>
  </rule>
 </rule>
</ccg>
</candc>
