<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="S[dcl]">
  <rule type="fa" cat="S[X]/S[X]">
   <lf start="0" span="1" word="If" lemma="if" pos="IN" chunk="I-SBAR" entity="O" cat="(S/S)/S[dcl]" />
   <rule type="ba" cat="S[dcl]">
    <lf start="1" span="1" word="there" lemma="there" pos="EX" chunk="I-NP" entity="O" cat="NP[thr]" />
    <lf start="2" span="1" word="exist" lemma="exist" pos="VBP" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
   </rule>
  </rule>
  <rule type="fa" cat="S[dcl]">
   <rule type="rtc" cat="S[X]/S[X]">
    <rule type="lex" cat="NP">
     <rule type="fa" cat="N">
      <lf start="3" span="1" word="multiple" lemma="multiple" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
      <rule type="fa" cat="N">
       <lf start="4" span="1" word="valid" lemma="valid" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="5" span="1" word="answers" lemma="answer" pos="NNS" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
    <lf start="6" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
   </rule>
   <rule type="rp" cat="S[dcl]">
    <rule type="ba" cat="S[dcl]">
     <rule type="ba" cat="NP">
      <lf start="7" span="1" word="any" lemma="any" pos="DT" chunk="I-NP" entity="O" cat="NP" />
      <rule type="fa" cat="NP\NP">
       <lf start="8" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
       <lf start="9" span="1" word="them" lemma="they" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
      </rule>
     </rule>
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="10" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pss]\NP)" />
      <rule type="fa" cat="S[pss]\NP">
       <lf start="11" span="1" word="returned" lemma="return" pos="VBN" chunk="I-VP" entity="O" cat="(S[pss]\NP)/PP" />
       <rule type="fa" cat="PP">
        <lf start="12" span="1" word="as" lemma="as" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
        <rule type="fa" cat="NP[nb]">
         <lf start="13" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <lf start="14" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
    <lf start="15" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
   </rule>
  </rule>
 </rule>
</ccg>
</candc>
