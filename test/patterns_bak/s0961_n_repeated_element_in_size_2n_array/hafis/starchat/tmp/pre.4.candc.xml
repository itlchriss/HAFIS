<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="ba" cat="NP">
    <rule type="lex" cat="NP">
     <rule type="fa" cat="N">
      <lf start="0" span="1" word="Exactly" lemma="exactly" pos="RB" chunk="I-NP" entity="O" cat="N/N" />
      <rule type="fa" cat="N">
       <lf start="1" span="1" word="1" lemma="1" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="2" span="1" word="element" lemma="element" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
    <rule type="fa" cat="NP\NP">
     <lf start="3" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="4" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <rule type="fa" cat="N">
       <lf start="5" span="1" word="type_integer_array_" lemma="type_integer_array_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="6" span="1" word="param_nums_" lemma="param_nums_" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="7" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pss]\NP)" />
    <rule type="ba" cat="S[pss]\NP">
     <lf start="8" span="1" word="repeated" lemma="repeat" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="9" span="1" word="n" lemma="n" pos="JJ" chunk="I-NP" entity="O" cat="((S\NP)\(S\NP))/((S\NP)\(S\NP))" />
      <lf start="10" span="1" word="times" lemma="time" pos="NNS" chunk="I-NP" entity="O" cat="(S\NP)\(S\NP)" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="11" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
