<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="ba" cat="S[dcl]">
  <rule type="fa" cat="NP[nb]">
   <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
   <rule type="fa" cat="N">
    <lf start="1" span="1" word="type_integer_" lemma="type_integer_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
    <lf start="2" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
  </rule>
  <rule type="ba" cat="S[dcl]\NP">
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
    <rule type="fa" cat="S[adj]\NP">
     <lf start="4" span="1" word="equal" lemma="equal" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
     <rule type="fa" cat="PP">
      <lf start="5" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
      <rule type="fa" cat="NP[nb]">
       <rule type="ba" cat="NP[nb]/N">
        <rule type="fa" cat="NP[nb]">
         <lf start="6" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <rule type="fa" cat="N">
          <lf start="7" span="1" word="starting" lemma="start" pos="VBG" chunk="I-NP" entity="O" cat="N/N" />
          <rule type="fa" cat="N">
           <lf start="8" span="1" word="gas" lemma="gas" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
           <lf start="9" span="1" word="station" lemma="station" pos="NN" chunk="I-NP" entity="O" cat="N" />
          </rule>
         </rule>
        </rule>
        <lf start="10" span="1" word="'s" lemma="'s" pos="POS" chunk="B-NP" entity="O" cat="(NP[nb]/N)\NP" />
       </rule>
       <lf start="11" span="1" word="index" lemma="index" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
   <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
    <lf start="12" span="1" word="if" lemma="if" pos="IN" chunk="I-SBAR" entity="O" cat="((S\NP)\(S\NP))/S[dcl]" />
    <rule type="rp" cat="S[dcl]">
     <rule type="ba" cat="S[dcl]">
      <lf start="13" span="1" word="it" lemma="it" pos="PRP" chunk="I-NP" entity="O" cat="NP[expl]" />
      <rule type="fa" cat="S[dcl]\NP[expl]">
       <rule type="fa" cat="(S[dcl]\NP[expl])/(S[to]\NP)">
        <lf start="14" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="((S[dcl]\NP[expl])/(S[to]\NP))/(S[adj]\NP)" />
        <lf start="15" span="1" word="possible" lemma="possible" pos="JJ" chunk="I-ADJP" entity="O" cat="S[adj]\NP" />
       </rule>
       <rule type="fa" cat="S[to]\NP">
        <lf start="16" span="1" word="to" lemma="to" pos="TO" chunk="I-VP" entity="O" cat="(S[to]\NP)/(S[b]\NP)" />
        <rule type="ba" cat="S[b]\NP">
         <rule type="ba" cat="S[b]\NP">
          <rule type="ba" cat="S[b]\NP">
           <lf start="17" span="1" word="travel" lemma="travel" pos="VB" chunk="I-VP" entity="O" cat="S[b]\NP" />
           <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
            <lf start="18" span="1" word="around" lemma="around" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
            <rule type="fa" cat="NP[nb]">
             <lf start="19" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
             <lf start="20" span="1" word="circuit" lemma="circuit" pos="NN" chunk="I-NP" entity="O" cat="N" />
            </rule>
           </rule>
          </rule>
          <lf start="21" span="1" word="once" lemma="once" pos="RB" chunk="I-ADVP" entity="O" cat="(S\NP)\(S\NP)" />
         </rule>
         <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
          <lf start="22" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
          <rule type="fa" cat="NP[nb]">
           <lf start="23" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
           <rule type="fa" cat="N">
            <lf start="24" span="1" word="clockwise" lemma="clockwise" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
            <lf start="25" span="1" word="direction" lemma="direction" pos="NN" chunk="I-NP" entity="O" cat="N" />
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
     <lf start="26" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
    </rule>
   </rule>
  </rule>
 </rule>
</ccg>
</candc>
