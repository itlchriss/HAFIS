<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="type_string_" lemma="type_string_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/PP" />
    <rule type="fa" cat="PP">
     <lf start="4" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
     <rule type="ba" cat="NP">
      <rule type="rp" cat="NP[nb]">
       <rule type="fa" cat="NP[nb]">
        <lf start="5" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <rule type="fa" cat="N">
         <lf start="6" span="1" word="format" lemma="format" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="7" span="1" word="stry_a" lemma="stry_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
       <lf start="8" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
      </rule>
      <rule type="fa" cat="NP\NP">
       <lf start="9" span="1" word="that" lemma="that" pos="IN" chunk="I-SBAR" entity="O" cat="(NP\NP)/S[dcl]" />
       <rule type="ba" cat="S[dcl]">
        <rule type="ba" cat="S[dcl]">
         <rule type="lex" cat="NP">
          <lf start="10" span="1" word="x" lemma="x" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
         <rule type="fa" cat="S[dcl]\NP">
          <lf start="11" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
          <rule type="ba" cat="NP">
           <rule type="fa" cat="NP[nb]">
            <lf start="12" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
            <lf start="13" span="1" word="number" lemma="number" pos="NN" chunk="I-NP" entity="O" cat="N" />
           </rule>
           <rule type="fa" cat="NP\NP">
            <lf start="14" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
            <rule type="lex" cat="NP">
             <lf start="15" span="1" word="bulls" lemma="bull" pos="NNS" chunk="I-NP" entity="O" cat="N" />
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
        <rule type="conj" cat="S[dcl]\S[dcl]">
         <lf start="16" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
         <rule type="ba" cat="S[dcl]">
          <rule type="lex" cat="NP">
           <lf start="17" span="1" word="y" lemma="y" pos="NN" chunk="I-NP" entity="O" cat="N" />
          </rule>
          <rule type="fa" cat="S[dcl]\NP">
           <lf start="18" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
           <rule type="ba" cat="NP">
            <rule type="fa" cat="NP[nb]">
             <lf start="19" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
             <lf start="20" span="1" word="number" lemma="number" pos="NN" chunk="I-NP" entity="O" cat="N" />
            </rule>
            <rule type="fa" cat="NP\NP">
             <lf start="21" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
             <rule type="lex" cat="NP">
              <lf start="22" span="1" word="cows" lemma="cow" pos="NNS" chunk="I-NP" entity="O" cat="N" />
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="23" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
