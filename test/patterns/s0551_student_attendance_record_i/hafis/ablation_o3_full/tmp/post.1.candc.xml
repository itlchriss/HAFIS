<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="ba" cat="NP">
  <rule type="lex" cat="NP">
   <lf start="0" span="1" word="Behaviour" lemma="Behaviour" pos="NNP" chunk="I-NP" entity="O" cat="N" />
  </rule>
  <rule type="fa" cat="NP\NP">
   <lf start="1" span="1" word=":" lemma=":" pos="IN" chunk="O" entity="O" cat="(NP\NP)/S[dcl]" />
   <rule type="fa" cat="S[dcl]">
    <rule type="fa" cat="S[X]/S[X]">
     <lf start="2" span="1" word="If" lemma="if" pos="IN" chunk="I-SBAR" entity="O" cat="(S/S)/S[dcl]" />
     <rule type="ba" cat="S[dcl]">
      <rule type="fa" cat="NP[nb]">
       <lf start="3" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="4" span="1" word="chry_a" lemma="chry_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
      <rule type="ba" cat="S[dcl]\NP">
       <rule type="fa" cat="S[dcl]\NP">
        <lf start="5" span="1" word="contains" lemma="contain" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
        <rule type="lex" cat="NP">
         <rule type="ba" cat="N">
          <lf start="6" span="1" word="2" lemma="2" pos="CD" chunk="I-NP" entity="O" cat="N" />
          <rule type="conj" cat="N\N">
           <lf start="7" span="1" word="or" lemma="or" pos="CC" chunk="I-NP" entity="O" cat="conj" />
           <rule type="fa" cat="N">
            <lf start="8" span="1" word="more" lemma="more" pos="JJR" chunk="I-NP" entity="O" cat="N/N" />
            <rule type="fa" cat="N">
             <lf start="9" span="1" word="characters" lemma="character" pos="NNS" chunk="I-NP" entity="O" cat="N/N" />
             <lf start="10" span="1" word="A" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="N" />
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
       <rule type="conj" cat="(S[dcl]\NP)\(S[dcl]\NP)">
        <lf start="11" span="1" word="or" lemma="or" pos="CC" chunk="O" entity="O" cat="conj" />
        <rule type="fa" cat="S[dcl]\NP">
         <lf start="12" span="1" word="contains" lemma="contain" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
         <rule type="ba" cat="NP">
          <rule type="fa" cat="NP[nb]">
           <lf start="13" span="1" word="any" lemma="any" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
           <lf start="14" span="1" word="sequence" lemma="sequence" pos="NN" chunk="I-NP" entity="O" cat="N" />
          </rule>
          <rule type="fa" cat="NP\NP">
           <lf start="15" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
           <rule type="lex" cat="NP">
            <rule type="fa" cat="N">
             <lf start="16" span="1" word="3" lemma="3" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
             <rule type="fa" cat="N">
              <lf start="17" span="1" word="consecutive" lemma="consecutive" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
              <lf start="18" span="1" word="characters" lemma="character" pos="NNS" chunk="I-NP" entity="O" cat="N" />
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
    <rule type="rp" cat="S[dcl]">
     <rule type="ba" cat="S[dcl]">
      <rule type="lex" cat="NP">
       <rule type="fa" cat="N">
        <lf start="19" span="1" word="L" lemma="L" pos="NNP" chunk="B-NP" entity="I-ORG" cat="N/N" />
        <rule type="fa" cat="N">
         <rule type="fa" cat="N/N">
          <lf start="20" span="1" word="then" lemma="then" pos="RB" chunk="I-NP" entity="O" cat="(N/N)/(N/N)" />
          <lf start="21" span="1" word="type_boolean_" lemma="type_boolean_" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
         </rule>
         <lf start="22" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
      <rule type="fa" cat="S[dcl]\NP">
       <lf start="23" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
       <rule type="ba" cat="S[adj]\NP">
        <lf start="24" span="1" word="equal" lemma="equal" pos="JJ" chunk="I-ADJP" entity="O" cat="S[adj]\NP" />
        <rule type="fa" cat="(S[adj]\NP)\(S[adj]\NP)">
         <lf start="25" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="((S[adj]\NP)\(S[adj]\NP))/NP" />
         <rule type="fa" cat="NP[nb]">
          <lf start="26" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
          <rule type="fa" cat="N">
           <lf start="27" span="1" word="false" lemma="false" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
           <lf start="28" span="1" word="literal" lemma="literal" pos="NN" chunk="I-NP" entity="O" cat="N" />
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
     <lf start="29" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
    </rule>
   </rule>
  </rule>
 </rule>
</ccg>
</candc>
