<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="S[dcl]">
  <rule type="fa" cat="S[X]/S[X]">
   <lf start="0" span="1" word="if" lemma="if" pos="IN" chunk="I-SBAR" entity="O" cat="(S/S)/S[dcl]" />
   <rule type="ba" cat="S[dcl]">
    <rule type="lex" cat="NP">
     <rule type="fa" cat="N">
      <lf start="1" span="1" word="Stringthe" lemma="Stringthe" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N/N" />
      <lf start="2" span="1" word="chry_a" lemma="chry_a" pos="NN" chunk="I-NP" entity="I-LOC" cat="N" />
     </rule>
    </rule>
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="3" span="1" word="contains" lemma="contain" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
     <rule type="ba" cat="NP">
      <rule type="fa" cat="NP[nb]">
       <lf start="4" span="1" word="any" lemma="any" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="5" span="1" word="character" lemma="character" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
      <rule type="lex" cat="NP\NP">
       <rule type="ba" cat="S[adj]\NP">
        <lf start="6" span="1" word="other" lemma="other" pos="JJ" chunk="I-ADJP" entity="O" cat="S[adj]\NP" />
        <rule type="fa" cat="(S[adj]\NP)\(S[adj]\NP)">
         <lf start="7" span="1" word="than" lemma="than" pos="IN" chunk="I-PP" entity="O" cat="((S[adj]\NP)\(S[adj]\NP))/NP" />
         <rule type="ba" cat="NP">
          <rule type="fa" cat="NP[nb]">
           <lf start="8" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
           <lf start="9" span="1" word="digits" lemma="digit" pos="NNS" chunk="I-NP" entity="O" cat="N" />
          </rule>
          <rule type="lex" cat="NP\NP">
           <rule type="fa" cat="S[dcl]\NP">
            <lf start="10" span="1" word="+" lemma="+" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
            <rule type="ba" cat="NP">
             <rule type="lex" cat="NP">
              <lf start="11" span="1" word="sign" lemma="sign" pos="NN" chunk="I-NP" entity="O" cat="N" />
             </rule>
             <rule type="fa" cat="NP\NP">
              <lf start="12" span="1" word="-" lemma="-" pos="IN" chunk="O" entity="O" cat="(NP\NP)/NP" />
              <rule type="ba" cat="NP">
               <rule type="ba" cat="NP">
                <rule type="lex" cat="NP">
                 <lf start="13" span="1" word="sign" lemma="sign" pos="NN" chunk="I-NP" entity="O" cat="N" />
                </rule>
                <rule type="fa" cat="NP\NP">
                 <lf start="14" span="1" word="dot" lemma="dot" pos="NN" chunk="I-NP" entity="O" cat="(NP\NP)/(NP\NP)" />
                 <rule type="fa" cat="NP\NP">
                  <lf start="15" span="1" word="uppercase" lemma="uppercase" pos="NN" chunk="I-NP" entity="O" cat="(NP\NP)/(NP\NP)" />
                  <lf start="16" span="1" word="E" lemma="e" pos="NN" chunk="I-NP" entity="O" cat="NP\NP" />
                 </rule>
                </rule>
               </rule>
               <rule type="conj" cat="NP\NP">
                <lf start="17" span="1" word="or" lemma="or" pos="CC" chunk="I-NP" entity="O" cat="conj" />
                <rule type="lex" cat="NP">
                 <rule type="fa" cat="N">
                  <lf start="18" span="1" word="lowercase" lemma="lowercase" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
                  <lf start="19" span="1" word="e" lemma="e" pos="NN" chunk="I-NP" entity="O" cat="N" />
                 </rule>
                </rule>
               </rule>
              </rule>
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <rule type="fa" cat="S[dcl]">
   <lf start="20" span="1" word="then" lemma="then" pos="RB" chunk="I-ADVP" entity="O" cat="S/S" />
   <rule type="rp" cat="S[dcl]">
    <rule type="ba" cat="S[dcl]">
     <rule type="lex" cat="NP">
      <rule type="fa" cat="N">
       <lf start="21" span="1" word="type_boolean_" lemma="type_boolean_" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="22" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="23" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
      <rule type="fa" cat="S[adj]\NP">
       <lf start="24" span="1" word="equal" lemma="equal" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
       <rule type="fa" cat="PP">
        <lf start="25" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
        <rule type="fa" cat="NP[nb]">
         <lf start="26" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <rule type="fa" cat="N">
          <lf start="27" span="1" word="false" lemma="false" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="28" span="1" word="literal" lemma="literal" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
    <lf start="29" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
   </rule>
  </rule>
 </rule>
</ccg>
</candc>
