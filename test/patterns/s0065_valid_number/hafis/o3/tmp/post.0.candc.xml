<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="S[dcl]">
  <rule type="fa" cat="S[X]/S[X]">
   <lf start="0" span="1" word="If" lemma="if" pos="IN" chunk="I-SBAR" entity="O" cat="(S/S)/S[dcl]" />
   <rule type="ba" cat="S[dcl]">
    <rule type="lex" cat="NP">
     <rule type="fa" cat="N">
      <lf start="1" span="1" word="type_boolean_" lemma="type_boolean_" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="2" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="3" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
     <rule type="fa" cat="S[adj]\NP">
      <lf start="4" span="1" word="equal" lemma="equal" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
      <rule type="fa" cat="PP">
       <lf start="5" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
       <rule type="fa" cat="NP[nb]">
        <lf start="6" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <rule type="fa" cat="N">
         <lf start="7" span="1" word="true" lemma="true" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="8" span="1" word="literal" lemma="literal" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <rule type="rp" cat="S[dcl]">
   <rule type="lp" cat="S[dcl]">
    <lf start="9" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
    <rule type="ba" cat="S[dcl]">
     <rule type="fa" cat="NP[nb]">
      <lf start="10" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <rule type="fa" cat="N">
       <lf start="11" span="1" word="type_string_" lemma="type_string_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="12" span="1" word="param_s_" lemma="param_s_" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="13" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
      <rule type="fa" cat="S[adj]\NP">
       <lf start="14" span="1" word="equal" lemma="equal" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
       <rule type="fa" cat="PP">
        <lf start="15" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
        <rule type="ba" cat="NP">
         <rule type="lex" cat="NP">
          <rule type="fa" cat="N">
           <lf start="16" span="1" word="decimal" lemma="decimal" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
           <lf start="17" span="1" word="number" lemma="number" pos="NN" chunk="I-NP" entity="O" cat="N" />
          </rule>
         </rule>
         <rule type="conj" cat="NP\NP">
          <lf start="18" span="1" word="or" lemma="or" pos="CC" chunk="O" entity="O" cat="conj" />
          <rule type="ba" cat="NP">
           <rule type="fa" cat="NP[nb]">
            <lf start="19" span="1" word="an" lemma="an" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
            <lf start="20" span="1" word="integer" lemma="integer" pos="NN" chunk="I-NP" entity="O" cat="N" />
           </rule>
           <rule type="fa" cat="NP\NP">
            <lf start="21" span="1" word="that" lemma="that" pos="WDT" chunk="B-NP" entity="O" cat="(NP\NP)/(S[dcl]\NP)" />
            <rule type="fa" cat="S[dcl]\NP">
             <lf start="22" span="1" word="may" lemma="may" pos="MD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[b]\NP)" />
             <rule type="fa" cat="S[b]\NP">
              <lf start="23" span="1" word="be" lemma="be" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/(S[pss]\NP)" />
              <rule type="ba" cat="S[pss]\NP">
               <lf start="24" span="1" word="followed" lemma="follow" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
               <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
                <lf start="25" span="1" word="by" lemma="by" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
                <rule type="ba" cat="NP[nb]">
                 <rule type="fa" cat="NP[nb]">
                  <lf start="26" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
                  <rule type="fa" cat="N">
                   <lf start="27" span="1" word="character" lemma="character" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
                   <lf start="28" span="1" word="e" lemma="e" pos="NN" chunk="I-NP" entity="O" cat="N" />
                  </rule>
                 </rule>
                 <rule type="conj" cat="NP[nb]\NP[nb]">
                  <lf start="29" span="1" word="or" lemma="or" pos="CC" chunk="O" entity="O" cat="conj" />
                  <rule type="ba" cat="NP[nb]">
                   <rule type="fa" cat="NP[nb]">
                    <lf start="30" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
                    <rule type="fa" cat="N">
                     <lf start="31" span="1" word="character" lemma="character" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
                     <lf start="32" span="1" word="E" lemma="e" pos="NN" chunk="I-NP" entity="I-ORG" cat="N" />
                    </rule>
                   </rule>
                   <rule type="conj" cat="NP[nb]\NP[nb]">
                    <lf start="33" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
                    <rule type="fa" cat="NP[nb]">
                     <lf start="34" span="1" word="an" lemma="an" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
                     <lf start="35" span="1" word="integer" lemma="integer" pos="NN" chunk="I-NP" entity="O" cat="N" />
                    </rule>
                   </rule>
                  </rule>
                 </rule>
                </rule>
               </rule>
              </rule>
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
   <lf start="36" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
</candc>
