<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <rule type="ba" cat="NP[nb]/N">
     <rule type="fa" cat="NP[nb]">
      <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <rule type="fa" cat="N">
       <lf start="1" span="1" word="type_string_" lemma="type_string_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="2" span="1" word="param_s_" lemma="param_s_" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <lf start="3" span="1" word="'s" lemma="'s" pos="POS" chunk="B-NP" entity="O" cat="(NP[nb]/N)\NP" />
    </rule>
    <lf start="4" span="1" word="values" lemma="value" pos="NNS" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="5" span="1" word="are" lemma="be" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="fa" cat="NP">
     <lf start="6" span="1" word="either" lemma="either" pos="DT" chunk="I-NP" entity="O" cat="NP/NP" />
     <rule type="ba" cat="NP[nb]">
      <rule type="ba" cat="NP">
       <rule type="lex" cat="NP">
        <lf start="7" span="1" word="uppercase_alphabets" lemma="uppercase_alphabets" pos="NNS" chunk="I-NP" entity="O" cat="N" />
       </rule>
       <rule type="conj" cat="NP\NP">
        <lf start="8" span="1" word="or" lemma="or" pos="CC" chunk="I-NP" entity="O" cat="conj" />
        <rule type="lex" cat="NP">
         <lf start="9" span="1" word="lowercase_alphabets" lemma="lowercase_alphabets" pos="NNS" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
      <rule type="conj" cat="NP[nb]\NP[nb]">
       <lf start="10" span="1" word="or" lemma="or" pos="CC" chunk="O" entity="O" cat="conj" />
       <rule type="ba" cat="NP[nb]">
        <rule type="lex" cat="NP">
         <rule type="fa" cat="N">
          <lf start="11" span="1" word="numeric" lemma="numeric" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="12" span="1" word="digits" lemma="digit" pos="NNS" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
        <rule type="conj" cat="NP[nb]\NP[nb]">
         <lf start="13" span="1" word="or" lemma="or" pos="CC" chunk="O" entity="O" cat="conj" />
         <rule type="ba" cat="NP[nb]">
          <rule type="fa" cat="NP[nb]">
           <lf start="14" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
           <rule type="fa" cat="N">
            <lf start="15" span="1" word="+" lemma="+" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
            <lf start="16" span="1" word="sign" lemma="sign" pos="NN" chunk="I-NP" entity="O" cat="N" />
           </rule>
          </rule>
          <rule type="conj" cat="NP[nb]\NP[nb]">
           <lf start="17" span="1" word="or" lemma="or" pos="CC" chunk="O" entity="O" cat="conj" />
           <rule type="ba" cat="NP[nb]">
            <rule type="fa" cat="NP[nb]">
             <lf start="18" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
             <rule type="lp" cat="N">
              <lf start="19" span="1" word="-" lemma="-" pos=":" chunk="I-NP" entity="O" cat=":" />
              <lf start="20" span="1" word="sign" lemma="sign" pos="NN" chunk="I-NP" entity="O" cat="N" />
             </rule>
            </rule>
            <rule type="conj" cat="NP[nb]\NP[nb]">
             <lf start="21" span="1" word="or" lemma="or" pos="CC" chunk="O" entity="O" cat="conj" />
             <rule type="fa" cat="NP[nb]">
              <lf start="22" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
              <rule type="fa" cat="N">
               <lf start="23" span="1" word="dot" lemma="dot" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
               <lf start="24" span="1" word="character" lemma="character" pos="NN" chunk="I-NP" entity="O" cat="N" />
              </rule>
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="25" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
