<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="int" lemma="int" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="ba" cat="S[dcl]\NP">
    <lf start="3" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
    <rule type="ba" cat="(S[X]\NP)\(S[X]\NP)">
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <rule type="fa" cat="((S[X]\NP)\(S[X]\NP))/((S[X]\NP)\(S[X]\NP))">
       <lf start="4" span="1" word="at" lemma="at" pos="IN" chunk="I-NP" entity="O" cat="(((S\NP)\(S\NP))/((S\NP)\(S\NP)))/(S[asup]\NP)" />
       <lf start="5" span="1" word="least" lemma="least" pos="JJS" chunk="I-NP" entity="O" cat="S[asup]\NP" />
      </rule>
      <lf start="6" span="1" word="0" lemma="0" pos="CD" chunk="I-NP" entity="O" cat="(S\NP)\(S\NP)" />
     </rule>
     <rule type="conj" cat="((S[X]\NP)\(S[X]\NP))\((S[X]\NP)\(S[X]\NP))">
      <lf start="7" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
      <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
       <lf start="8" span="1" word="at" lemma="at" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
       <rule type="lex" cat="NP">
        <rule type="ba" cat="N">
         <lf start="9" span="1" word="most" lemma="most" pos="JJS" chunk="I-NP" entity="O" cat="N" />
         <lf start="10" span="1" word="31" lemma="31" pos="CD" chunk="I-NP" entity="I-DAT" cat="N\N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="11" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
