<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="S[dcl]">
  <rule type="fa" cat="S[X]/S[X]">
   <lf start="0" span="1" word="For" lemma="for" pos="IN" chunk="I-PP" entity="O" cat="(S/S)/NP" />
   <rule type="ba" cat="NP">
    <rule type="fa" cat="NP[nb]">
     <lf start="1" span="1" word="any" lemma="any" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="2" span="1" word="int" lemma="int" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
    <rule type="lex" cat="NP\NP">
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="3" span="1" word="parameters" lemma="parameter" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
      <rule type="ba" cat="NP[nb]">
       <rule type="fa" cat="NP[nb]">
        <lf start="4" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="5" span="1" word="chry_a" lemma="chry_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
       <rule type="conj" cat="NP[nb]\NP[nb]">
        <lf start="6" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
        <rule type="fa" cat="NP[nb]">
         <lf start="7" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <lf start="8" span="1" word="chry_b" lemma="chry_b" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <rule type="rp" cat="S[dcl]">
   <rule type="ba" cat="S[dcl]">
    <rule type="fa" cat="NP[nb]">
     <lf start="9" span="1" word="the" lemma="the" pos="DT" chunk="B-NP" entity="O" cat="NP[nb]/N" />
     <rule type="fa" cat="N">
      <lf start="10" span="1" word="int" lemma="int" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="11" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="12" span="1" word="equals" lemma="equal" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
     <rule type="ba" cat="NP">
      <rule type="fa" cat="NP[nb]">
       <lf start="13" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="14" span="1" word="count" lemma="count" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
      <rule type="fa" cat="NP\NP">
       <lf start="15" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
       <rule type="ba" cat="NP">
        <rule type="lex" cat="NP">
         <rule type="fa" cat="N">
          <lf start="16" span="1" word="bit" lemma="bit" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="17" span="1" word="positions" lemma="position" pos="NNS" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
        <rule type="fa" cat="NP\NP">
         <rule type="ba" cat="(NP\NP)/S[dcl]">
          <lf start="18" span="1" word="at" lemma="at" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
          <lf start="19" span="1" word="which" lemma="which" pos="WDT" chunk="I-NP" entity="O" cat="((NP\NP)/S[dcl])\((NP\NP)/NP)" />
         </rule>
         <rule type="ba" cat="S[dcl]">
          <rule type="ba" cat="NP">
           <rule type="fa" cat="NP[nb]">
            <lf start="20" span="1" word="the" lemma="the" pos="DT" chunk="B-NP" entity="O" cat="NP[nb]/N" />
            <rule type="fa" cat="N">
             <lf start="21" span="1" word="binary" lemma="binary" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
             <lf start="22" span="1" word="representations" lemma="representation" pos="NNS" chunk="I-NP" entity="O" cat="N" />
            </rule>
           </rule>
           <rule type="fa" cat="NP\NP">
            <lf start="23" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
            <rule type="ba" cat="NP[nb]">
             <rule type="fa" cat="NP[nb]">
              <lf start="24" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
              <lf start="25" span="1" word="chry_c" lemma="chry_c" pos="NN" chunk="I-NP" entity="O" cat="N" />
             </rule>
             <rule type="conj" cat="NP[nb]\NP[nb]">
              <lf start="26" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
              <rule type="fa" cat="NP[nb]">
               <lf start="27" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
               <lf start="28" span="1" word="chry_d" lemma="chry_d" pos="NN" chunk="I-NP" entity="O" cat="N" />
              </rule>
             </rule>
            </rule>
           </rule>
          </rule>
          <lf start="29" span="1" word="differ" lemma="differ" pos="VBP" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
   <lf start="30" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
</candc>
