<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="fa" cat="S[dcl]">
   <rule type="fa" cat="S[X]/S[X]">
    <lf start="0" span="1" word="When" lemma="when" pos="WRB" chunk="I-ADVP" entity="O" cat="(S/S)/S[dcl]" />
    <rule type="fa" cat="S[dcl]">
     <lf start="1" span="1" word="int" lemma="int" pos="NN" chunk="I-NP" entity="O" cat="S/S" />
     <rule type="ba" cat="S[dcl]">
      <rule type="fa" cat="NP[nb]">
       <lf start="2" span="1" word="the" lemma="the" pos="DT" chunk="B-NP" entity="O" cat="NP[nb]/N" />
       <rule type="fa" cat="N">
        <lf start="3" span="1" word="type_character_" lemma="type_character_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="4" span="1" word="chrs_a" lemma="chrs_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
      <rule type="fa" cat="S[dcl]\NP">
       <lf start="5" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
       <rule type="fa" cat="S[adj]\NP">
        <lf start="6" span="1" word="such" lemma="such" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/S[em]" />
        <rule type="fa" cat="S[em]">
         <lf start="7" span="1" word="that" lemma="that" pos="IN" chunk="I-SBAR" entity="O" cat="S[em]/S[dcl]" />
         <rule type="ba" cat="S[dcl]">
          <rule type="lex" cat="NP">
           <rule type="fa" cat="S[ng]\NP">
            <lf start="8" span="1" word="reversing" lemma="reverse" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
            <rule type="fa" cat="NP[nb]">
             <lf start="9" span="1" word="its" lemma="its" pos="PRP$" chunk="I-NP" entity="O" cat="NP[nb]/N" />
             <rule type="fa" cat="N">
              <lf start="10" span="1" word="decimal" lemma="decimal" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
              <lf start="11" span="1" word="digits" lemma="digit" pos="NNS" chunk="I-NP" entity="O" cat="N" />
             </rule>
            </rule>
           </rule>
          </rule>
          <rule type="ba" cat="S[dcl]\NP">
           <rule type="fa" cat="S[dcl]\NP">
            <lf start="12" span="1" word="yields" lemma="yield" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
            <rule type="ba" cat="NP">
             <rule type="fa" cat="NP[nb]">
              <lf start="13" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
              <lf start="14" span="1" word="value" lemma="value" pos="NN" chunk="I-NP" entity="O" cat="N" />
             </rule>
             <rule type="fa" cat="NP\NP">
              <lf start="15" span="1" word="outside" lemma="outside" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
              <rule type="ba" cat="NP">
               <rule type="fa" cat="NP[nb]">
                <lf start="16" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
                <lf start="17" span="1" word="range" lemma="range" pos="NN" chunk="I-NP" entity="O" cat="N" />
               </rule>
               <rule type="lex" cat="NP\NP">
                <rule type="ba" cat="S[adj]\NP">
                 <lf start="18" span="1" word="negative" lemma="negative" pos="JJ" chunk="I-ADJP" entity="I-DAT" cat="S[adj]\NP" />
                 <lf start="19" span="1" word="2147483648" lemma="2147483648" pos="CD" chunk="I-NP" entity="I-DAT" cat="(S[adj]\NP)\(S[adj]\NP)" />
                </rule>
               </rule>
              </rule>
             </rule>
            </rule>
           </rule>
           <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
            <lf start="20" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="I-DAT" cat="((S\NP)\(S\NP))/NP" />
            <rule type="lex" cat="NP">
             <lf start="21" span="1" word="2147483647" lemma="2147483647" pos="CD" chunk="I-NP" entity="I-DAT" cat="N" />
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
   <rule type="lp" cat="S[dcl]">
    <lf start="22" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
    <rule type="ba" cat="S[dcl]">
     <rule type="ba" cat="NP">
      <rule type="lex" cat="NP">
       <lf start="23" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
      <lf start="24" span="1" word="int" lemma="int" pos="NN" chunk="I-NP" entity="O" cat="NP\NP" />
     </rule>
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="25" span="1" word="equals" lemma="equal" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
      <rule type="lex" cat="NP">
       <lf start="26" span="1" word="0" lemma="0" pos="CD" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="27" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
