<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="type_integer_" lemma="type_integer_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <rule type="bx" cat="(S[dcl]\NP)/NP">
     <lf start="3" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
     <lf start="4" span="1" word="always" lemma="always" pos="RB" chunk="I-ADVP" entity="O" cat="(S\NP)\(S\NP)" />
    </rule>
    <rule type="ba" cat="NP">
     <rule type="fa" cat="NP[nb]">
      <lf start="5" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <rule type="fa" cat="N">
       <lf start="6" span="1" word="maximum" lemma="maximum" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
       <rule type="fa" cat="N">
        <lf start="7" span="1" word="possible" lemma="possible" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="8" span="1" word="sum" lemma="sum" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="9" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
      <rule type="ba" cat="NP">
       <rule type="fa" cat="NP[nb]">
        <lf start="10" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <rule type="fa" cat="N">
         <lf start="11" span="1" word="minimum" lemma="minimum" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="12" span="1" word="values" lemma="value" pos="NNS" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
       <rule type="fa" cat="NP\NP">
        <lf start="13" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
        <rule type="fa" cat="NP[nb]">
         <lf start="14" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <lf start="15" span="1" word="pairs" lemma="pair" pos="NNS" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="16" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
