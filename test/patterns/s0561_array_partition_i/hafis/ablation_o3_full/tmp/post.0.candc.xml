<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="ba" cat="NP">
    <rule type="lex" cat="NP">
     <lf start="0" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
    <rule type="fa" cat="NP\NP">
     <lf start="1" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
     <rule type="lex" cat="NP">
      <rule type="fa" cat="N">
       <lf start="2" span="1" word="type" lemma="type" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="3" span="1" word="int" lemma="int" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
   <rule type="ba" cat="S[dcl]\NP">
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="4" span="1" word="equals" lemma="equal" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
     <rule type="ba" cat="NP">
      <rule type="fa" cat="NP[nb]">
       <lf start="5" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <rule type="fa" cat="N">
        <lf start="6" span="1" word="maximum" lemma="maximum" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
        <rule type="fa" cat="N">
         <lf start="7" span="1" word="obtainable" lemma="obtainable" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="8" span="1" word="total" lemma="total" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
      <rule type="fa" cat="NP\NP">
       <lf start="9" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
       <rule type="ba" cat="NP">
        <rule type="fa" cat="NP[nb]">
         <lf start="10" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <rule type="fa" cat="N">
          <lf start="11" span="1" word="smaller" lemma="smaller" pos="JJR" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="12" span="1" word="member" lemma="member" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
        <rule type="fa" cat="NP\NP">
         <lf start="13" span="1" word="from" lemma="from" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
         <rule type="fa" cat="NP[nb]">
          <lf start="14" span="1" word="every" lemma="every" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
          <lf start="15" span="1" word="pair" lemma="pair" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
    <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
     <lf start="16" span="1" word="when" lemma="when" pos="WRB" chunk="I-ADVP" entity="O" cat="((S\NP)\(S\NP))/S[dcl]" />
     <rule type="ba" cat="S[dcl]">
      <rule type="ba" cat="NP">
       <rule type="fa" cat="NP[nb]">
        <lf start="17" span="1" word="all" lemma="all" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="18" span="1" word="elements" lemma="element" pos="NNS" chunk="I-NP" entity="O" cat="N" />
       </rule>
       <rule type="fa" cat="NP\NP">
        <lf start="19" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
        <rule type="lex" cat="NP">
         <rule type="fa" cat="N">
          <lf start="20" span="1" word="int" lemma="int" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="21" span="1" word="[" lemma="[" pos="NNS" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
      <rule type="fa" cat="S[dcl]\NP">
       <lf start="22" span="1" word="]" lemma="]" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/S[dcl]" />
       <rule type="ba" cat="S[dcl]">
        <rule type="fa" cat="NP[nb]">
         <lf start="23" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <rule type="fa" cat="N">
          <lf start="24" span="1" word="type_string_" lemma="type_string_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="25" span="1" word="strs_a" lemma="strs_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
        <rule type="fa" cat="S[dcl]\NP">
         <lf start="26" span="1" word="are" lemma="be" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pss]\NP)" />
         <rule type="fa" cat="S[pss]\NP">
          <lf start="27" span="1" word="grouped" lemma="group" pos="VBN" chunk="I-VP" entity="O" cat="(S[pss]\NP)/PP" />
          <rule type="fa" cat="PP">
           <lf start="28" span="1" word="into" lemma="into" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
           <rule type="lex" cat="NP">
            <rule type="fa" cat="N">
             <lf start="29" span="1" word="n" lemma="n" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
             <rule type="fa" cat="N">
              <lf start="30" span="1" word="non-overlapping" lemma="non-overlapping" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
              <lf start="31" span="1" word="pairs" lemma="pair" pos="NNS" chunk="I-NP" entity="O" cat="N" />
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="32" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
