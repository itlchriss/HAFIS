<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="type_string_" lemma="type_string_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="follows" lemma="follow" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="fa" cat="NP[nb]">
     <lf start="4" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <rule type="fa" cat="N">
      <lf start="5" span="1" word="format" lemma="format" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
      <rule type="fa" cat="N">
       <lf start="6" span="1" word="stry_a" lemma="stry_a" pos="NN" chunk="I-NP" entity="O" cat="N/S[em]" />
       <rule type="fa" cat="S[em]">
        <lf start="7" span="1" word="that" lemma="that" pos="IN" chunk="B-NP" entity="O" cat="S[em]/S[dcl]" />
        <rule type="ba" cat="S[dcl]">
         <rule type="lex" cat="NP">
          <rule type="ba" cat="N">
           <lf start="8" span="1" word="x" lemma="x" pos="NN" chunk="I-NP" entity="O" cat="N" />
           <rule type="conj" cat="N\N">
            <lf start="9" span="1" word="and" lemma="and" pos="CC" chunk="I-NP" entity="O" cat="conj" />
            <lf start="10" span="1" word="y" lemma="y" pos="NN" chunk="I-NP" entity="O" cat="N" />
           </rule>
          </rule>
         </rule>
         <rule type="fa" cat="S[dcl]\NP">
          <lf start="11" span="1" word="are" lemma="be" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
          <rule type="fa" cat="S[adj]\NP">
           <lf start="12" span="1" word="greater_than" lemma="greater_than" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
           <rule type="fa" cat="PP">
            <lf start="13" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
            <rule type="lex" cat="NP">
             <rule type="fa" cat="N">
              <lf start="14" span="1" word="0" lemma="0" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
              <lf start="15" span="1" word="integers" lemma="integer" pos="NNS" chunk="I-NP" entity="O" cat="N" />
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="16" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
