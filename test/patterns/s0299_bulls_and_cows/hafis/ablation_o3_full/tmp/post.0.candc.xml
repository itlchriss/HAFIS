<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <rule type="fa" cat="N">
     <lf start="0" span="1" word="Ensures" lemma="ensure" pos="NNS" chunk="I-NP" entity="O" cat="N/N" />
     <rule type="fa" cat="N">
      <lf start="1" span="1" word="String" lemma="string" pos="VBG" chunk="I-VP" entity="O" cat="N/N" />
      <lf start="2" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pss]\NP)" />
    <rule type="fa" cat="S[pss]\NP">
     <lf start="4" span="1" word="formatted" lemma="format" pos="VBN" chunk="I-VP" entity="O" cat="(S[pss]\NP)/PP" />
     <rule type="fa" cat="PP">
      <lf start="5" span="1" word="as" lemma="as" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
      <rule type="ba" cat="NP">
       <rule type="lex" cat="NP">
        <rule type="fa" cat="N">
         <rule type="ba" cat="N/N">
          <lf start="6" span="1" word="1" lemma="1" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
          <rule type="conj" cat="(N/N)\(N/N)">
           <lf start="7" span="1" word="or" lemma="or" pos="CC" chunk="I-NP" entity="O" cat="conj" />
           <lf start="8" span="1" word="more" lemma="more" pos="JJR" chunk="I-NP" entity="O" cat="N/N" />
          </rule>
         </rule>
         <rule type="fa" cat="N">
          <lf start="9" span="1" word="decimal" lemma="decimal" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="10" span="1" word="digits" lemma="digit" pos="NNS" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
       <rule type="lex" cat="NP\NP">
        <rule type="ba" cat="S[pss]\NP">
         <lf start="11" span="1" word="followed" lemma="follow" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
         <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
          <lf start="12" span="1" word="by" lemma="by" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
          <rule type="ba" cat="NP">
           <rule type="fa" cat="NP[nb]">
            <lf start="13" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
            <rule type="fa" cat="N">
             <lf start="14" span="1" word="character" lemma="character" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
             <lf start="15" span="1" word="A" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="N" />
            </rule>
           </rule>
           <rule type="lex" cat="NP\NP">
            <rule type="ba" cat="S[pss]\NP">
             <lf start="16" span="1" word="followed" lemma="follow" pos="VBN" chunk="I-NP" entity="O" cat="S[pss]\NP" />
             <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
              <lf start="17" span="1" word="by" lemma="by" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
              <rule type="ba" cat="NP">
               <rule type="lex" cat="NP">
                <rule type="fa" cat="N">
                 <rule type="ba" cat="N/N">
                  <lf start="18" span="1" word="1" lemma="1" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
                  <rule type="conj" cat="(N/N)\(N/N)">
                   <lf start="19" span="1" word="or" lemma="or" pos="CC" chunk="I-NP" entity="O" cat="conj" />
                   <lf start="20" span="1" word="more" lemma="more" pos="JJR" chunk="I-NP" entity="O" cat="N/N" />
                  </rule>
                 </rule>
                 <rule type="fa" cat="N">
                  <lf start="21" span="1" word="decimal" lemma="decimal" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
                  <lf start="22" span="1" word="digits" lemma="digit" pos="NNS" chunk="I-NP" entity="O" cat="N" />
                 </rule>
                </rule>
               </rule>
               <rule type="lex" cat="NP\NP">
                <rule type="ba" cat="S[pss]\NP">
                 <lf start="23" span="1" word="followed" lemma="follow" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
                 <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
                  <lf start="24" span="1" word="by" lemma="by" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
                  <rule type="fa" cat="NP[nb]">
                   <lf start="25" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
                   <rule type="fa" cat="N">
                    <lf start="26" span="1" word="character" lemma="character" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
                    <lf start="27" span="1" word="B" lemma="b" pos="NN" chunk="I-NP" entity="O" cat="N" />
                   </rule>
                  </rule>
                 </rule>
                </rule>
               </rule>
              </rule>
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="28" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
