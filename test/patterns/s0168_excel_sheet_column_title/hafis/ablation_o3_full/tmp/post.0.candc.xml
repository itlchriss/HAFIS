<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="String" lemma="string" pos="VBG" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="equals" lemma="equal" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="ba" cat="NP">
     <rule type="fa" cat="NP[nb]">
      <lf start="4" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <rule type="fa" cat="N">
       <lf start="5" span="1" word="Excel" lemma="Excel" pos="NNP" chunk="I-NP" entity="I-ORG" cat="N/N" />
       <rule type="fa" cat="N">
        <lf start="6" span="1" word="column" lemma="column" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="7" span="1" word="title" lemma="title" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="8" span="1" word="that" lemma="that" pos="WDT" chunk="B-NP" entity="O" cat="(NP\NP)/(S[dcl]\NP)" />
      <rule type="fa" cat="S[dcl]\NP">
       <lf start="9" span="1" word="matches" lemma="match" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
       <rule type="ba" cat="NP">
        <rule type="fa" cat="NP[nb]">
         <lf start="10" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <rule type="fa" cat="N">
          <lf start="11" span="1" word="positional" lemma="positional" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="12" span="1" word="value" lemma="value" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
        <rule type="lex" cat="NP\NP">
         <rule type="ba" cat="S[pss]\NP">
          <lf start="13" span="1" word="specified" lemma="specify" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
          <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
           <lf start="14" span="1" word="by" lemma="by" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
           <rule type="fa" cat="NP">
            <lf start="15" span="1" word="int" lemma="int" pos="NN" chunk="I-NP" entity="O" cat="NP/NP" />
            <rule type="fa" cat="NP[nb]">
             <lf start="16" span="1" word="the" lemma="the" pos="DT" chunk="B-NP" entity="O" cat="NP[nb]/N" />
             <lf start="17" span="1" word="chry_a" lemma="chry_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="18" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
