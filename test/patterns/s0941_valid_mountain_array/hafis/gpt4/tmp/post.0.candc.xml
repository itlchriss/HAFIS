<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="S[X]/S[X]">
  <lf start="0" span="1" word="If" lemma="if" pos="IN" chunk="I-SBAR" entity="O" cat="(S/S)/S[dcl]" />
  <rule type="rp" cat="S[dcl]">
   <rule type="fa" cat="S[dcl]">
    <rule type="bx" cat="S[dcl]/NP">
     <rule type="bx" cat="S[dcl]/NP">
      <rule type="fc" cat="S[dcl]/NP">
       <rule type="tr" cat="S[X]/(S[X]\NP)">
        <rule type="fa" cat="NP[nb]">
         <lf start="1" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <rule type="fa" cat="N">
          <lf start="2" span="1" word="type_boolean_" lemma="type_boolean_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="3" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
       <rule type="fc" cat="(S[dcl]\NP)/NP">
        <lf start="4" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
        <rule type="fc" cat="(S[adj]\NP)/NP">
         <lf start="5" span="1" word="equal" lemma="equal" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
         <lf start="6" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
        </rule>
       </rule>
      </rule>
      <rule type="lex" cat="S[X]\S[X]">
       <rule type="ba" cat="S[dcl]">
        <rule type="ba" cat="NP[thr]">
         <rule type="fa" cat="NP[nb]">
          <lf start="7" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
          <rule type="fa" cat="N">
           <lf start="8" span="1" word="true" lemma="true" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
           <lf start="9" span="1" word="literal" lemma="literal" pos="NN" chunk="I-NP" entity="O" cat="N" />
          </rule>
         </rule>
         <rule type="conj" cat="NP[thr]\NP[thr]">
          <lf start="10" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
          <lf start="11" span="1" word="there" lemma="there" pos="EX" chunk="I-NP" entity="O" cat="NP[thr]" />
         </rule>
        </rule>
        <rule type="fa" cat="S[dcl]\NP[thr]">
         <lf start="12" span="1" word="exists" lemma="exist" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP[thr])/NP" />
         <rule type="fa" cat="NP[nb]">
          <lf start="13" span="1" word="an" lemma="an" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
          <lf start="14" span="1" word="index" lemma="index" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
     <rule type="lex" cat="S[X]\S[X]">
      <rule type="ba" cat="S[dcl]">
       <rule type="ba" cat="NP[nb]">
        <rule type="ba" cat="NP">
         <rule type="fa" cat="NP[nb]">
          <lf start="15" span="1" word="the" lemma="the" pos="DT" chunk="B-NP" entity="O" cat="NP[nb]/N" />
          <rule type="fa" cat="N">
           <lf start="16" span="1" word="type_character_" lemma="type_character_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
           <lf start="17" span="1" word="chrs_a" lemma="chrs_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
          </rule>
         </rule>
         <rule type="lex" cat="NP\NP">
          <rule type="fa" cat="S[adj]\NP">
           <lf start="18" span="1" word="such" lemma="such" pos="JJ" chunk="I-PP" entity="O" cat="(S[adj]\NP)/S[em]" />
           <rule type="fa" cat="S[em]">
            <lf start="19" span="1" word="that" lemma="that" pos="IN" chunk="I-SBAR" entity="O" cat="S[em]/S[dcl]" />
            <rule type="ba" cat="S[dcl]">
             <rule type="fa" cat="NP[nb]">
              <lf start="20" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
              <lf start="21" span="1" word="type_string_" lemma="type_string_" pos="NN" chunk="I-NP" entity="O" cat="N" />
             </rule>
             <rule type="fa" cat="S[dcl]\NP">
              <lf start="22" span="1" word="strs_b" lemma="strs_b" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
              <rule type="ba" cat="NP">
               <rule type="fa" cat="NP[nb]">
                <rule type="ba" cat="NP[nb]/N">
                 <rule type="fa" cat="NP[nb]">
                  <lf start="23" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
                  <rule type="fa" cat="N">
                   <lf start="24" span="1" word="type_integer_array_" lemma="type_integer_array_" pos="NN" chunk="I-NP" entity="I-ORG" cat="N/N" />
                   <lf start="25" span="1" word="param_arr_" lemma="param_arr_" pos="NN" chunk="I-NP" entity="I-ORG" cat="N" />
                  </rule>
                 </rule>
                 <lf start="26" span="1" word="'s" lemma="'s" pos="POS" chunk="B-NP" entity="I-ORG" cat="(NP[nb]/N)\NP" />
                </rule>
                <lf start="27" span="1" word="length" lemma="length" pos="NN" chunk="I-NP" entity="I-ORG" cat="N" />
               </rule>
               <rule type="fa" cat="NP\NP">
                <lf start="28" span="1" word="-" lemma="-" pos="IN" chunk="I-NP" entity="I-DAT" cat="(NP\NP)/NP" />
                <rule type="lex" cat="NP">
                 <lf start="29" span="1" word="1" lemma="1" pos="CD" chunk="I-NP" entity="I-DAT" cat="N" />
                </rule>
               </rule>
              </rule>
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
        <rule type="conj" cat="NP[nb]\NP[nb]">
         <lf start="30" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
         <rule type="conj" cat="NP[nb]\NP[nb]">
          <lf start="31" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
          <rule type="fa" cat="NP[nb]">
           <lf start="32" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
           <rule type="fa" cat="N">
            <lf start="33" span="1" word="type_integer_array_" lemma="type_integer_array_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
            <lf start="34" span="1" word="param_arr_" lemma="param_arr_" pos="NN" chunk="I-NP" entity="O" cat="N" />
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
       <rule type="fa" cat="S[dcl]\NP">
        <rule type="bx" cat="(S[dcl]\NP)/(S[ng]\NP)">
         <lf start="35" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[ng]\NP)" />
         <lf start="36" span="1" word="strictly" lemma="strictly" pos="RB" chunk="I-VP" entity="O" cat="(S\NP)\(S\NP)" />
        </rule>
        <rule type="ba" cat="S[ng]\NP">
         <rule type="fa" cat="S[ng]\NP">
          <rule type="bx" cat="(S[ng]\NP)/NP">
           <rule type="bx" cat="(S[ng]\NP)/NP">
            <lf start="37" span="1" word="increasing" lemma="increase" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
            <lf start="38" span="1" word="up" lemma="up" pos="RP" chunk="I-ADVP" entity="O" cat="(S\NP)\(S\NP)" />
           </rule>
           <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
            <lf start="39" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
            <rule type="lex" cat="NP">
             <lf start="40" span="1" word="index" lemma="index" pos="NN" chunk="I-NP" entity="O" cat="N" />
            </rule>
           </rule>
          </rule>
          <rule type="fa" cat="NP[nb]">
           <lf start="41" span="1" word="the" lemma="the" pos="DT" chunk="B-NP" entity="O" cat="NP[nb]/N" />
           <rule type="fa" cat="N">
            <lf start="42" span="1" word="type_character_" lemma="type_character_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
            <lf start="43" span="1" word="chrs_c" lemma="chrs_c" pos="NN" chunk="I-NP" entity="O" cat="N" />
           </rule>
          </rule>
         </rule>
         <rule type="conj" cat="(S[ng]\NP)\(S[ng]\NP)">
          <lf start="44" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
          <rule type="fa" cat="S[ng]\NP">
           <lf start="45" span="1" word="strictly" lemma="strictly" pos="RB" chunk="I-VP" entity="O" cat="(S\NP)/(S\NP)" />
           <rule type="ba" cat="S[ng]\NP">
            <lf start="46" span="1" word="decreasing" lemma="decrease" pos="VBG" chunk="I-VP" entity="O" cat="S[ng]\NP" />
            <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
             <lf start="47" span="1" word="after" lemma="after" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
             <rule type="lex" cat="NP">
              <lf start="48" span="1" word="index" lemma="index" pos="NN" chunk="I-NP" entity="O" cat="N" />
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
    <rule type="fa" cat="NP[nb]">
     <lf start="49" span="1" word="the" lemma="the" pos="DT" chunk="B-NP" entity="O" cat="NP[nb]/N" />
     <rule type="fa" cat="N">
      <lf start="50" span="1" word="type_character_" lemma="type_character_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="51" span="1" word="chrs_d" lemma="chrs_d" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
   <lf start="52" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
</candc>
