<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="ba" cat="S[dcl]">
  <rule type="fa" cat="NP[nb]">
   <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
   <rule type="fa" cat="N">
    <lf start="1" span="1" word="type_boolean_" lemma="type_boolean_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
    <lf start="2" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
  </rule>
  <rule type="ba" cat="S[dcl]\NP">
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
    <rule type="fa" cat="S[adj]\NP">
     <lf start="4" span="1" word="equal" lemma="equal" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
     <rule type="fa" cat="PP">
      <lf start="5" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="6" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <rule type="fa" cat="N">
        <lf start="7" span="1" word="true" lemma="true" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="8" span="1" word="literal" lemma="literal" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
   <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
    <lf start="9" span="1" word="when" lemma="when" pos="WRB" chunk="I-ADVP" entity="O" cat="((S\NP)\(S\NP))/S[dcl]" />
    <rule type="rp" cat="S[dcl]">
     <rule type="ba" cat="S[dcl]">
      <rule type="ba" cat="S[dcl]">
       <rule type="ba" cat="NP">
        <rule type="ba" cat="NP">
         <rule type="fa" cat="NP[nb]">
          <lf start="10" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
          <lf start="11" span="1" word="usage" lemma="usage" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
         <rule type="fa" cat="NP\NP">
          <lf start="12" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
          <rule type="lex" cat="NP">
           <rule type="fa" cat="N">
            <lf start="13" span="1" word="capital" lemma="capital" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
            <lf start="14" span="1" word="letters" lemma="letter" pos="NNS" chunk="I-NP" entity="O" cat="N" />
           </rule>
          </rule>
         </rule>
        </rule>
        <rule type="fa" cat="NP\NP">
         <lf start="15" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
         <rule type="fa" cat="NP[nb]">
          <lf start="16" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
          <rule type="fa" cat="N">
           <lf start="17" span="1" word="type_string_" lemma="type_string_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
           <lf start="18" span="1" word="param_word_" lemma="param_word_" pos="NN" chunk="I-NP" entity="O" cat="N" />
          </rule>
         </rule>
        </rule>
       </rule>
       <rule type="fa" cat="S[dcl]\NP">
        <lf start="19" span="1" word="follows" lemma="follow" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
        <rule type="ba" cat="NP">
         <rule type="fa" cat="NP[nb]">
          <lf start="20" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
          <lf start="21" span="1" word="rules" lemma="rule" pos="NNS" chunk="I-NP" entity="O" cat="N" />
         </rule>
         <rule type="lex" cat="NP\NP">
          <rule type="ba" cat="S[pss]\NP">
           <lf start="22" span="1" word="stated" lemma="state" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
           <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
            <lf start="23" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
            <rule type="fa" cat="NP[nb]">
             <lf start="24" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
             <rule type="fa" cat="N">
              <lf start="25" span="1" word="software" lemma="software" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
              <lf start="26" span="1" word="specification" lemma="specification" pos="NN" chunk="I-NP" entity="O" cat="N" />
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
      <rule type="conj" cat="S[dcl]\S[dcl]">
       <lf start="27" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
       <rule type="ba" cat="S[dcl]">
        <rule type="fa" cat="NP[nb]">
         <lf start="28" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <rule type="fa" cat="N">
          <lf start="29" span="1" word="type_boolean_" lemma="type_boolean_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="30" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
        <rule type="ba" cat="S[dcl]\NP">
         <rule type="fa" cat="S[dcl]\NP">
          <lf start="31" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
          <rule type="fa" cat="S[adj]\NP">
           <lf start="32" span="1" word="equal" lemma="equal" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
           <rule type="fa" cat="PP">
            <lf start="33" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
            <rule type="fa" cat="NP[nb]">
             <lf start="34" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
             <rule type="fa" cat="N">
              <lf start="35" span="1" word="false" lemma="false" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
              <lf start="36" span="1" word="literal" lemma="literal" pos="NN" chunk="I-NP" entity="O" cat="N" />
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
         <lf start="37" span="1" word="otherwise" lemma="otherwise" pos="RB" chunk="I-ADVP" entity="O" cat="(S\NP)\(S\NP)" />
        </rule>
       </rule>
      </rule>
     </rule>
     <lf start="38" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
    </rule>
   </rule>
  </rule>
 </rule>
</ccg>
</candc>
