<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <rule type="fa" cat="N">
     <lf start="0" span="1" word="boolean" lemma="boolean" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="1" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
    <rule type="fa" cat="S[adj]\NP">
     <lf start="3" span="1" word="equal" lemma="equal" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
     <rule type="fa" cat="PP">
      <lf start="4" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
      <rule type="ba" cat="NP">
       <rule type="fa" cat="NP[nb]">
        <lf start="5" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <rule type="fa" cat="N">
         <lf start="6" span="1" word="true" lemma="true" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="7" span="1" word="literal" lemma="literal" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
       <rule type="fa" cat="NP\NP">
        <lf start="8" span="1" word="when" lemma="when" pos="WRB" chunk="I-ADVP" entity="O" cat="(NP\NP)/S[dcl]" />
        <rule type="ba" cat="S[dcl]">
         <rule type="lex" cat="NP">
          <rule type="fa" cat="N">
           <lf start="9" span="1" word="String" lemma="string" pos="VBG" chunk="I-NP" entity="O" cat="N/N" />
           <lf start="10" span="1" word="stry_a" lemma="stry_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
          </rule>
         </rule>
         <rule type="fa" cat="S[dcl]\NP">
          <lf start="11" span="1" word="has" lemma="have" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
          <rule type="ba" cat="NP">
           <rule type="ba" cat="NP">
            <rule type="fa" cat="NP[nb]">
             <lf start="12" span="1" word="its" lemma="its" pos="PRP$" chunk="I-NP" entity="O" cat="NP[nb]/N" />
             <rule type="fa" cat="N">
              <lf start="13" span="1" word="first" lemma="first" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
              <lf start="14" span="1" word="letter" lemma="letter" pos="NN" chunk="I-NP" entity="O" cat="N" />
             </rule>
            </rule>
            <lf start="15" span="1" word="uppercase" lemma="uppercase" pos="NN" chunk="I-NP" entity="O" cat="NP\NP" />
           </rule>
           <rule type="conj" cat="NP\NP">
            <lf start="16" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
            <rule type="ba" cat="NP">
             <rule type="fa" cat="NP[nb]">
              <lf start="17" span="1" word="every" lemma="every" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
              <rule type="fa" cat="N">
               <lf start="18" span="1" word="subsequent" lemma="subsequent" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
               <lf start="19" span="1" word="letter" lemma="letter" pos="NN" chunk="I-NP" entity="O" cat="N" />
              </rule>
             </rule>
             <lf start="20" span="1" word="lowercase" lemma="lowercase" pos="NN" chunk="I-NP" entity="O" cat="NP\NP" />
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="21" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
