<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="S[dcl]">
  <rule type="fa" cat="S[X]/S[X]">
   <lf start="0" span="1" word="if" lemma="if" pos="IN" chunk="I-SBAR" entity="O" cat="(S/S)/S[dcl]" />
   <rule type="fa" cat="S[dcl]">
    <rule type="tr" cat="S[X]/(S[X]/NP)">
     <rule type="lex" cat="NP">
      <lf start="1" span="1" word="int" lemma="int" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
    <rule type="fc" cat="S[dcl]/NP">
     <rule type="tr" cat="S[X]/(S[X]\NP)">
      <rule type="fa" cat="NP[nb]">
       <lf start="2" span="1" word="the" lemma="the" pos="DT" chunk="B-NP" entity="O" cat="NP[nb]/N" />
       <rule type="fa" cat="N">
        <lf start="3" span="1" word="type_string_" lemma="type_string_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="4" span="1" word="strs_a" lemma="strs_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
     <rule type="fc" cat="(S[dcl]\NP)/NP">
      <lf start="5" span="1" word="occurs" lemma="occur" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/PP" />
      <lf start="6" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
     </rule>
    </rule>
   </rule>
  </rule>
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <rule type="fa" cat="N">
     <lf start="7" span="1" word="int" lemma="int" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="8" span="1" word="[" lemma="[" pos="NNS" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="9" span="1" word="]" lemma="]" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/S[dcl]" />
    <rule type="rp" cat="S[dcl]">
     <rule type="ba" cat="S[dcl]">
      <rule type="ba" cat="NP">
       <rule type="ba" cat="NP">
        <rule type="fa" cat="NP[nb]">
         <lf start="10" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <rule type="fa" cat="N">
          <lf start="11" span="1" word="type_string_" lemma="type_string_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="12" span="1" word="strs_b" lemma="strs_b" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
        <rule type="lex" cat="NP\NP">
         <rule type="fc" cat="S[dcl]/NP">
          <rule type="tr" cat="S[X]/(S[X]\NP)">
           <rule type="lex" cat="NP">
            <rule type="fa" cat="N">
             <rule type="fa" cat="N/N">
              <lf start="13" span="1" word="then" lemma="then" pos="RB" chunk="I-ADVP" entity="O" cat="(N/N)/(N/N)" />
              <lf start="14" span="1" word="int" lemma="int" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
             </rule>
             <lf start="15" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
            </rule>
           </rule>
          </rule>
          <rule type="fc" cat="(S[dcl]\NP)/NP">
           <lf start="16" span="1" word="equals" lemma="equal" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
           <rule type="fc" cat="NP/NP">
            <rule type="lex" cat="NP/(NP\NP)">
             <rule type="fa" cat="NP[nb]">
              <lf start="17" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
              <lf start="18" span="1" word="index" lemma="index" pos="NN" chunk="I-NP" entity="O" cat="N" />
             </rule>
            </rule>
            <lf start="19" span="1" word="at" lemma="at" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
       <rule type="fa" cat="NP\NP">
        <lf start="20" span="1" word="which" lemma="which" pos="WDT" chunk="I-NP" entity="O" cat="(NP\NP)/(S[dcl]\NP)" />
        <rule type="fa" cat="S[dcl]\NP">
         <lf start="21" span="1" word="int" lemma="int" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
         <rule type="fa" cat="NP[nb]">
          <lf start="22" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
          <rule type="fa" cat="N">
           <lf start="23" span="1" word="type_string_" lemma="type_string_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
           <lf start="24" span="1" word="strs_c" lemma="strs_c" pos="NN" chunk="I-NP" entity="O" cat="N" />
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
      <rule type="fa" cat="S[dcl]\NP">
       <lf start="25" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pss]\NP)" />
       <rule type="ba" cat="S[pss]\NP">
        <lf start="26" span="1" word="found" lemma="find" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
        <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
         <lf start="27" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
         <rule type="ba" cat="NP">
          <rule type="lex" cat="NP">
           <rule type="fa" cat="N">
            <lf start="28" span="1" word="int" lemma="int" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
            <lf start="29" span="1" word="[" lemma="[" pos="NNS" chunk="I-NP" entity="O" cat="N" />
           </rule>
          </rule>
          <rule type="lex" cat="NP\NP">
           <rule type="fa" cat="S[dcl]\NP">
            <lf start="30" span="1" word="]" lemma="]" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
            <rule type="fa" cat="NP[nb]">
             <lf start="31" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
             <rule type="fa" cat="N">
              <lf start="32" span="1" word="type_string_" lemma="type_string_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
              <lf start="33" span="1" word="strs_d" lemma="strs_d" pos="NN" chunk="I-NP" entity="O" cat="N" />
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
     <lf start="34" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
    </rule>
   </rule>
  </rule>
 </rule>
</ccg>
</candc>
