<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="String" lemma="string" pos="VBG" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="param_stry_a_" lemma="param_stry_a_" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="only" lemma="only" pos="RB" chunk="I-ADVP" entity="O" cat="(S\NP)/(S\NP)" />
    <rule type="fa" cat="S[dcl]\NP">
     <rule type="fa" cat="(S[dcl]\NP)/NP">
      <lf start="4" span="1" word="contain" lemma="contain" pos="VBP" chunk="I-VP" entity="O" cat="((S[dcl]\NP)/NP)/NP" />
      <rule type="lex" cat="NP">
       <lf start="5" span="1" word="lowercase_alphabets" lemma="lowercase_alphabets" pos="NNS" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <rule type="ba" cat="NP[nb]">
      <rule type="fa" cat="NP[nb]">
       <lf start="6" span="1" word="the" lemma="the" pos="DT" chunk="B-NP" entity="O" cat="NP[nb]/N" />
       <rule type="fa" cat="N">
        <lf start="7" span="1" word="question" lemma="question" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
        <rule type="fa" cat="N">
         <lf start="8" span="1" word="mark" lemma="mark" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="9" span="1" word="character" lemma="character" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
      <rule type="conj" cat="NP[nb]\NP[nb]">
       <lf start="10" span="1" word="or" lemma="or" pos="CC" chunk="O" entity="O" cat="conj" />
       <rule type="fa" cat="NP[nb]">
        <lf start="11" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <rule type="fa" cat="N">
         <lf start="12" span="1" word="asterisk" lemma="asterisk" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="13" span="1" word="character" lemma="character" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="14" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
