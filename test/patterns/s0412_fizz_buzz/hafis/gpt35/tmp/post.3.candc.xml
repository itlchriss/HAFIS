<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="S[dcl]">
  <rule type="fa" cat="S[X]/S[X]">
   <lf start="0" span="1" word="If" lemma="if" pos="IN" chunk="I-SBAR" entity="O" cat="(S/S)/S[dcl]" />
   <rule type="fa" cat="S[dcl]">
    <rule type="tr" cat="S[X]/(S[X]/NP)">
     <rule type="fa" cat="NP[nb]">
      <lf start="1" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="2" span="1" word="index" lemma="index" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
    <rule type="fc" cat="S[dcl]/NP">
     <rule type="tr" cat="S[X]/(S[X]\NP)">
      <rule type="fa" cat="NP[nb]">
       <lf start="3" span="1" word="the" lemma="the" pos="DT" chunk="B-NP" entity="O" cat="NP[nb]/N" />
       <rule type="fa" cat="N">
        <lf start="4" span="1" word="type_character_" lemma="type_character_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="5" span="1" word="chrs_a" lemma="chrs_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
     <rule type="fc" cat="(S[dcl]\NP)/NP">
      <lf start="6" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
      <rule type="fc" cat="(S[adj]\NP)/NP">
       <lf start="7" span="1" word="divisible" lemma="divisible" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
       <lf start="8" span="1" word="by" lemma="by" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <rule type="rp" cat="S[dcl]">
   <rule type="ba" cat="S[dcl]">
    <rule type="ba" cat="NP">
     <rule type="lex" cat="NP">
      <lf start="9" span="1" word="5" lemma="5" pos="CD" chunk="I-NP" entity="I-DAT" cat="N" />
     </rule>
     <rule type="conj" cat="NP\NP">
      <lf start="10" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
      <rule type="ba" cat="NP">
       <rule type="ba" cat="NP">
        <rule type="fa" cat="NP[nb]">
         <lf start="11" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <lf start="12" span="1" word="string" lemma="string" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
        <rule type="fa" cat="NP\NP">
         <lf start="13" span="1" word="at" lemma="at" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
         <rule type="ba" cat="NP">
          <rule type="lex" cat="NP">
           <lf start="14" span="1" word="index" lemma="index" pos="NN" chunk="I-NP" entity="O" cat="N" />
          </rule>
          <rule type="fa" cat="NP\NP">
           <lf start="15" span="1" word="the" lemma="the" pos="DT" chunk="B-NP" entity="O" cat="(NP\NP)/N" />
           <rule type="fa" cat="N">
            <lf start="16" span="1" word="type_character_" lemma="type_character_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
            <lf start="17" span="1" word="chrs_b" lemma="chrs_b" pos="NN" chunk="I-NP" entity="O" cat="N" />
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
       <rule type="fa" cat="NP\NP">
        <lf start="18" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
        <rule type="fa" cat="NP[nb]">
         <lf start="19" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <rule type="fa" cat="N">
          <lf start="20" span="1" word="type_string_array_" lemma="type_string_array_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="21" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="22" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
     <rule type="ba" cat="S[adj]\NP">
      <lf start="23" span="1" word="equal" lemma="equal" pos="JJ" chunk="I-ADJP" entity="O" cat="S[adj]\NP" />
      <rule type="fa" cat="(S[adj]\NP)\(S[adj]\NP)">
       <lf start="24" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="((S[adj]\NP)\(S[adj]\NP))/NP" />
       <rule type="lex" cat="NP">
        <lf start="25" span="1" word="stry_a" lemma="stry_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
   <lf start="26" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
</candc>
