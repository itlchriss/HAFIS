<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="type_string_" lemma="type_string_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="contains" lemma="contain" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="ba" cat="NP">
     <rule type="fa" cat="NP[nb]">
      <lf start="4" span="1" word="an" lemma="an" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <rule type="fa" cat="N">
       <lf start="5" span="1" word="optional" lemma="optional" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
       <rule type="fa" cat="N">
        <lf start="6" span="1" word="leading" lemma="lead" pos="VBG" chunk="I-NP" entity="O" cat="N/N" />
        <rule type="lp" cat="N">
         <lf start="7" span="1" word="-" lemma="-" pos=":" chunk="I-NP" entity="O" cat=":" />
         <lf start="8" span="1" word="sign" lemma="sign" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
     <rule type="lex" cat="NP\NP">
      <rule type="ba" cat="S[pss]\NP">
       <rule type="ba" cat="S[pss]\NP">
        <lf start="9" span="1" word="followed" lemma="follow" pos="VBD" chunk="I-VP" entity="O" cat="S[pss]\NP" />
        <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
         <lf start="10" span="1" word="exclusively" lemma="exclusively" pos="RB" chunk="I-ADVP" entity="O" cat="((S\NP)\(S\NP))/((S\NP)\(S\NP))" />
         <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
          <lf start="11" span="1" word="by" lemma="by" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
          <rule type="ba" cat="NP">
           <rule type="lex" cat="NP">
            <lf start="12" span="1" word="characters" lemma="character" pos="NNS" chunk="I-NP" entity="O" cat="N" />
           </rule>
           <rule type="fa" cat="NP\NP">
            <lf start="13" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
            <rule type="fa" cat="NP[nb]">
             <lf start="14" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
             <rule type="ba" cat="N">
              <lf start="15" span="1" word="range" lemma="range" pos="NN" chunk="I-NP" entity="O" cat="N" />
              <lf start="16" span="1" word="0" lemma="0" pos="CD" chunk="I-NP" entity="O" cat="N\N" />
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
       <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
        <lf start="17" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
        <rule type="lex" cat="NP">
         <lf start="18" span="1" word="6" lemma="6" pos="CD" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="19" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
