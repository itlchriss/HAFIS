<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <rule type="fa" cat="S[ng]\NP">
     <lf start="0" span="1" word="String" lemma="string" pos="VBG" chunk="I-NP" entity="O" cat="(S[ng]\NP)/NP" />
     <rule type="lex" cat="NP">
      <lf start="1" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
   <rule type="ba" cat="S[dcl]\NP">
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="2" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
     <rule type="ba" cat="NP">
      <rule type="fa" cat="NP[nb]">
       <lf start="3" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <rule type="fa" cat="N">
        <lf start="4" span="1" word="base" lemma="base" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
        <rule type="fa" cat="N">
         <lf start="5" span="1" word="7" lemma="7" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="6" span="1" word="representation" lemma="representation" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
      <rule type="fa" cat="NP\NP">
       <lf start="7" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
       <rule type="lex" cat="NP">
        <rule type="fa" cat="N">
         <lf start="8" span="1" word="int" lemma="int" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="9" span="1" word="stry_a" lemma="stry_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
    <rule type="conj" cat="(S[dcl]\NP)\(S[dcl]\NP)">
     <lf start="10" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="11" span="1" word="includes" lemma="include" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
      <rule type="ba" cat="NP">
       <lf start="12" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP" />
       <rule type="lex" cat="NP\NP">
        <rule type="ba" cat="S[ng]\NP">
         <rule type="ba" cat="S[ng]\NP">
          <lf start="13" span="1" word="leading" lemma="lead" pos="VBG" chunk="I-NP" entity="O" cat="S[ng]\NP" />
          <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
           <lf start="14" span="1" word="-" lemma="-" pos="IN" chunk="O" entity="O" cat="((S\NP)\(S\NP))/NP" />
           <rule type="lex" cat="NP">
            <lf start="15" span="1" word="sign" lemma="sign" pos="NN" chunk="I-NP" entity="O" cat="N" />
           </rule>
          </rule>
         </rule>
         <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
          <lf start="16" span="1" word="if" lemma="if" pos="IN" chunk="I-SBAR" entity="O" cat="((S\NP)\(S\NP))/S[dcl]" />
          <rule type="ba" cat="S[dcl]">
           <rule type="lex" cat="NP">
            <rule type="fa" cat="N">
             <lf start="17" span="1" word="int" lemma="int" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
             <lf start="18" span="1" word="stry_b" lemma="stry_b" pos="NN" chunk="I-NP" entity="O" cat="N" />
            </rule>
           </rule>
           <rule type="fa" cat="S[dcl]\NP">
            <lf start="19" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
            <lf start="20" span="1" word="negative" lemma="negative" pos="JJ" chunk="I-ADJP" entity="O" cat="S[adj]\NP" />
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="21" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
