<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="ba" cat="NP">
    <rule type="lex" cat="NP">
     <lf start="0" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
    <lf start="1" span="1" word="int" lemma="int" pos="NN" chunk="I-NP" entity="O" cat="NP\NP" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="equals" lemma="equal" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/S[dcl]" />
    <rule type="ba" cat="S[dcl]">
     <rule type="ba" cat="NP">
      <rule type="fa" cat="NP[nb]">
       <lf start="3" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="4" span="1" word="number" lemma="number" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
      <rule type="fa" cat="NP\NP">
       <lf start="5" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
       <rule type="ba" cat="NP">
        <rule type="lex" cat="NP">
         <lf start="6" span="1" word="bulbs" lemma="bulb" pos="NNS" chunk="I-NP" entity="O" cat="N" />
        </rule>
        <rule type="fa" cat="NP\NP">
         <lf start="7" span="1" word="that" lemma="that" pos="WDT" chunk="B-NP" entity="O" cat="(NP\NP)/(S[dcl]\NP)" />
         <rule type="fa" cat="S[dcl]\NP">
          <rule type="bx" cat="(S[dcl]\NP)/NP">
           <rule type="fc" cat="(S[dcl]\NP)/NP">
            <lf start="8" span="1" word="are" lemma="be" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/PP" />
            <lf start="9" span="1" word="on" lemma="on" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
           </rule>
           <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
            <lf start="10" span="1" word="after" lemma="after" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/(S[ng]\NP)" />
            <rule type="fa" cat="S[ng]\NP">
             <lf start="11" span="1" word="completing" lemma="complete" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
             <rule type="fa" cat="NP[nb]">
              <lf start="12" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
              <rule type="fa" cat="N">
               <lf start="13" span="1" word="type_character_" lemma="type_character_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
               <lf start="14" span="1" word="chrs_a" lemma="chrs_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
              </rule>
             </rule>
            </rule>
           </rule>
          </rule>
          <rule type="lex" cat="NP">
           <rule type="fa" cat="N">
            <lf start="15" span="1" word="sequential" lemma="sequential" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
            <lf start="16" span="1" word="toggle" lemma="toggle" pos="NN" chunk="I-NP" entity="O" cat="N" />
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
     <rule type="ba" cat="S[dcl]\NP">
      <lf start="17" span="1" word="rounds" lemma="round" pos="VBZ" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
      <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
       <lf start="18" span="1" word="as" lemma="as" pos="IN" chunk="I-SBAR" entity="O" cat="((S\NP)\(S\NP))/(S[pss]\NP)" />
       <rule type="ba" cat="S[pss]\NP">
        <lf start="19" span="1" word="described" lemma="describe" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
        <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
         <lf start="20" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
         <rule type="fa" cat="NP[nb]">
          <lf start="21" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
          <lf start="22" span="1" word="specification" lemma="specification" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="23" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
