<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="S[dcl]">
  <rule type="fa" cat="S[X]/S[X]">
   <lf start="0" span="1" word="When" lemma="when" pos="WRB" chunk="I-ADVP" entity="O" cat="(S/S)/S[dcl]" />
   <rule type="ba" cat="S[dcl]">
    <rule type="ba" cat="S[dcl]">
     <rule type="ba" cat="NP">
      <rule type="lex" cat="NP">
       <rule type="fa" cat="N">
        <lf start="1" span="1" word="data" lemma="datum" pos="NNS" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="2" span="1" word="type" lemma="type" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
      <rule type="lex" cat="NP\NP">
       <rule type="fa" cat="S[ng]\NP">
        <lf start="3" span="1" word="String" lemma="string" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
        <rule type="lex" cat="NP">
         <rule type="fa" cat="N">
          <lf start="4" span="1" word="param_the_" lemma="param_the_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="5" span="1" word="chry_a" lemma="chry_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
     <rule type="ba" cat="S[dcl]\NP">
      <rule type="rp" cat="S[dcl]\NP">
       <rule type="ba" cat="S[dcl]\NP">
        <rule type="fa" cat="S[dcl]\NP">
         <lf start="6" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/PP" />
         <rule type="fa" cat="PP">
          <lf start="7" span="1" word="at" lemma="at" pos="IN" chunk="I-NP" entity="O" cat="PP/NP" />
          <rule type="lex" cat="NP">
           <rule type="fa" cat="N">
            <lf start="8" span="1" word="least" lemma="least" pos="JJS" chunk="I-NP" entity="O" cat="N/N" />
            <rule type="fa" cat="N">
             <lf start="9" span="1" word="1" lemma="1" pos="CD" chunk="I-NP" entity="I-PCT" cat="N/N" />
             <lf start="10" span="1" word="character" lemma="character" pos="NN" chunk="I-NP" entity="I-PCT" cat="N" />
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
        <lf start="11" span="1" word="long" lemma="long" pos="RB" chunk="I-ADVP" entity="O" cat="(S\NP)\(S\NP)" />
       </rule>
       <lf start="12" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
      </rule>
      <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
       <rule type="fa" cat="((S[X]\NP)\(S[X]\NP))/((S[X]\NP)\(S[X]\NP))">
        <lf start="13" span="1" word="at" lemma="at" pos="IN" chunk="I-PP" entity="O" cat="(((S\NP)\(S\NP))/((S\NP)\(S\NP)))/(S[asup]\NP)" />
        <lf start="14" span="1" word="most" lemma="most" pos="JJS" chunk="I-NP" entity="O" cat="S[asup]\NP" />
       </rule>
       <rule type="ba" cat="(S[X]\NP)\(S[X]\NP)">
        <rule type="lex" cat="NP">
         <rule type="fa" cat="N">
          <lf start="15" span="1" word="10" lemma="10" pos="CD" chunk="I-NP" entity="I-PCT" cat="N/N" />
          <rule type="fa" cat="N">
           <lf start="16" span="1" word="1000" lemma="1000" pos="CD" chunk="I-NP" entity="I-PCT" cat="N/N" />
           <lf start="17" span="1" word="characters" lemma="character" pos="NNS" chunk="I-NP" entity="O" cat="N" />
          </rule>
         </rule>
        </rule>
        <lf start="18" span="1" word="long" lemma="long" pos="RB" chunk="I-ADVP" entity="O" cat="((S\NP)\(S\NP))\NP" />
       </rule>
      </rule>
     </rule>
    </rule>
    <rule type="conj" cat="S[dcl]\S[dcl]">
     <lf start="19" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
     <rule type="conj" cat="S[dcl]\S[dcl]">
      <lf start="20" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
      <rule type="ba" cat="S[dcl]">
       <rule type="fa" cat="NP[nb]">
        <lf start="21" span="1" word="every" lemma="every" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="22" span="1" word="character" lemma="character" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
       <rule type="fa" cat="S[dcl]\NP">
        <lf start="23" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
        <rule type="ba" cat="S[adj]\NP">
         <lf start="24" span="1" word="equal" lemma="equal" pos="JJ" chunk="I-ADJP" entity="O" cat="S[adj]\NP" />
         <rule type="fa" cat="(S[adj]\NP)\(S[adj]\NP)">
          <lf start="25" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="((S[adj]\NP)\(S[adj]\NP))/NP" />
          <rule type="ba" cat="NP">
           <rule type="lex" cat="NP">
            <rule type="fa" cat="N">
             <lf start="26" span="1" word="decimal" lemma="decimal" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
             <rule type="ba" cat="N">
              <lf start="27" span="1" word="digit" lemma="digit" pos="NN" chunk="I-NP" entity="O" cat="N" />
              <lf start="28" span="1" word="0" lemma="0" pos="CD" chunk="I-NP" entity="O" cat="N\N" />
             </rule>
            </rule>
           </rule>
           <rule type="fa" cat="NP\NP">
            <lf start="29" span="1" word="through" lemma="through" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
            <rule type="lex" cat="NP">
             <lf start="30" span="1" word="9" lemma="9" pos="CD" chunk="I-NP" entity="I-DAT" cat="N" />
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <rule type="rp" cat="S[dcl]">
   <rule type="lp" cat="S[dcl]">
    <lf start="31" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
    <rule type="ba" cat="S[dcl]">
     <rule type="fa" cat="NP[nb]">
      <lf start="32" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="33" span="1" word="method" lemma="method" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="34" span="1" word="accepts" lemma="accept" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="35" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <rule type="fa" cat="N">
        <lf start="36" span="1" word="as" lemma="as" pos="RB" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="37" span="1" word="valid" lemma="valid" pos="JJ" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
   <lf start="38" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
</candc>
