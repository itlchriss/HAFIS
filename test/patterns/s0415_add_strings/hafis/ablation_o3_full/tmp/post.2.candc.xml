<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="S[dcl]">
  <rule type="fa" cat="S[X]/S[X]">
   <lf start="0" span="1" word="When" lemma="when" pos="WRB" chunk="I-ADVP" entity="O" cat="(S/S)/S[dcl]" />
   <rule type="ba" cat="S[dcl]">
    <rule type="ba" cat="NP">
     <rule type="ba" cat="NP">
      <rule type="lex" cat="NP">
       <rule type="fa" cat="N">
        <lf start="1" span="1" word="data" lemma="datum" pos="NNS" chunk="I-NP" entity="O" cat="N/N" />
        <rule type="fa" cat="N">
         <lf start="2" span="1" word="type" lemma="type" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
         <rule type="fa" cat="N">
          <lf start="3" span="1" word="String" lemma="string" pos="VBG" chunk="I-VP" entity="O" cat="N/N" />
          <lf start="4" span="1" word="parameters" lemma="parameter" pos="NNS" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
      <rule type="fa" cat="NP\NP">
       <lf start="5" span="1" word="the" lemma="the" pos="DT" chunk="B-NP" entity="O" cat="(NP\NP)/N" />
       <lf start="6" span="1" word="chry_a" lemma="chry_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <rule type="conj" cat="NP\NP">
      <lf start="7" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
      <rule type="ba" cat="NP">
       <rule type="fa" cat="NP[nb]">
        <lf start="8" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="9" span="1" word="chry_b" lemma="chry_b" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
       <lf start="10" span="1" word="each" lemma="each" pos="DT" chunk="B-NP" entity="O" cat="NP\NP" />
      </rule>
     </rule>
    </rule>
    <rule type="ba" cat="S[dcl]\NP">
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="11" span="1" word="represent" lemma="represent" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
      <rule type="lex" cat="NP">
       <lf start="12" span="1" word="non_negative_integers" lemma="non_negative_integers" pos="NNS" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="13" span="1" word="without" lemma="without" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/(S[ng]\NP)" />
      <rule type="fa" cat="S[ng]\NP">
       <lf start="14" span="1" word="leading" lemma="lead" pos="VBG" chunk="I-NP" entity="O" cat="(S[ng]\NP)/NP" />
       <rule type="lex" cat="NP">
        <lf start="15" span="1" word="zeros" lemma="zero" pos="NNS" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <rule type="rp" cat="S[dcl]">
   <rule type="ba" cat="S[dcl]">
    <rule type="fa" cat="NP[nb]">
     <lf start="16" span="1" word="the" lemma="the" pos="DT" chunk="B-NP" entity="O" cat="NP[nb]/N" />
     <rule type="fa" cat="N">
      <lf start="17" span="1" word="data" lemma="datum" pos="NNS" chunk="I-NP" entity="O" cat="N/N" />
      <rule type="fa" cat="N">
       <lf start="18" span="1" word="type" lemma="type" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
       <rule type="fa" cat="N">
        <lf start="19" span="1" word="String" lemma="string" pos="VBG" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="20" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="21" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
     <rule type="ba" cat="NP">
      <rule type="fa" cat="NP[nb]">
       <lf start="22" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <rule type="fa" cat="N">
        <lf start="23" span="1" word="arithmetic" lemma="arithmetic" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="24" span="1" word="sum" lemma="sum" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
      <rule type="fa" cat="NP\NP">
       <lf start="25" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
       <rule type="ba" cat="NP">
        <rule type="fa" cat="NP[nb]">
         <lf start="26" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <rule type="fa" cat="N">
          <lf start="27" span="1" word="2" lemma="2" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="28" span="1" word="numbers" lemma="number" pos="NNS" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
        <rule type="lex" cat="NP\NP">
         <rule type="fa" cat="S[pss]\NP">
          <lf start="29" span="1" word="expressed" lemma="express" pos="VBN" chunk="I-VP" entity="O" cat="(S[pss]\NP)/PP" />
          <rule type="fa" cat="PP">
           <lf start="30" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
           <rule type="ba" cat="NP">
            <rule type="lex" cat="NP">
             <rule type="ba" cat="N">
              <lf start="31" span="1" word="base" lemma="base" pos="NN" chunk="I-NP" entity="O" cat="N" />
              <lf start="32" span="1" word="10" lemma="10" pos="CD" chunk="I-NP" entity="O" cat="N\N" />
             </rule>
            </rule>
            <rule type="fa" cat="NP\NP">
             <lf start="33" span="1" word="with" lemma="with" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
             <rule type="fa" cat="NP[nb]">
              <lf start="34" span="1" word="no" lemma="no" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
              <rule type="fa" cat="N">
               <lf start="35" span="1" word="leading" lemma="lead" pos="VBG" chunk="I-NP" entity="O" cat="N/N" />
               <lf start="36" span="1" word="zeros" lemma="zero" pos="NNS" chunk="I-NP" entity="O" cat="N" />
              </rule>
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
   <lf start="37" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
</candc>
