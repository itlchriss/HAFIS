<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="type_string_" lemma="type_string_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="param_s_" lemma="param_s_" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
    <rule type="ba" cat="S[adj]\NP">
     <lf start="4" span="1" word="equal" lemma="equal" pos="JJ" chunk="I-ADJP" entity="O" cat="S[adj]\NP" />
     <rule type="fa" cat="(S[adj]\NP)\(S[adj]\NP)">
      <lf start="5" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="((S[adj]\NP)\(S[adj]\NP))/NP" />
      <rule type="ba" cat="NP">
       <rule type="lex" cat="NP">
        <rule type="fa" cat="N">
         <rule type="fa" cat="N/N">
          <lf start="6" span="1" word="syntactically" lemma="syntactically" pos="RB" chunk="I-NP" entity="O" cat="(N/N)/(N/N)" />
          <lf start="7" span="1" word="valid" lemma="valid" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
         </rule>
         <rule type="fa" cat="N">
          <lf start="8" span="1" word="arithmetic" lemma="arithmetic" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="9" span="1" word="expression" lemma="expression" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
       <rule type="lex" cat="NP\NP">
        <rule type="ba" cat="S[pss]\NP">
         <lf start="10" span="1" word="formed" lemma="form" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
         <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
          <lf start="11" span="1" word="with" lemma="with" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
          <rule type="ba" cat="NP">
           <rule type="ba" cat="NP">
            <rule type="fa" cat="NP[nb]">
             <lf start="12" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
             <rule type="fa" cat="N">
              <lf start="13" span="1" word="operators" lemma="operator" pos="NNS" chunk="I-NP" entity="O" cat="N/N" />
              <lf start="14" span="1" word="+" lemma="+" pos="SYM" chunk="B-NP" entity="O" cat="N" />
             </rule>
            </rule>
            <rule type="lp" cat="NP\NP">
             <lf start="15" span="1" word="-" lemma="-" pos=":" chunk="I-NP" entity="O" cat=":" />
             <rule type="lex" cat="NP\NP">
              <lf start="16" span="1" word="asterisk" lemma="asterisk" pos="JJ" chunk="I-NP" entity="O" cat="S[adj]\NP" />
             </rule>
            </rule>
           </rule>
           <rule type="conj" cat="NP\NP">
            <lf start="17" span="1" word="and" lemma="and" pos="CC" chunk="I-NP" entity="O" cat="conj" />
            <rule type="lex" cat="NP">
             <rule type="fa" cat="N">
              <lf start="18" span="1" word="forward" lemma="forward" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
              <lf start="19" span="1" word="slash" lemma="slash" pos="NN" chunk="I-NP" entity="O" cat="N" />
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="20" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
