<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="S[dcl]">
  <rule type="fa" cat="S[X]/S[X]">
   <lf start="0" span="1" word="If" lemma="if" pos="IN" chunk="I-SBAR" entity="O" cat="(S/S)/S[dcl]" />
   <rule type="ba" cat="S[dcl]">
    <rule type="fa" cat="NP[nb]">
     <lf start="1" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <rule type="fa" cat="N">
      <lf start="2" span="1" word="type_string_" lemma="type_string_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="3" span="1" word="param_s_" lemma="param_s_" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="4" span="1" word="contains" lemma="contain" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
     <rule type="ba" cat="NP">
      <rule type="fa" cat="NP[nb]">
       <lf start="5" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="6" span="1" word="division" lemma="division" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
      <lf start="7" span="1" word="operator" lemma="operator" pos="NN" chunk="I-NP" entity="O" cat="NP\NP" />
     </rule>
    </rule>
   </rule>
  </rule>
  <rule type="lp" cat="S[dcl]">
   <lf start="8" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
   <rule type="fa" cat="S[dcl]">
    <rule type="fa" cat="S[X]/S[X]">
     <lf start="9" span="1" word="each" lemma="each" pos="DT" chunk="I-NP" entity="O" cat="(S/S)/N" />
     <lf start="10" span="1" word="division" lemma="division" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
    <rule type="rp" cat="S[dcl]">
     <rule type="ba" cat="S[dcl]">
      <rule type="ba" cat="NP">
       <rule type="lex" cat="NP">
        <lf start="11" span="1" word="operation" lemma="operation" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
       <rule type="lex" cat="NP\NP">
        <rule type="fa" cat="S[ng]\NP">
         <lf start="12" span="1" word="contributing" lemma="contribute" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/PP" />
         <rule type="fa" cat="PP">
          <lf start="13" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
          <rule type="fa" cat="NP[nb]">
           <lf start="14" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
           <rule type="fa" cat="N">
            <lf start="15" span="1" word="type_integer_" lemma="type_integer_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
            <lf start="16" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
      <rule type="fa" cat="S[dcl]\NP">
       <lf start="17" span="1" word="uses" lemma="use" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
       <rule type="ba" cat="NP">
        <rule type="lex" cat="NP">
         <lf start="18" span="1" word="truncation" lemma="truncation" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
        <rule type="fa" cat="NP\NP">
         <lf start="19" span="1" word="toward" lemma="toward" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
         <rule type="fa" cat="NP[nb]">
          <lf start="20" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
          <rule type="fa" cat="N">
           <lf start="21" span="1" word="0" lemma="0" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
           <lf start="22" span="1" word="literal" lemma="literal" pos="NN" chunk="I-NP" entity="O" cat="N" />
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
     <lf start="23" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
    </rule>
   </rule>
  </rule>
 </rule>
</ccg>
</candc>
