<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <rule type="ba" cat="NP[nb]/N">
     <rule type="fa" cat="NP[nb]">
      <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <rule type="fa" cat="N">
       <lf start="1" span="1" word="type_string_" lemma="type_string_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="2" span="1" word="param_s_" lemma="param_s_" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <lf start="3" span="1" word="'s" lemma="'s" pos="POS" chunk="B-NP" entity="O" cat="(NP[nb]/N)\NP" />
    </rule>
    <lf start="4" span="1" word="values" lemma="value" pos="NNS" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <rule type="bx" cat="(S[dcl]\NP)/PP">
     <lf start="5" span="1" word="are" lemma="be" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/PP" />
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="6" span="1" word="either" lemma="either" pos="DT" chunk="I-NP" entity="O" cat="((S\NP)\(S\NP))/N" />
      <rule type="fa" cat="N">
       <lf start="7" span="1" word="space" lemma="space" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
       <rule type="fa" cat="N">
        <lf start="8" span="1" word="characters" lemma="character" pos="NNS" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="9" span="1" word="digits" lemma="digit" pos="NNS" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
    <rule type="ba" cat="PP">
     <rule type="ba" cat="PP">
      <rule type="ba" cat="PP">
       <rule type="fa" cat="PP">
        <rule type="fa" cat="PP/PP">
         <lf start="10" span="1" word="from" lemma="from" pos="IN" chunk="I-PP" entity="O" cat="(PP/PP)/NP" />
         <rule type="lex" cat="NP">
          <lf start="11" span="1" word="0" lemma="0" pos="CD" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
        <rule type="fa" cat="PP">
         <lf start="12" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
         <rule type="lex" cat="NP">
          <lf start="13" span="1" word="9" lemma="9" pos="CD" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
       <rule type="fa" cat="PP\PP">
        <lf start="14" span="1" word="the" lemma="the" pos="DT" chunk="B-NP" entity="O" cat="(PP\PP)/N" />
        <rule type="fa" cat="N">
         <lf start="15" span="1" word="+" lemma="+" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
         <rule type="fa" cat="N">
          <lf start="16" span="1" word="sign" lemma="sign" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="17" span="1" word="character" lemma="character" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
      <rule type="fa" cat="PP\PP">
       <lf start="18" span="1" word="the" lemma="the" pos="DT" chunk="B-NP" entity="O" cat="(PP\PP)/N" />
       <rule type="lp" cat="N">
        <lf start="19" span="1" word="-" lemma="-" pos=":" chunk="I-NP" entity="O" cat=":" />
        <rule type="fa" cat="N">
         <lf start="20" span="1" word="sign" lemma="sign" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="21" span="1" word="character" lemma="character" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
     <rule type="fa" cat="PP\PP">
      <lf start="22" span="1" word="the" lemma="the" pos="DT" chunk="B-NP" entity="O" cat="(PP\PP)/N" />
      <rule type="fa" cat="N">
       <lf start="23" span="1" word="asterisk" lemma="asterisk" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
       <rule type="ba" cat="N">
        <lf start="24" span="1" word="character" lemma="character" pos="NN" chunk="I-NP" entity="O" cat="N" />
        <rule type="conj" cat="N\N">
         <lf start="25" span="1" word="or" lemma="or" pos="CC" chunk="O" entity="O" cat="conj" />
         <rule type="fa" cat="N">
          <lf start="26" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="N/N" />
          <rule type="fa" cat="N">
           <lf start="27" span="1" word="forward" lemma="forward" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
           <rule type="fa" cat="N">
            <lf start="28" span="1" word="slash" lemma="slash" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
            <lf start="29" span="1" word="character" lemma="character" pos="NN" chunk="I-NP" entity="O" cat="N" />
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="30" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
