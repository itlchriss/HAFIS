<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="S[dcl]">
  <rule type="fa" cat="S[X]/S[X]">
   <lf start="0" span="1" word="If" lemma="if" pos="IN" chunk="I-SBAR" entity="O" cat="(S/S)/S[dcl]" />
   <rule type="ba" cat="S[dcl]">
    <rule type="ba" cat="NP">
     <rule type="fa" cat="NP[nb]">
      <lf start="1" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <rule type="fa" cat="N">
       <lf start="2" span="1" word="computed" lemma="computed" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
       <rule type="fa" cat="N">
        <lf start="3" span="1" word="numeric" lemma="numeric" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="4" span="1" word="value" lemma="value" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="5" span="1" word="before" lemma="before" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/(S[ng]\NP)" />
      <lf start="6" span="1" word="clamping" lemma="clamp" pos="VBG" chunk="I-VP" entity="O" cat="S[ng]\NP" />
     </rule>
    </rule>
    <rule type="ba" cat="S[dcl]\NP">
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="7" span="1" word="exceeds" lemma="exceed" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
      <rule type="lex" cat="NP">
       <lf start="8" span="1" word="2147483647" lemma="2147483647" pos="CD" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <lf start="9" span="1" word="then" lemma="then" pos="RB" chunk="I-NP" entity="O" cat="(S\NP)\(S\NP)" />
    </rule>
   </rule>
  </rule>
  <rule type="rp" cat="S[dcl]">
   <rule type="ba" cat="S[dcl]">
    <rule type="lex" cat="NP">
     <rule type="fa" cat="N">
      <lf start="10" span="1" word="int" lemma="int" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="11" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="12" span="1" word="equals" lemma="equal" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
     <rule type="lex" cat="NP">
      <lf start="13" span="1" word="2147483647" lemma="2147483647" pos="CD" chunk="I-NP" entity="I-DAT" cat="N" />
     </rule>
    </rule>
   </rule>
   <lf start="14" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
</candc>
