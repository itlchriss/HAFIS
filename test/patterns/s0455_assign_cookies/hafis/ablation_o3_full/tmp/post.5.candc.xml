<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="ba" cat="S[dcl]">
    <rule type="ba" cat="NP">
     <rule type="ba" cat="NP">
      <lf start="0" span="1" word="There" lemma="there" pos="EX" chunk="I-NP" entity="O" cat="NP[thr]" />
      <rule type="lex" cat="NP\NP">
       <rule type="fa" cat="S[dcl]\NP[thr]">
        <lf start="1" span="1" word="exists" lemma="exist" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP[thr])/NP" />
        <rule type="ba" cat="NP">
         <rule type="fa" cat="NP[nb]">
          <lf start="2" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
          <rule type="fa" cat="N">
           <lf start="3" span="1" word="feasible" lemma="feasible" pos="JJ" chunk="I-ADJP" entity="O" cat="N/N" />
           <lf start="4" span="1" word="assignment" lemma="assignment" pos="NN" chunk="I-NP" entity="O" cat="N" />
          </rule>
         </rule>
         <rule type="fa" cat="NP\NP">
          <lf start="5" span="1" word="that" lemma="that" pos="IN" chunk="I-SBAR" entity="O" cat="(NP\NP)/(S[dcl]/NP)" />
          <rule type="fc" cat="S[dcl]/NP">
           <rule type="tr" cat="S[X]/(S[X]\NP)">
            <rule type="fa" cat="NP[nb]">
             <lf start="6" span="1" word="each" lemma="each" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
             <lf start="7" span="1" word="child" lemma="child" pos="NN" chunk="I-NP" entity="O" cat="N" />
            </rule>
           </rule>
           <rule type="bx" cat="(S[dcl]\NP)/NP">
            <lf start="8" span="1" word="receives" lemma="receive" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
            <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
             <lf start="9" span="1" word="at" lemma="at" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
             <rule type="lex" cat="NP">
              <rule type="fa" cat="N">
               <lf start="10" span="1" word="most" lemma="most" pos="JJS" chunk="I-NP" entity="O" cat="N/N" />
               <rule type="fa" cat="N">
                <lf start="11" span="1" word="1" lemma="1" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
                <lf start="12" span="1" word="cookie" lemma="cookie" pos="NN" chunk="I-NP" entity="O" cat="N" />
               </rule>
              </rule>
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
     <rule type="fa" cat="NP\NP">
      <rule type="fa" cat="(NP\NP)/(S[dcl]\NP)">
       <lf start="13" span="1" word="whose" lemma="whose" pos="WP$" chunk="B-NP" entity="O" cat="((NP\NP)/(S[dcl]\NP))/N" />
       <lf start="14" span="1" word="size" lemma="size" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
      <rule type="fa" cat="S[dcl]\NP">
       <lf start="15" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
       <rule type="fa" cat="S[adj]\NP">
        <lf start="16" span="1" word="greater_than_or_equal" lemma="greater_than_or_equal" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
        <rule type="fa" cat="PP">
         <lf start="17" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
         <rule type="fa" cat="NP[nb]">
          <lf start="18" span="1" word="that" lemma="that" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
          <lf start="19" span="1" word="child’s" lemma="child’s" pos="NNS" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="20" span="1" word="greed" lemma="gree" pos="VBD" chunk="I-NP" entity="O" cat="(S[dcl]\NP)/NP" />
     <rule type="lex" cat="NP">
      <lf start="21" span="1" word="factor" lemma="factor" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
   <rule type="conj" cat="S[dcl]\S[dcl]">
    <lf start="22" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
    <rule type="ba" cat="S[dcl]">
     <rule type="ba" cat="NP">
      <rule type="ba" cat="NP">
       <rule type="fa" cat="NP[nb]">
        <lf start="23" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="24" span="1" word="number" lemma="number" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
       <rule type="fa" cat="NP\NP">
        <lf start="25" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
        <rule type="lex" cat="NP">
         <rule type="fa" cat="N">
          <lf start="26" span="1" word="content" lemma="content" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="27" span="1" word="children" lemma="child" pos="NNS" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
      <rule type="fa" cat="NP\NP">
       <lf start="28" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
       <rule type="fa" cat="NP[nb]">
        <lf start="29" span="1" word="that" lemma="that" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="30" span="1" word="assignment" lemma="assignment" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
     <rule type="ba" cat="S[dcl]\NP">
      <rule type="fa" cat="S[dcl]\NP">
       <lf start="31" span="1" word="equals" lemma="equal" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
       <rule type="lex" cat="NP">
        <lf start="32" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
      <lf start="33" span="1" word="int" lemma="int" pos="NN" chunk="I-NP" entity="O" cat="(S\NP)\(S\NP)" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="34" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
