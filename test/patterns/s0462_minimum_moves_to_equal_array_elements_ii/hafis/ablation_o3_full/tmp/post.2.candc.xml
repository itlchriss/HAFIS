<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="int" lemma="int" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="always" lemma="always" pos="RB" chunk="I-ADVP" entity="O" cat="(S\NP)/(S\NP)" />
    <rule type="ba" cat="S[dcl]\NP">
     <lf start="4" span="1" word="fits" lemma="fit" pos="VBZ" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="5" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
      <rule type="ba" cat="NP">
       <rule type="fa" cat="NP[nb]">
        <lf start="6" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="7" span="1" word="range" lemma="range" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
       <rule type="lex" cat="NP\NP">
        <rule type="ba" cat="S[pss]\NP">
         <lf start="8" span="1" word="supported" lemma="support" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
         <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
          <lf start="9" span="1" word="by" lemma="by" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
          <rule type="fa" cat="NP[nb]">
           <lf start="10" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
           <rule type="fa" cat="N">
            <lf start="11" span="1" word="signed" lemma="signed" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
            <rule type="fa" cat="N">
             <rule type="fa" cat="N/N">
              <rule type="fa" cat="(N/N)/(N/N)">
               <lf start="12" span="1" word="30" lemma="30" pos="CD" chunk="I-NP" entity="O" cat="((N/N)/(N/N))/((N/N)/(N/N))" />
               <lf start="13" span="1" word="2" lemma="2" pos="CD" chunk="I-NP" entity="O" cat="(N/N)/(N/N)" />
              </rule>
              <lf start="14" span="1" word="bit" lemma="bit" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
             </rule>
             <lf start="15" span="1" word="integer" lemma="integer" pos="NN" chunk="I-NP" entity="O" cat="N" />
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="16" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
