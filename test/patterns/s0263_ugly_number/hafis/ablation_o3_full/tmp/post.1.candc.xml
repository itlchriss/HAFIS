<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="fa" cat="S[dcl]">
   <rule type="fa" cat="S[X]/S[X]">
    <lf start="0" span="1" word="If" lemma="if" pos="IN" chunk="I-SBAR" entity="O" cat="(S/S)/NP" />
    <rule type="lex" cat="NP">
     <rule type="fa" cat="N">
      <lf start="1" span="1" word="int" lemma="int" pos="NN" chunk="I-NP" entity="O" cat="N/S[dcl]" />
      <rule type="ba" cat="S[dcl]">
       <rule type="fa" cat="NP[nb]">
        <lf start="2" span="1" word="the" lemma="the" pos="DT" chunk="B-NP" entity="O" cat="NP[nb]/N" />
        <rule type="fa" cat="N">
         <lf start="3" span="1" word="type_character_" lemma="type_character_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="4" span="1" word="chrs_a" lemma="chrs_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
       <rule type="fa" cat="S[dcl]\NP">
        <lf start="5" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
        <rule type="fa" cat="S[adj]\NP">
         <lf start="6" span="1" word="greater_than" lemma="greater_than" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
         <rule type="fa" cat="PP">
          <lf start="7" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
          <rule type="ba" cat="NP">
           <rule type="lex" cat="NP">
            <lf start="8" span="1" word="1" lemma="1" pos="CD" chunk="I-NP" entity="O" cat="N" />
           </rule>
           <rule type="conj" cat="NP\NP">
            <lf start="9" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
            <rule type="ba" cat="NP">
             <rule type="fa" cat="NP[nb]">
              <lf start="10" span="1" word="every" lemma="every" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
              <rule type="fa" cat="N">
               <lf start="11" span="1" word="prime" lemma="prime" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
               <lf start="12" span="1" word="factor" lemma="factor" pos="NN" chunk="I-NP" entity="O" cat="N" />
              </rule>
             </rule>
             <rule type="fa" cat="NP\NP">
              <lf start="13" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
              <rule type="lex" cat="NP">
               <lf start="14" span="1" word="int" lemma="int" pos="NN" chunk="I-NP" entity="O" cat="N" />
              </rule>
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
   <rule type="ba" cat="S[dcl]">
    <rule type="fa" cat="NP[nb]">
     <lf start="15" span="1" word="the" lemma="the" pos="DT" chunk="B-NP" entity="O" cat="NP[nb]/N" />
     <rule type="fa" cat="N">
      <lf start="16" span="1" word="type_character_" lemma="type_character_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="17" span="1" word="chrs_b" lemma="chrs_b" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="18" span="1" word="belongs" lemma="belong" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/PP" />
     <rule type="fa" cat="PP">
      <lf start="19" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
      <rule type="ba" cat="NP">
       <rule type="fa" cat="NP[nb]">
        <lf start="20" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="21" span="1" word="set" lemma="set" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
       <rule type="fa" cat="NP\NP">
        <lf start="22" span="1" word="235" lemma="235" pos="CD" chunk="I-NP" entity="O" cat="(NP\NP)/(NP\NP)" />
        <rule type="lex" cat="NP\NP">
         <rule type="fa" cat="S[dcl]">
          <lf start="23" span="1" word="then" lemma="then" pos="RB" chunk="I-NP" entity="O" cat="S/S" />
          <rule type="ba" cat="S[dcl]">
           <rule type="lex" cat="NP">
            <rule type="fa" cat="N">
             <lf start="24" span="1" word="type_boolean_" lemma="type_boolean_" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
             <lf start="25" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
            </rule>
           </rule>
           <rule type="fa" cat="S[dcl]\NP">
            <lf start="26" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
            <rule type="fa" cat="S[adj]\NP">
             <lf start="27" span="1" word="equal" lemma="equal" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
             <rule type="fa" cat="PP">
              <lf start="28" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
              <rule type="fa" cat="NP[nb]">
               <lf start="29" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
               <rule type="fa" cat="N">
                <lf start="30" span="1" word="true" lemma="true" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
                <lf start="31" span="1" word="literal" lemma="literal" pos="NN" chunk="I-NP" entity="O" cat="N" />
               </rule>
              </rule>
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="32" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
