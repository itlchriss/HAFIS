<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="type_string_" lemma="type_string_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
    <rule type="ba" cat="S[adj]\NP">
     <lf start="4" span="1" word="equal" lemma="equal" pos="JJ" chunk="I-ADJP" entity="O" cat="S[adj]\NP" />
     <rule type="fa" cat="(S[adj]\NP)\(S[adj]\NP)">
      <lf start="5" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="((S[adj]\NP)\(S[adj]\NP))/NP" />
      <rule type="ba" cat="NP">
       <rule type="lex" cat="NP">
        <rule type="fa" cat="N">
         <lf start="6" span="1" word="binary" lemma="binary" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="7" span="1" word="string" lemma="string" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
       <rule type="lex" cat="NP\NP">
        <rule type="fa" cat="S[ng]\NP">
         <lf start="8" span="1" word="representing" lemma="represent" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
         <rule type="ba" cat="NP[nb]">
          <rule type="ba" cat="NP">
           <rule type="fa" cat="NP[nb]">
            <lf start="9" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
            <lf start="10" span="1" word="sum" lemma="sum" pos="NN" chunk="I-NP" entity="O" cat="N" />
           </rule>
           <rule type="fa" cat="NP\NP">
            <lf start="11" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
            <rule type="ba" cat="NP">
             <rule type="fa" cat="NP[nb]">
              <lf start="12" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
              <rule type="fa" cat="N">
               <lf start="13" span="1" word="2" lemma="2" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
               <rule type="fa" cat="N">
                <lf start="14" span="1" word="input" lemma="input" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
                <rule type="fa" cat="N">
                 <lf start="15" span="1" word="binary" lemma="binary" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
                 <lf start="16" span="1" word="strings" lemma="string" pos="NNS" chunk="I-NP" entity="O" cat="N" />
                </rule>
               </rule>
              </rule>
             </rule>
             <rule type="fa" cat="NP\NP">
              <lf start="17" span="1" word="the" lemma="the" pos="DT" chunk="B-NP" entity="O" cat="(NP\NP)/N" />
              <rule type="fa" cat="N">
               <lf start="18" span="1" word="type_character_" lemma="type_character_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
               <lf start="19" span="1" word="chrs_a" lemma="chrs_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
              </rule>
             </rule>
            </rule>
           </rule>
          </rule>
          <rule type="conj" cat="NP[nb]\NP[nb]">
           <lf start="20" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
           <rule type="fa" cat="NP[nb]">
            <lf start="21" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
            <rule type="fa" cat="N">
             <lf start="22" span="1" word="type_character_" lemma="type_character_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
             <lf start="23" span="1" word="chrs_b" lemma="chrs_b" pos="NN" chunk="I-NP" entity="O" cat="N" />
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="24" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
