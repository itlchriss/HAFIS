<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="NP[nb]">
  <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
  <rule type="fa" cat="N">
   <lf start="1" span="1" word="param_String_" lemma="param_String_" pos="NN" chunk="I-NP" entity="O" cat="N/S[dcl]" />
   <rule type="rp" cat="S[dcl]">
    <rule type="ba" cat="S[dcl]">
     <rule type="fa" cat="NP[nb]">
      <lf start="2" span="1" word="the" lemma="the" pos="DT" chunk="B-NP" entity="O" cat="NP[nb]/N" />
      <rule type="fa" cat="N">
       <lf start="3" span="1" word="type_character_" lemma="type_character_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="4" span="1" word="chrs_a" lemma="chrs_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="5" span="1" word="must" lemma="must" pos="MD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[b]\NP)" />
      <rule type="fa" cat="S[b]\NP">
       <lf start="6" span="1" word="contain" lemma="contain" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/NP" />
       <rule type="ba" cat="NP">
        <rule type="fa" cat="NP">
         <lf start="7" span="1" word="exclusively" lemma="exclusively" pos="RB" chunk="I-ADVP" entity="O" cat="NP/NP" />
         <rule type="fa" cat="NP[nb]">
          <lf start="8" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
          <rule type="fa" cat="N">
           <lf start="9" span="1" word="2" lemma="2" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
           <lf start="10" span="1" word="characters" lemma="character" pos="NNS" chunk="I-NP" entity="O" cat="N" />
          </rule>
         </rule>
        </rule>
        <rule type="fa" cat="NP\NP">
         <lf start="11" span="1" word="that" lemma="that" pos="WDT" chunk="B-NP" entity="O" cat="(NP\NP)/(S[dcl]\NP)" />
         <rule type="fa" cat="S[dcl]\NP">
          <lf start="12" span="1" word="represent" lemma="represent" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
          <rule type="ba" cat="NP[nb]">
           <rule type="fa" cat="NP[nb]">
            <lf start="13" span="1" word="an" lemma="an" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
            <rule type="fa" cat="N">
             <lf start="14" span="1" word="opening" lemma="opening" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
             <lf start="15" span="1" word="parenthesis" lemma="parenthesis" pos="NN" chunk="I-NP" entity="O" cat="N" />
            </rule>
           </rule>
           <rule type="conj" cat="NP[nb]\NP[nb]">
            <lf start="16" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
            <rule type="fa" cat="NP[nb]">
             <lf start="17" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
             <rule type="fa" cat="N">
              <lf start="18" span="1" word="closing" lemma="closing" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
              <lf start="19" span="1" word="parenthesis" lemma="parenthesis" pos="NN" chunk="I-NP" entity="O" cat="N" />
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
    <lf start="20" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
   </rule>
  </rule>
 </rule>
</ccg>
</candc>
