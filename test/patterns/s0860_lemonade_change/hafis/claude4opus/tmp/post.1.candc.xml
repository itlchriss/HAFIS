<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="S[dcl]">
  <rule type="fa" cat="S[X]/S[X]">
   <lf start="0" span="1" word="If" lemma="if" pos="IN" chunk="I-SBAR" entity="O" cat="(S/S)/S[dcl]" />
   <rule type="ba" cat="S[dcl]">
    <rule type="fa" cat="NP[nb]">
     <lf start="1" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <rule type="fa" cat="N">
      <lf start="2" span="1" word="type_boolean_" lemma="type_boolean_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="3" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="4" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
     <rule type="ba" cat="S[adj]\NP">
      <lf start="5" span="1" word="equal" lemma="equal" pos="JJ" chunk="I-ADJP" entity="O" cat="S[adj]\NP" />
      <rule type="fa" cat="(S[adj]\NP)\(S[adj]\NP)">
       <lf start="6" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="((S[adj]\NP)\(S[adj]\NP))/NP" />
       <rule type="fa" cat="NP[nb]">
        <lf start="7" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <rule type="fa" cat="N">
         <lf start="8" span="1" word="false" lemma="false" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="9" span="1" word="literal" lemma="literal" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <rule type="lp" cat="S[dcl]">
   <lf start="10" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
   <rule type="fa" cat="S[dcl]">
    <rule type="fa" cat="S[X]/S[X]">
     <lf start="11" span="1" word="at" lemma="at" pos="IN" chunk="I-NP" entity="O" cat="(S/S)/(S[asup]\NP)" />
     <lf start="12" span="1" word="least" lemma="least" pos="JJS" chunk="I-NP" entity="O" cat="S[asup]\NP" />
    </rule>
    <rule type="rp" cat="S[dcl]">
     <rule type="ba" cat="S[dcl]">
      <rule type="lex" cat="NP">
       <rule type="fa" cat="N">
        <lf start="13" span="1" word="1" lemma="1" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="14" span="1" word="customer" lemma="customer" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
      <rule type="fa" cat="S[dcl]\NP">
       <rule type="bx" cat="(S[dcl]\NP)/(S[b]\NP)">
        <lf start="15" span="1" word="does" lemma="do" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[b]\NP)" />
        <lf start="16" span="1" word="not" lemma="not" pos="RB" chunk="I-VP" entity="O" cat="(S\NP)\(S\NP)" />
       </rule>
       <rule type="fa" cat="S[b]\NP">
        <lf start="17" span="1" word="receive" lemma="receive" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/NP" />
        <rule type="lex" cat="NP">
         <rule type="fa" cat="N">
          <lf start="18" span="1" word="correct" lemma="correct" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="19" span="1" word="change" lemma="change" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
     <lf start="20" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
    </rule>
   </rule>
  </rule>
 </rule>
</ccg>
</candc>
