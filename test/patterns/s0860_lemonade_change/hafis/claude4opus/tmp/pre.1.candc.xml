<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <rule type="ba" cat="NP[nb]/N">
     <rule type="fa" cat="NP[nb]">
      <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <rule type="fa" cat="N">
       <lf start="1" span="1" word="type_integer_array_" lemma="type_integer_array_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="2" span="1" word="param_bills_" lemma="param_bills_" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <lf start="3" span="1" word="'s" lemma="'s" pos="POS" chunk="B-NP" entity="O" cat="(NP[nb]/N)\NP" />
    </rule>
    <lf start="4" span="1" word="values" lemma="value" pos="NNS" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="5" span="1" word="are" lemma="be" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
    <rule type="fa" cat="S[adj]\NP">
     <lf start="6" span="1" word="equal" lemma="equal" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
     <rule type="fa" cat="PP">
      <lf start="7" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
      <rule type="lex" cat="NP">
       <rule type="ba" cat="N">
        <lf start="8" span="1" word="5" lemma="5" pos="CD" chunk="I-NP" entity="I-DAT" cat="N" />
        <rule type="conj" cat="N\N">
         <lf start="9" span="1" word="," lemma="," pos="," chunk="I-NP" entity="I-DAT" cat="," />
         <rule type="ba" cat="N">
          <lf start="10" span="1" word="10" lemma="10" pos="CD" chunk="I-NP" entity="I-DAT" cat="N" />
          <rule type="conj" cat="N\N">
           <lf start="11" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
           <rule type="conj" cat="N\N">
            <lf start="12" span="1" word="or" lemma="or" pos="CC" chunk="O" entity="O" cat="conj" />
            <lf start="13" span="1" word="20" lemma="20" pos="CD" chunk="I-NP" entity="I-DAT" cat="N" />
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="14" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
