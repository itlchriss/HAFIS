<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="S[dcl]">
  <rule type="fa" cat="S[X]/S[X]">
   <lf start="0" span="1" word="If" lemma="if" pos="IN" chunk="I-SBAR" entity="O" cat="(S/S)/S[dcl]" />
   <rule type="ba" cat="S[dcl]">
    <rule type="fa" cat="NP[nb]">
     <lf start="1" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <rule type="fa" cat="N">
      <lf start="2" span="1" word="type_boolean_" lemma="type_boolean_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="3" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="4" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
     <rule type="fa" cat="S[adj]\NP">
      <lf start="5" span="1" word="equal" lemma="equal" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
      <rule type="fa" cat="PP">
       <lf start="6" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
       <rule type="fa" cat="NP[nb]">
        <lf start="7" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <rule type="fa" cat="N">
         <lf start="8" span="1" word="true" lemma="true" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="9" span="1" word="literal" lemma="literal" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <rule type="rp" cat="S[dcl]">
   <rule type="lp" cat="S[dcl]">
    <lf start="10" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
    <rule type="ba" cat="S[dcl]">
     <lf start="11" span="1" word="it" lemma="it" pos="PRP" chunk="I-NP" entity="O" cat="NP[expl]" />
     <rule type="fa" cat="S[dcl]\NP[expl]">
      <rule type="fa" cat="(S[dcl]\NP[expl])/(S[to]\NP)">
       <lf start="12" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="((S[dcl]\NP[expl])/(S[to]\NP))/(S[adj]\NP)" />
       <lf start="13" span="1" word="possible" lemma="possible" pos="JJ" chunk="I-ADJP" entity="O" cat="S[adj]\NP" />
      </rule>
      <rule type="fa" cat="S[to]\NP">
       <lf start="14" span="1" word="to" lemma="to" pos="TO" chunk="I-VP" entity="O" cat="(S[to]\NP)/(S[b]\NP)" />
       <rule type="ba" cat="S[b]\NP">
        <rule type="fa" cat="S[b]\NP">
         <rule type="fa" cat="(S[b]\NP)/PP">
          <lf start="15" span="1" word="provide" lemma="provide" pos="VB" chunk="I-VP" entity="O" cat="((S[b]\NP)/PP)/NP" />
          <rule type="fa" cat="NP[nb]">
           <lf start="16" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
           <rule type="fa" cat="N">
            <lf start="17" span="1" word="correct" lemma="correct" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
            <lf start="18" span="1" word="change" lemma="change" pos="NN" chunk="I-NP" entity="O" cat="N" />
           </rule>
          </rule>
         </rule>
         <rule type="fa" cat="PP">
          <lf start="19" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
          <rule type="fa" cat="NP[nb]">
           <lf start="20" span="1" word="every" lemma="every" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
           <lf start="21" span="1" word="customer" lemma="customer" pos="NN" chunk="I-NP" entity="O" cat="N" />
          </rule>
         </rule>
        </rule>
        <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
         <lf start="22" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
         <rule type="fa" cat="NP[nb]">
          <lf start="23" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
          <rule type="fa" cat="N">
           <lf start="24" span="1" word="type_integer_array_" lemma="type_integer_array_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
           <lf start="25" span="1" word="param_bills_" lemma="param_bills_" pos="NN" chunk="I-NP" entity="O" cat="N" />
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
   <lf start="26" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
</candc>
