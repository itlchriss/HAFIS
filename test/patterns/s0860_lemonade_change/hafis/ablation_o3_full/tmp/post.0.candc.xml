<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[b]">
  <rule type="ba" cat="S[b]">
   <rule type="ba" cat="S[b]">
    <rule type="ba" cat="NP">
     <rule type="lex" cat="NP">
      <rule type="fa" cat="N">
       <lf start="0" span="1" word="boolean" lemma="boolean" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="1" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <rule type="lex" cat="NP\NP">
      <rule type="ba" cat="S[dcl]\NP">
       <rule type="fa" cat="S[dcl]\NP">
        <lf start="2" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
        <rule type="fa" cat="S[adj]\NP">
         <lf start="3" span="1" word="equal" lemma="equal" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
         <rule type="fa" cat="PP">
          <lf start="4" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
          <rule type="fa" cat="NP[nb]">
           <lf start="5" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
           <rule type="fa" cat="N">
            <lf start="6" span="1" word="true" lemma="true" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
            <lf start="7" span="1" word="literal" lemma="literal" pos="NN" chunk="I-NP" entity="O" cat="N" />
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
       <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
        <lf start="8" span="1" word="exclusively" lemma="exclusively" pos="RB" chunk="I-ADVP" entity="O" cat="((S\NP)\(S\NP))/((S\NP)\(S\NP))" />
        <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
         <lf start="9" span="1" word="when" lemma="when" pos="WRB" chunk="B-ADVP" entity="O" cat="((S\NP)\(S\NP))/S[dcl]" />
         <rule type="ba" cat="S[dcl]">
          <rule type="ba" cat="NP">
           <rule type="fa" cat="NP[nb]">
            <lf start="10" span="1" word="every" lemma="every" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
            <lf start="11" span="1" word="customer" lemma="customer" pos="NN" chunk="I-NP" entity="O" cat="N" />
           </rule>
           <rule type="lex" cat="NP\NP">
            <rule type="ba" cat="S[pss]\NP">
             <lf start="12" span="1" word="represented" lemma="represent" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
             <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
              <lf start="13" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
              <rule type="lex" cat="NP">
               <rule type="fa" cat="N">
                <lf start="14" span="1" word="int" lemma="int" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
                <lf start="15" span="1" word="[" lemma="[" pos="NNS" chunk="I-NP" entity="O" cat="N" />
               </rule>
              </rule>
             </rule>
            </rule>
           </rule>
          </rule>
          <rule type="fa" cat="S[dcl]\NP">
           <lf start="16" span="1" word="]" lemma="]" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
           <rule type="fa" cat="NP[nb]">
            <lf start="17" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
            <lf start="18" span="1" word="chry_a" lemma="chry_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
    <rule type="fa" cat="S[b]\NP">
     <lf start="19" span="1" word="receive" lemma="receive" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/NP" />
     <rule type="ba" cat="NP">
      <rule type="fa" cat="NP[nb]">
       <lf start="20" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <rule type="fa" cat="N">
        <lf start="21" span="1" word="correct" lemma="correct" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="22" span="1" word="change" lemma="change" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
      <rule type="lex" cat="NP\NP">
       <rule type="ba" cat="S[ng]\NP">
        <rule type="fa" cat="S[ng]\NP">
         <lf start="23" span="1" word="starting" lemma="start" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/PP" />
         <rule type="fa" cat="PP">
          <lf start="24" span="1" word="from" lemma="from" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
          <rule type="fa" cat="NP[nb]">
           <lf start="25" span="1" word="no" lemma="no" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
           <rule type="fa" cat="N">
            <lf start="26" span="1" word="initial" lemma="initial" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
            <lf start="27" span="1" word="cash" lemma="cash" pos="NN" chunk="I-NP" entity="O" cat="N" />
           </rule>
          </rule>
         </rule>
        </rule>
        <lf start="28" span="1" word="otherwise" lemma="otherwise" pos="RB" chunk="I-NP" entity="O" cat="(S\NP)\(S\NP)" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
   <rule type="lex" cat="S[X]\S[X]">
    <rule type="ba" cat="S[dcl]">
     <rule type="lex" cat="NP">
      <lf start="29" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="30" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
      <rule type="fa" cat="S[adj]\NP">
       <lf start="31" span="1" word="equal" lemma="equal" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
       <rule type="fa" cat="PP">
        <lf start="32" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
        <rule type="fa" cat="NP[nb]">
         <lf start="33" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <rule type="fa" cat="N">
          <lf start="34" span="1" word="false" lemma="false" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="35" span="1" word="literal" lemma="literal" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="36" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
