<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="S[dcl]">
  <rule type="fa" cat="S[X]/S[X]">
   <lf start="0" span="1" word="If" lemma="if" pos="IN" chunk="I-SBAR" entity="O" cat="(S/S)/S[dcl]" />
   <rule type="ba" cat="S[dcl]">
    <rule type="fa" cat="NP[nb]">
     <lf start="1" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <rule type="fa" cat="N">
      <lf start="2" span="1" word="type_boolean_" lemma="type_boolean_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="3" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="4" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
     <rule type="fa" cat="S[adj]\NP">
      <lf start="5" span="1" word="equal" lemma="equal" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
      <rule type="fa" cat="PP">
       <lf start="6" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
       <rule type="fa" cat="NP[nb]">
        <lf start="7" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <rule type="fa" cat="N">
         <lf start="8" span="1" word="true" lemma="true" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="9" span="1" word="literal" lemma="literal" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <rule type="lp" cat="S[dcl]">
   <lf start="10" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
   <rule type="rp" cat="S[dcl]">
    <rule type="ba" cat="S[dcl]">
     <lf start="11" span="1" word="it" lemma="it" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="12" span="1" word="means" lemma="mean" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/S[em]" />
      <rule type="fa" cat="S[em]">
       <lf start="13" span="1" word="that" lemma="that" pos="IN" chunk="I-SBAR" entity="O" cat="S[em]/S[dcl]" />
       <rule type="fa" cat="S[dcl]">
        <rule type="fa" cat="S[X]/S[X]">
         <lf start="14" span="1" word="for" lemma="for" pos="IN" chunk="I-PP" entity="O" cat="(S/S)/NP" />
         <rule type="fa" cat="NP[nb]">
          <lf start="15" span="1" word="each" lemma="each" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
          <lf start="16" span="1" word="customer" lemma="customer" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
        <rule type="lp" cat="S[dcl]">
         <lf start="17" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
         <rule type="ba" cat="S[dcl]">
          <rule type="fa" cat="NP[nb]">
           <lf start="18" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
           <rule type="fa" cat="N">
            <lf start="19" span="1" word="correct" lemma="correct" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
            <lf start="20" span="1" word="change" lemma="change" pos="NN" chunk="I-NP" entity="O" cat="N" />
           </rule>
          </rule>
          <rule type="fa" cat="S[dcl]\NP">
           <lf start="21" span="1" word="was" lemma="be" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pss]\NP)" />
           <rule type="ba" cat="S[pss]\NP">
            <lf start="22" span="1" word="provided" lemma="provide" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
            <rule type="lex" cat="(S[X]\NP)\(S[X]\NP)">
             <rule type="fa" cat="S[adj]\NP">
              <lf start="23" span="1" word="such" lemma="such" pos="JJ" chunk="I-PP" entity="O" cat="(S[adj]\NP)/S[em]" />
              <rule type="fa" cat="S[em]">
               <lf start="24" span="1" word="that" lemma="that" pos="IN" chunk="I-SBAR" entity="O" cat="S[em]/S[dcl]" />
               <rule type="ba" cat="S[dcl]">
                <rule type="fa" cat="NP[nb]">
                 <lf start="25" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
                 <rule type="fa" cat="N">
                  <lf start="26" span="1" word="net" lemma="net" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
                  <lf start="27" span="1" word="transaction" lemma="transaction" pos="NN" chunk="I-NP" entity="O" cat="N" />
                 </rule>
                </rule>
                <rule type="fa" cat="S[dcl]\NP">
                 <lf start="28" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/S[em]" />
                 <rule type="fa" cat="S[em]">
                  <lf start="29" span="1" word="that" lemma="that" pos="IN" chunk="I-SBAR" entity="O" cat="S[em]/S[dcl]" />
                  <rule type="ba" cat="S[dcl]">
                   <rule type="fa" cat="NP[nb]">
                    <lf start="30" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
                    <lf start="31" span="1" word="customer" lemma="customer" pos="NN" chunk="I-NP" entity="O" cat="N" />
                   </rule>
                   <rule type="fa" cat="S[dcl]\NP">
                    <lf start="32" span="1" word="pays" lemma="pay" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
                    <rule type="lex" cat="NP">
                     <rule type="fa" cat="N">
                      <lf start="33" span="1" word="$" lemma="$" pos="$" chunk="I-NP" entity="I-MON" cat="N/N[num]" />
                      <lf start="34" span="1" word="5" lemma="5" pos="CD" chunk="I-NP" entity="I-MON" cat="N[num]" />
                     </rule>
                    </rule>
                   </rule>
                  </rule>
                 </rule>
                </rule>
               </rule>
              </rule>
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
    <lf start="35" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
   </rule>
  </rule>
 </rule>
</ccg>
</candc>
