<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <rule type="fa" cat="N">
     <lf start="0" span="1" word="int" lemma="int" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
     <rule type="fa" cat="N">
      <lf start="1" span="1" word="[" lemma="[" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
      <rule type="fa" cat="N">
       <lf start="2" span="1" word="]" lemma="]" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="3" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="4" span="1" word="ensures" lemma="ensure" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/S[em]" />
    <rule type="fa" cat="S[em]">
     <lf start="5" span="1" word="that" lemma="that" pos="IN" chunk="I-SBAR" entity="O" cat="S[em]/S[dcl]" />
     <rule type="fa" cat="S[dcl]">
      <rule type="fa" cat="S[X]/S[X]">
       <lf start="6" span="1" word="for" lemma="for" pos="IN" chunk="I-PP" entity="O" cat="(S/S)/NP" />
       <rule type="ba" cat="NP">
        <rule type="fa" cat="NP[nb]">
         <lf start="7" span="1" word="every" lemma="every" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <rule type="fa" cat="N">
          <lf start="8" span="1" word="index" lemma="index" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="9" span="1" word="i" lemma="i" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
        <rule type="fa" cat="NP\NP">
         <lf start="10" span="1" word="with" lemma="with" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
         <rule type="ba" cat="NP">
          <rule type="lex" cat="NP">
           <lf start="11" span="1" word="i" lemma="i" pos="NN" chunk="I-NP" entity="O" cat="N" />
          </rule>
          <rule type="ba" cat="NP\NP">
           <rule type="lex" cat="NP\NP">
            <rule type="fa" cat="S[dcl]\NP">
             <lf start="12" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
             <rule type="fa" cat="S[adj]\NP">
              <lf start="13" span="1" word="greater_than_or_equal" lemma="greater_than_or_equal" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
              <rule type="fa" cat="PP">
               <lf start="14" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
               <rule type="lex" cat="NP">
                <lf start="15" span="1" word="0" lemma="0" pos="CD" chunk="I-NP" entity="O" cat="N" />
               </rule>
              </rule>
             </rule>
            </rule>
           </rule>
           <rule type="conj" cat="(NP\NP)\(NP\NP)">
            <lf start="16" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
            <rule type="lex" cat="NP\NP">
             <rule type="fc" cat="S[dcl]/NP">
              <rule type="tr" cat="S[X]/(S[X]\NP)">
               <rule type="lex" cat="NP">
                <lf start="17" span="1" word="i" lemma="i" pos="NNP" chunk="I-NP" entity="O" cat="N" />
               </rule>
              </rule>
              <rule type="fc" cat="(S[dcl]\NP)/NP">
               <lf start="18" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
               <rule type="fc" cat="(S[adj]\NP)/NP">
                <lf start="19" span="1" word="less_than" lemma="less_than" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
                <lf start="20" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
               </rule>
              </rule>
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
      <rule type="ba" cat="S[dcl]">
       <rule type="ba" cat="NP">
        <rule type="lex" cat="NP">
         <lf start="21" span="1" word="length" lemma="length" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
        <rule type="fa" cat="NP\NP">
         <lf start="22" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/(S[ng]\NP)" />
         <rule type="fa" cat="S[ng]\NP">
          <lf start="23" span="1" word="String" lemma="string" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
          <rule type="ba" cat="NP">
           <rule type="fa" cat="NP[nb]">
            <lf start="24" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
            <rule type="fa" cat="N">
             <lf start="25" span="1" word="type_character_" lemma="type_character_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
             <lf start="26" span="1" word="chrs_a" lemma="chrs_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
            </rule>
           </rule>
           <rule type="fa" cat="NP\NP">
            <lf start="27" span="1" word="when" lemma="when" pos="WRB" chunk="I-ADVP" entity="O" cat="(NP\NP)/S[dcl]" />
            <rule type="ba" cat="S[dcl]">
             <rule type="ba" cat="NP">
              <rule type="fa" cat="NP[nb]">
               <lf start="28" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
               <lf start="29" span="1" word="character" lemma="character" pos="NN" chunk="I-NP" entity="O" cat="N" />
              </rule>
              <rule type="fa" cat="NP\NP">
               <lf start="30" span="1" word="at" lemma="at" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
               <rule type="ba" cat="NP">
                <rule type="lex" cat="NP">
                 <rule type="fa" cat="N">
                  <lf start="31" span="1" word="index" lemma="index" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
                  <lf start="32" span="1" word="i" lemma="i" pos="NN" chunk="I-NP" entity="O" cat="N" />
                 </rule>
                </rule>
                <rule type="fa" cat="NP\NP">
                 <lf start="33" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/(S[ng]\NP)" />
                 <rule type="fa" cat="S[ng]\NP">
                  <lf start="34" span="1" word="String" lemma="string" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
                  <rule type="fa" cat="NP[nb]">
                   <lf start="35" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
                   <rule type="fa" cat="N">
                    <lf start="36" span="1" word="type_character_" lemma="type_character_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
                    <lf start="37" span="1" word="chrs_b" lemma="chrs_b" pos="NN" chunk="I-NP" entity="O" cat="N" />
                   </rule>
                  </rule>
                 </rule>
                </rule>
               </rule>
              </rule>
             </rule>
             <rule type="fa" cat="S[dcl]\NP">
              <lf start="38" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
              <rule type="ba" cat="NP">
               <rule type="lex" cat="NP">
                <rule type="fa" cat="N">
                 <lf start="39" span="1" word="uppercase" lemma="uppercase" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
                 <lf start="40" span="1" word="D" lemma="D" pos="NNP" chunk="I-NP" entity="O" cat="N" />
                </rule>
               </rule>
               <rule type="lex" cat="NP\NP">
                <rule type="fa" cat="S[dcl]">
                 <lf start="41" span="1" word="then" lemma="then" pos="RB" chunk="I-ADVP" entity="O" cat="S/S" />
                 <rule type="ba" cat="S[dcl]">
                  <rule type="ba" cat="NP">
                   <rule type="fa" cat="NP[nb]">
                    <lf start="42" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
                    <lf start="43" span="1" word="element" lemma="element" pos="NN" chunk="I-NP" entity="O" cat="N" />
                   </rule>
                   <rule type="fa" cat="NP\NP">
                    <lf start="44" span="1" word="at" lemma="at" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
                    <rule type="ba" cat="NP">
                     <rule type="lex" cat="NP">
                      <rule type="fa" cat="N">
                       <lf start="45" span="1" word="index" lemma="index" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
                       <lf start="46" span="1" word="i" lemma="i" pos="NN" chunk="I-NP" entity="O" cat="N" />
                      </rule>
                     </rule>
                     <rule type="fa" cat="NP\NP">
                      <lf start="47" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
                      <rule type="lex" cat="NP">
                       <rule type="fa" cat="N">
                        <lf start="48" span="1" word="int" lemma="int" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
                        <rule type="fa" cat="N">
                         <lf start="49" span="1" word="[" lemma="[" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
                         <rule type="fa" cat="N">
                          <lf start="50" span="1" word="]" lemma="]" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
                          <lf start="51" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
                         </rule>
                        </rule>
                       </rule>
                      </rule>
                     </rule>
                    </rule>
                   </rule>
                  </rule>
                  <rule type="ba" cat="S[dcl]\NP">
                   <rule type="fa" cat="S[dcl]\NP">
                    <lf start="52" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
                    <rule type="fa" cat="S[adj]\NP">
                     <lf start="53" span="1" word="greater_than" lemma="greater_than" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
                     <rule type="fa" cat="PP">
                      <lf start="54" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
                      <rule type="fa" cat="NP[nb]">
                       <lf start="55" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
                       <lf start="56" span="1" word="element" lemma="element" pos="NN" chunk="I-NP" entity="O" cat="N" />
                      </rule>
                     </rule>
                    </rule>
                   </rule>
                   <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
                    <lf start="57" span="1" word="at" lemma="at" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
                    <rule type="lex" cat="NP">
                     <rule type="fa" cat="N">
                      <lf start="58" span="1" word="index" lemma="index" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
                      <lf start="59" span="1" word="i" lemma="i" pos="NN" chunk="I-NP" entity="O" cat="N" />
                     </rule>
                    </rule>
                   </rule>
                  </rule>
                 </rule>
                </rule>
               </rule>
              </rule>
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
       <rule type="ba" cat="S[dcl]\NP">
        <rule type="ba" cat="S[dcl]\NP">
         <lf start="60" span="1" word="+" lemma="+" pos="VBD" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
         <lf start="61" span="1" word="1" lemma="1" pos="CD" chunk="I-NP" entity="O" cat="(S\NP)\(S\NP)" />
        </rule>
        <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
         <lf start="62" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
         <rule type="lex" cat="NP">
          <rule type="fa" cat="N">
           <lf start="63" span="1" word="int" lemma="int" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
           <rule type="fa" cat="N">
            <lf start="64" span="1" word="[" lemma="[" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
            <rule type="fa" cat="N">
             <lf start="65" span="1" word="]" lemma="]" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
             <lf start="66" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="67" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
