<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <rule type="fa" cat="N">
     <lf start="0" span="1" word="int" lemma="int" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
     <rule type="fa" cat="N">
      <lf start="1" span="1" word="[" lemma="[" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
      <rule type="fa" cat="N">
       <lf start="2" span="1" word="]" lemma="]" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="3" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="4" span="1" word="ensures" lemma="ensure" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="ba" cat="NP">
     <rule type="lex" cat="NP">
      <lf start="5" span="1" word="length" lemma="length" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
     <rule type="lex" cat="NP\NP">
      <rule type="fa" cat="S[dcl]\NP">
       <lf start="6" span="1" word="equals" lemma="equal" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
       <rule type="ba" cat="NP">
        <rule type="lex" cat="NP">
         <lf start="7" span="1" word="length" lemma="length" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
        <rule type="fa" cat="NP\NP">
         <lf start="8" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/(S[ng]\NP)" />
         <rule type="fa" cat="S[ng]\NP">
          <lf start="9" span="1" word="String" lemma="string" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
          <rule type="ba" cat="NP">
           <rule type="fa" cat="NP[nb]">
            <lf start="10" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
            <rule type="fa" cat="N">
             <lf start="11" span="1" word="type_character_" lemma="type_character_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
             <lf start="12" span="1" word="chrs_a" lemma="chrs_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
            </rule>
           </rule>
           <rule type="lex" cat="NP\NP">
            <rule type="ba" cat="S[dcl]\NP">
             <lf start="13" span="1" word="+" lemma="+" pos="VBD" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
             <lf start="14" span="1" word="1" lemma="1" pos="CD" chunk="I-NP" entity="O" cat="(S\NP)\(S\NP)" />
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="15" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
