<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="ba" cat="NP">
    <rule type="lex" cat="NP">
     <lf start="0" span="1" word="Ensures" lemma="ensure" pos="NNS" chunk="I-NP" entity="O" cat="N" />
    </rule>
    <rule type="lex" cat="NP\NP">
     <rule type="ba" cat="S[ng]\NP">
      <rule type="fa" cat="S[ng]\NP">
       <lf start="1" span="1" word="starting" lemma="start" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/PP" />
       <rule type="fa" cat="PP">
        <lf start="2" span="1" word="at" lemma="at" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
        <rule type="ba" cat="NP">
         <rule type="lex" cat="NP">
          <lf start="3" span="1" word="index" lemma="index" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
         <lf start="4" span="1" word="0" lemma="0" pos="CD" chunk="B-NP" entity="O" cat="NP\NP" />
        </rule>
       </rule>
      </rule>
      <rule type="conj" cat="(S[ng]\NP)\(S[ng]\NP)">
       <lf start="5" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
       <rule type="fa" cat="S[ng]\NP">
        <lf start="6" span="1" word="obeying" lemma="obey" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
        <rule type="ba" cat="NP">
         <rule type="fa" cat="NP[nb]">
          <lf start="7" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
          <rule type="fa" cat="N">
           <lf start="8" span="1" word="maximum" lemma="maximum" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
           <rule type="fa" cat="N">
            <lf start="9" span="1" word="jump" lemma="jump" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
            <lf start="10" span="1" word="lengths" lemma="length" pos="NNS" chunk="I-NP" entity="O" cat="N" />
           </rule>
          </rule>
         </rule>
         <rule type="lex" cat="NP\NP">
          <rule type="ba" cat="S[pss]\NP">
           <lf start="11" span="1" word="given" lemma="give" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
           <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
            <lf start="12" span="1" word="by" lemma="by" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
            <rule type="lex" cat="NP">
             <rule type="fa" cat="N">
              <lf start="13" span="1" word="int" lemma="int" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
              <lf start="14" span="1" word="[" lemma="[" pos="NNS" chunk="I-NP" entity="O" cat="N" />
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="15" span="1" word="]" lemma="]" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="ba" cat="NP">
     <rule type="fa" cat="NP[nb]">
      <lf start="16" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="17" span="1" word="chry_a" lemma="chry_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
     <rule type="lex" cat="NP\NP">
      <rule type="ba" cat="S[dcl]">
       <rule type="fa" cat="NP[nb]">
        <lf start="18" span="1" word="the" lemma="the" pos="DT" chunk="B-NP" entity="O" cat="NP[nb]/N" />
        <rule type="fa" cat="N">
         <lf start="19" span="1" word="last" lemma="last" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="20" span="1" word="index" lemma="index" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
       <rule type="ba" cat="S[dcl]\NP">
        <rule type="fa" cat="S[dcl]\NP">
         <lf start="21" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
         <lf start="22" span="1" word="reachable" lemma="reachable" pos="JJ" chunk="I-ADJP" entity="O" cat="S[adj]\NP" />
        </rule>
        <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
         <lf start="23" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
         <rule type="ba" cat="NP">
          <rule type="lex" cat="NP">
           <lf start="24" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
          </rule>
          <lf start="25" span="1" word="jumps" lemma="jump" pos="NNS" chunk="I-NP" entity="O" cat="NP\NP" />
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="26" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
