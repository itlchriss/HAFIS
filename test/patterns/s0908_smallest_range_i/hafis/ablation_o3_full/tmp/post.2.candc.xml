<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="ba" cat="S[dcl]">
    <rule type="ba" cat="NP">
     <rule type="lex" cat="NP">
      <rule type="fa" cat="N">
       <lf start="0" span="1" word="Otherwise" lemma="otherwise" pos="RB" chunk="I-ADVP" entity="O" cat="N/N" />
       <lf start="1" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="2" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
      <rule type="lex" cat="NP">
       <rule type="fa" cat="N">
        <lf start="3" span="1" word="type" lemma="type" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="4" span="1" word="int" lemma="int" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="5" span="1" word="equals" lemma="equal" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
     <rule type="ba" cat="NP">
      <rule type="fa" cat="NP[nb]">
       <lf start="6" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="7" span="1" word="difference" lemma="difference" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
      <rule type="fa" cat="NP\NP">
       <lf start="8" span="1" word="between" lemma="between" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
       <rule type="fa" cat="NP[nb]">
        <lf start="9" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <rule type="fa" cat="N">
         <lf start="10" span="1" word="maximum" lemma="maximum" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="11" span="1" word="value" lemma="value" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
   <rule type="conj" cat="S[dcl]\S[dcl]">
    <lf start="12" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
    <rule type="ba" cat="S[dcl]">
     <rule type="ba" cat="NP">
      <rule type="fa" cat="NP[nb]">
       <lf start="13" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <rule type="fa" cat="N">
        <lf start="14" span="1" word="minimum" lemma="minimum" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="15" span="1" word="value" lemma="value" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
      <rule type="fa" cat="NP\NP">
       <lf start="16" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
       <rule type="fa" cat="NP[nb]">
        <lf start="17" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="18" span="1" word="type_string_" lemma="type_string_" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="19" span="1" word="strs_a" lemma="strs_a" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
      <rule type="ba" cat="NP">
       <rule type="fa" cat="NP[nb]">
        <lf start="20" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <rule type="fa" cat="N">
         <lf start="21" span="1" word="type_string_" lemma="type_string_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="22" span="1" word="strs_b" lemma="strs_b" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
       <rule type="lex" cat="NP\NP">
        <rule type="lp" cat="S[dcl]">
         <lf start="23" span="1" word="-" lemma="-" pos=":" chunk="O" entity="O" cat=":" />
         <rule type="ba" cat="S[dcl]">
          <rule type="fa" cat="NP">
           <lf start="24" span="1" word="twice" lemma="twice" pos="RB" chunk="I-NP" entity="O" cat="NP/NP" />
           <rule type="fa" cat="NP[nb]">
            <lf start="25" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
            <lf start="26" span="1" word="type_string_" lemma="type_string_" pos="NN" chunk="I-NP" entity="O" cat="N" />
           </rule>
          </rule>
          <rule type="fa" cat="S[dcl]\NP">
           <lf start="27" span="1" word="strs_c" lemma="strs_c" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
           <rule type="fa" cat="NP[nb]">
            <lf start="28" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
            <rule type="fa" cat="N">
             <lf start="29" span="1" word="type_character_" lemma="type_character_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
             <lf start="30" span="1" word="chrs_d" lemma="chrs_d" pos="NN" chunk="I-NP" entity="O" cat="N" />
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="31" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
