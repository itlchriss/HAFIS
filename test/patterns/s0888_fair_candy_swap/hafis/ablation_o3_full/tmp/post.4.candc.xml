<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="S[dcl]">
  <rule type="fa" cat="S[X]/S[X]">
   <lf start="0" span="1" word="For" lemma="for" pos="IN" chunk="I-PP" entity="O" cat="(S/S)/NP" />
   <rule type="ba" cat="NP">
    <rule type="fa" cat="NP[nb]">
     <lf start="1" span="1" word="every" lemma="every" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="2" span="1" word="input" lemma="input" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
    <rule type="fa" cat="NP\NP">
     <lf start="3" span="1" word="that" lemma="that" pos="WDT" chunk="B-NP" entity="O" cat="(NP\NP)/(S[dcl]\NP)" />
     <rule type="ba" cat="S[dcl]\NP">
      <rule type="fa" cat="S[dcl]\NP">
       <lf start="4" span="1" word="meets" lemma="meet" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
       <rule type="fa" cat="NP[nb]">
        <lf start="5" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="6" span="1" word="prerequisites" lemma="prerequisite" pos="NNS" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
      <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
       <lf start="7" span="1" word="at" lemma="at" pos="IN" chunk="I-NP" entity="O" cat="((S\NP)\(S\NP))/NP" />
       <rule type="lex" cat="NP">
        <rule type="fa" cat="N">
         <lf start="8" span="1" word="least" lemma="least" pos="JJS" chunk="I-NP" entity="O" cat="N/N" />
         <rule type="fa" cat="N">
          <lf start="9" span="1" word="1" lemma="1" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="10" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <rule type="rp" cat="S[dcl]">
   <rule type="ba" cat="S[dcl]">
    <rule type="lex" cat="NP">
     <lf start="11" span="1" word="int" lemma="int" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="12" span="1" word="[" lemma="[" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
     <rule type="ba" cat="NP">
      <rule type="lex" cat="NP">
       <rule type="fa" cat="N">
        <lf start="13" span="1" word="]" lemma="]" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="14" span="1" word="meeting" lemma="meeting" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
      <rule type="lex" cat="NP\NP">
       <rule type="fc" cat="S[dcl]/NP">
        <rule type="tr" cat="S[X]/(S[X]\NP)">
         <rule type="fa" cat="NP[nb]">
          <lf start="15" span="1" word="these" lemma="these" pos="DT" chunk="B-NP" entity="O" cat="NP[nb]/N" />
          <rule type="fa" cat="N">
           <lf start="16" span="1" word="behavioural" lemma="behavioural" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
           <lf start="17" span="1" word="specifications" lemma="specification" pos="NNS" chunk="I-NP" entity="O" cat="N" />
          </rule>
         </rule>
        </rule>
        <rule type="fc" cat="(S[dcl]\NP)/NP">
         <lf start="18" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pss]\NP)" />
         <rule type="bx" cat="(S[pss]\NP)/NP">
          <lf start="19" span="1" word="guaranteed" lemma="guarantee" pos="VBN" chunk="I-VP" entity="O" cat="(S[pss]\NP)/NP" />
          <rule type="lex" cat="(S[X]\NP)\(S[X]\NP)">
           <rule type="fa" cat="S[to]\NP">
            <lf start="20" span="1" word="to" lemma="to" pos="TO" chunk="I-VP" entity="O" cat="(S[to]\NP)/(S[b]\NP)" />
            <lf start="21" span="1" word="exist" lemma="exist" pos="VB" chunk="I-VP" entity="O" cat="S[b]\NP" />
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
   <lf start="22" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
</candc>
