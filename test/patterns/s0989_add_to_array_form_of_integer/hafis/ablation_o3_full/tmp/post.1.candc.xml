<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="ba" cat="NP">
    <rule type="fa" cat="NP[nb]">
     <lf start="0" span="1" word="Each" lemma="each" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <rule type="fa" cat="N">
      <lf start="1" span="1" word="Integer" lemma="Integer" pos="NNP" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="2" span="1" word="element" lemma="element" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
    <rule type="fa" cat="NP\NP">
     <lf start="3" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
     <rule type="lex" cat="NP">
      <rule type="fa" cat="N">
       <lf start="4" span="1" word="List" lemma="List" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N/N" />
       <rule type="fa" cat="N">
        <lf start="5" span="1" word="&lt;" lemma="&lt;" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N/N" />
        <rule type="fa" cat="N">
         <lf start="6" span="1" word="Integer" lemma="Integer" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N/N" />
         <rule type="fa" cat="N">
          <lf start="7" span="1" word="&gt;" lemma="&gt;" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N/N" />
          <lf start="8" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
   <rule type="ba" cat="S[dcl]\NP">
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="9" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
     <rule type="fa" cat="S[adj]\NP">
      <lf start="10" span="1" word="greater_than" lemma="greater_than" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
      <rule type="fa" cat="PP">
       <lf start="11" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
       <rule type="lex" cat="NP">
        <lf start="12" span="1" word="0" lemma="0" pos="CD" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
    <rule type="conj" cat="(S[dcl]\NP)\(S[dcl]\NP)">
     <lf start="13" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="14" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
      <rule type="fa" cat="S[adj]\NP">
       <lf start="15" span="1" word="less_than" lemma="less_than" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
       <rule type="fa" cat="PP">
        <lf start="16" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
        <rule type="lex" cat="NP">
         <rule type="ba" cat="N">
          <lf start="17" span="1" word="9" lemma="9" pos="CD" chunk="I-NP" entity="O" cat="N" />
          <lf start="18" span="1" word="inclusive" lemma="inclusive" pos="JJ" chunk="I-NP" entity="O" cat="N\N" />
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="19" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
