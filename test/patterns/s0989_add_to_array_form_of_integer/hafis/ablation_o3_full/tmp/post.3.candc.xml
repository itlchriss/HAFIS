<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="ba" cat="NP">
    <rule type="fa" cat="NP[nb]">
     <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="1" span="1" word="size" lemma="size" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
    <rule type="fa" cat="NP\NP">
     <lf start="2" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
     <rule type="lex" cat="NP">
      <rule type="fa" cat="N">
       <lf start="3" span="1" word="List" lemma="List" pos="NNP" chunk="I-NP" entity="O" cat="N/N" />
       <rule type="fa" cat="N">
        <lf start="4" span="1" word="&lt;" lemma="&lt;" pos="NNP" chunk="I-NP" entity="O" cat="N/N" />
        <rule type="fa" cat="N">
         <lf start="5" span="1" word="Integer" lemma="Integer" pos="NNP" chunk="I-NP" entity="O" cat="N/N" />
         <rule type="fa" cat="N">
          <lf start="6" span="1" word="&gt;" lemma="&gt;" pos="NNP" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="7" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="8" span="1" word="equals" lemma="equal" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="ba" cat="NP">
     <rule type="fa" cat="NP[nb]">
      <lf start="9" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <rule type="fa" cat="N">
       <lf start="10" span="1" word="minimal" lemma="minimal" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="11" span="1" word="number" lemma="number" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="12" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
      <rule type="ba" cat="NP">
       <rule type="lex" cat="NP">
        <lf start="13" span="1" word="digits" lemma="digit" pos="NNS" chunk="I-NP" entity="O" cat="N" />
       </rule>
       <rule type="lex" cat="NP\NP">
        <rule type="fa" cat="S[pss]\NP">
         <lf start="14" span="1" word="needed" lemma="need" pos="VBN" chunk="I-VP" entity="O" cat="(S[pss]\NP)/(S[to]\NP)" />
         <rule type="fa" cat="S[to]\NP">
          <lf start="15" span="1" word="to" lemma="to" pos="TO" chunk="B-VP" entity="O" cat="(S[to]\NP)/(S[b]\NP)" />
          <rule type="fa" cat="S[b]\NP">
           <lf start="16" span="1" word="express" lemma="express" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/NP" />
           <rule type="ba" cat="NP">
            <rule type="fa" cat="NP[nb]">
             <lf start="17" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
             <lf start="18" span="1" word="sum" lemma="sum" pos="NN" chunk="I-NP" entity="O" cat="N" />
            </rule>
            <rule type="fa" cat="NP\NP">
             <lf start="19" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
             <rule type="ba" cat="NP">
              <rule type="fa" cat="NP[nb]">
               <lf start="20" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
               <lf start="21" span="1" word="integer" lemma="integer" pos="NN" chunk="I-NP" entity="O" cat="N" />
              </rule>
              <rule type="lex" cat="NP\NP">
               <rule type="ba" cat="S[pss]\NP">
                <lf start="22" span="1" word="represented" lemma="represent" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
                <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
                 <lf start="23" span="1" word="by" lemma="by" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
                 <rule type="ba" cat="NP">
                  <rule type="ba" cat="NP">
                   <rule type="lex" cat="NP">
                    <rule type="fa" cat="N">
                     <lf start="24" span="1" word="int" lemma="int" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
                     <lf start="25" span="1" word="[" lemma="[" pos="NNS" chunk="I-NP" entity="O" cat="N" />
                    </rule>
                   </rule>
                   <rule type="lex" cat="NP\NP">
                    <rule type="fa" cat="S[dcl]\NP">
                     <lf start="26" span="1" word="]" lemma="]" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
                     <rule type="fa" cat="NP[nb]">
                      <lf start="27" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
                      <lf start="28" span="1" word="chry_a" lemma="chry_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
                     </rule>
                    </rule>
                   </rule>
                  </rule>
                  <rule type="conj" cat="NP\NP">
                   <lf start="29" span="1" word="and" lemma="and" pos="CC" chunk="I-NP" entity="O" cat="conj" />
                   <rule type="ba" cat="NP">
                    <rule type="lex" cat="NP">
                     <lf start="30" span="1" word="int" lemma="int" pos="NN" chunk="I-NP" entity="O" cat="N" />
                    </rule>
                    <rule type="fa" cat="NP\NP">
                     <lf start="31" span="1" word="the" lemma="the" pos="DT" chunk="B-NP" entity="O" cat="(NP\NP)/N" />
                     <lf start="32" span="1" word="chry_b" lemma="chry_b" pos="NN" chunk="I-NP" entity="O" cat="N" />
                    </rule>
                   </rule>
                  </rule>
                 </rule>
                </rule>
               </rule>
              </rule>
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="33" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
