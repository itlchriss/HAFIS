<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="ba" cat="NP">
  <rule type="lex" cat="NP">
   <lf start="0" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
  </rule>
  <rule type="fa" cat="NP\NP">
   <lf start="1" span="1" word="for" lemma="for" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
   <rule type="ba" cat="NP">
    <rule type="lex" cat="NP">
     <lf start="2" span="1" word="int" lemma="int" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
    <rule type="fa" cat="NP\NP">
     <lf start="3" span="1" word=":" lemma=":" pos="IN" chunk="O" entity="O" cat="(NP\NP)/S[dcl]" />
     <rule type="rp" cat="S[dcl]">
      <rule type="ba" cat="S[dcl]">
       <rule type="lex" cat="NP">
        <lf start="4" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
       <rule type="ba" cat="S[dcl]\NP">
        <rule type="fa" cat="S[dcl]\NP">
         <lf start="5" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
         <rule type="fa" cat="S[adj]\NP">
          <lf start="6" span="1" word="greater_than_or_equal" lemma="greater_than_or_equal" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
          <rule type="fa" cat="PP">
           <lf start="7" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
           <rule type="lex" cat="NP">
            <lf start="8" span="1" word="0" lemma="0" pos="CD" chunk="I-NP" entity="O" cat="N" />
           </rule>
          </rule>
         </rule>
        </rule>
        <rule type="conj" cat="(S[dcl]\NP)\(S[dcl]\NP)">
         <lf start="9" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
         <rule type="fa" cat="S[dcl]\NP">
          <lf start="10" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
          <rule type="fa" cat="S[adj]\NP">
           <lf start="11" span="1" word="less_than_or_equal" lemma="less_than_or_equal" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
           <rule type="fa" cat="PP">
            <lf start="12" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
            <rule type="lex" cat="NP">
             <rule type="fa" cat="N">
              <lf start="13" span="1" word="30" lemma="30" pos="CD" chunk="I-NP" entity="I-DAT" cat="N/N" />
              <lf start="14" span="1" word="2" lemma="2" pos="CD" chunk="I-NP" entity="I-DAT" cat="N" />
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
      <lf start="15" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
     </rule>
    </rule>
   </rule>
  </rule>
 </rule>
</ccg>
</candc>
