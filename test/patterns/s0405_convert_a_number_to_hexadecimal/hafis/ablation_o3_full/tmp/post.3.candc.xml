<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="S[dcl]">
  <rule type="fa" cat="S[X]/S[X]">
   <lf start="0" span="1" word="For" lemma="for" pos="IN" chunk="I-PP" entity="O" cat="(S/S)/NP" />
   <rule type="fa" cat="NP[nb]">
    <lf start="1" span="1" word="every" lemma="every" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="2" span="1" word="int" lemma="int" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
  </rule>
  <rule type="rp" cat="S[dcl]">
   <rule type="fa" cat="S[dcl]">
    <rule type="tr" cat="S[X]/(S[X]/NP)">
     <rule type="fa" cat="NP[nb]">
      <lf start="3" span="1" word="the" lemma="the" pos="DT" chunk="B-NP" entity="O" cat="NP[nb]/N" />
      <rule type="fa" cat="N">
       <lf start="4" span="1" word="type_string_" lemma="type_string_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="5" span="1" word="strs_a" lemma="strs_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
    <rule type="fc" cat="S[dcl]/NP">
     <rule type="tr" cat="S[X]/(S[X]\NP)">
      <rule type="ba" cat="NP">
       <rule type="fa" cat="NP[nb]">
        <lf start="6" span="1" word="every" lemma="every" pos="DT" chunk="B-NP" entity="O" cat="NP[nb]/N" />
        <lf start="7" span="1" word="character" lemma="character" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
       <rule type="fa" cat="NP\NP">
        <lf start="8" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/(S[ng]\NP)" />
        <rule type="fa" cat="S[ng]\NP">
         <lf start="9" span="1" word="String" lemma="string" pos="VBG" chunk="I-NP" entity="O" cat="(S[ng]\NP)/NP" />
         <rule type="lex" cat="NP">
          <lf start="10" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
     <rule type="bx" cat="(S[dcl]\NP)/NP">
      <lf start="11" span="1" word="belongs" lemma="belong" pos="VBZ" chunk="I-VP" entity="O" cat="((S[dcl]\NP)/PP)/NP" />
      <rule type="tr" cat="(S[X]\NP)\((S[X]\NP)/PP)">
       <rule type="fa" cat="PP">
        <lf start="12" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
        <rule type="fa" cat="NP[nb]">
         <lf start="13" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <rule type="fa" cat="N">
          <lf start="14" span="1" word="set" lemma="set" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="15" span="1" word="stry_a" lemma="stry_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
   <lf start="16" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
</candc>
