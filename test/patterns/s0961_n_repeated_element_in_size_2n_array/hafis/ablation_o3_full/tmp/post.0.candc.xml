<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="int" lemma="int" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="equals" lemma="equal" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="ba" cat="NP">
     <rule type="fa" cat="NP[nb]">
      <lf start="4" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="5" span="1" word="element" lemma="element" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="6" span="1" word="that" lemma="that" pos="WDT" chunk="B-NP" entity="O" cat="(NP\NP)/(S[dcl]\NP)" />
      <rule type="fa" cat="S[dcl]\NP">
       <lf start="7" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pss]\NP)" />
       <rule type="ba" cat="S[pss]\NP">
        <rule type="ba" cat="S[pss]\NP">
         <lf start="8" span="1" word="repeated" lemma="repeat" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
         <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
          <lf start="9" span="1" word="n" lemma="n" pos="JJ" chunk="I-NP" entity="O" cat="((S\NP)\(S\NP))/((S\NP)\(S\NP))" />
          <lf start="10" span="1" word="times" lemma="time" pos="NNS" chunk="I-NP" entity="O" cat="(S\NP)\(S\NP)" />
         </rule>
        </rule>
        <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
         <lf start="11" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
         <rule type="lex" cat="NP">
          <lf start="12" span="1" word="param_nums_" lemma="param_nums_" pos="NN" chunk="I-NP" entity="I-LOC" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="13" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
