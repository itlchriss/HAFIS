<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="ba" cat="NP">
    <rule type="lex" cat="NP">
     <rule type="fa" cat="N">
      <lf start="0" span="1" word="Exactly" lemma="exactly" pos="RB" chunk="I-NP" entity="O" cat="N/N" />
      <rule type="fa" cat="N">
       <lf start="1" span="1" word="1" lemma="1" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="2" span="1" word="pair" lemma="pair" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
    <rule type="fa" cat="NP\NP">
     <lf start="3" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
     <rule type="lex" cat="NP">
      <rule type="fa" cat="N">
       <lf start="4" span="1" word="unique" lemma="unique" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="5" span="1" word="indices" lemma="index" pos="NNS" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
   <rule type="ba" cat="S[dcl]\NP">
    <rule type="ba" cat="S[dcl]\NP">
     <lf start="6" span="1" word="exists" lemma="exist" pos="VBZ" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="7" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
      <rule type="ba" cat="NP">
       <rule type="fa" cat="NP[nb]">
        <lf start="8" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <rule type="fa" cat="N">
         <lf start="9" span="1" word="type_integer_array_" lemma="type_integer_array_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="10" span="1" word="param_numbers_" lemma="param_numbers_" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
       <rule type="fa" cat="NP\NP">
        <rule type="fa" cat="(NP\NP)/(S[dcl]\NP)">
         <lf start="11" span="1" word="whose" lemma="whose" pos="WP$" chunk="B-NP" entity="O" cat="((NP\NP)/(S[dcl]\NP))/N" />
         <rule type="fa" cat="N">
          <lf start="12" span="1" word="corresponding" lemma="corresponding" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="13" span="1" word="values" lemma="value" pos="NNS" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
        <lf start="14" span="1" word="sum" lemma="sum" pos="NN" chunk="I-NP" entity="O" cat="S[dcl]\NP" />
       </rule>
      </rule>
     </rule>
    </rule>
    <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
     <lf start="15" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="16" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <rule type="fa" cat="N">
       <lf start="17" span="1" word="type_integer_" lemma="type_integer_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="18" span="1" word="param_target_" lemma="param_target_" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="19" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
