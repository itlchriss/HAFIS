<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="S[dcl]">
  <rule type="fa" cat="S[X]/S[X]">
   <lf start="0" span="1" word="Upon" lemma="upon" pos="IN" chunk="I-PP" entity="O" cat="(S/S)/NP" />
   <rule type="lex" cat="NP">
    <lf start="1" span="1" word="completion" lemma="completion" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
  </rule>
  <rule type="rp" cat="S[dcl]">
   <rule type="ba" cat="S[dcl]">
    <rule type="ba" cat="NP">
     <rule type="ba" cat="NP">
      <rule type="fa" cat="NP[nb]">
       <lf start="2" span="1" word="the" lemma="the" pos="DT" chunk="B-NP" entity="O" cat="NP[nb]/N" />
       <rule type="fa" cat="N">
        <lf start="3" span="1" word="2" lemma="2" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="4" span="1" word="elements" lemma="element" pos="NNS" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
      <rule type="fa" cat="NP\NP">
       <lf start="5" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
       <rule type="lex" cat="NP">
        <lf start="6" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="7" span="1" word="int" lemma="int" pos="NN" chunk="I-NP" entity="O" cat="(NP\NP)/(NP\NP)" />
      <lf start="8" span="1" word="array" lemma="array" pos="NN" chunk="I-NP" entity="O" cat="NP\NP" />
     </rule>
    </rule>
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="9" span="1" word="are" lemma="be" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
     <lf start="10" span="1" word="unique" lemma="unique" pos="JJ" chunk="I-ADJP" entity="O" cat="S[adj]\NP" />
    </rule>
   </rule>
   <lf start="11" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
</candc>
