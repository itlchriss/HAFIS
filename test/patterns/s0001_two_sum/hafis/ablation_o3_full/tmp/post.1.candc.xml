<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="S[dcl]">
  <rule type="fa" cat="S[X]/S[X]">
   <lf start="0" span="1" word="Upon" lemma="upon" pos="IN" chunk="I-PP" entity="O" cat="(S/S)/NP" />
   <rule type="lex" cat="NP">
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="completion" lemma="completion" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
  </rule>
  <rule type="rp" cat="S[dcl]">
   <rule type="ba" cat="S[dcl]">
    <rule type="fa" cat="NP[nb]">
     <rule type="ba" cat="NP[nb]/N">
      <rule type="lex" cat="NP">
       <rule type="fa" cat="N">
        <lf start="3" span="1" word="int" lemma="int" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="4" span="1" word="array" lemma="array" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
      <lf start="5" span="1" word="'s" lemma="'s" pos="POS" chunk="B-NP" entity="O" cat="(NP[nb]/N)\NP" />
     </rule>
     <lf start="6" span="1" word="length" lemma="length" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="7" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
     <rule type="fa" cat="S[adj]\NP">
      <lf start="8" span="1" word="equal" lemma="equal" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
      <rule type="fa" cat="PP">
       <lf start="9" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
       <rule type="lex" cat="NP">
        <lf start="10" span="1" word="2" lemma="2" pos="CD" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
   <lf start="11" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
</candc>
