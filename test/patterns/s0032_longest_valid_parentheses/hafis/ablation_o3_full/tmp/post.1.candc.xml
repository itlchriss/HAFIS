<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="int" lemma="int" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="denotes" lemma="denote" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="ba" cat="NP">
     <rule type="fa" cat="NP[nb]">
      <lf start="4" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <rule type="fa" cat="N">
       <lf start="5" span="1" word="maximum" lemma="maximum" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="6" span="1" word="length" lemma="length" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="7" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
      <rule type="ba" cat="NP">
       <rule type="fa" cat="NP[nb]">
        <lf start="8" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <rule type="fa" cat="N">
         <lf start="9" span="1" word="contiguous" lemma="contiguous" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="10" span="1" word="substring" lemma="substring" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
       <rule type="fa" cat="NP\NP">
        <lf start="11" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
        <rule type="ba" cat="NP">
         <lf start="12" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP" />
         <rule type="lex" cat="NP\NP">
          <rule type="fa" cat="S[ng]\NP">
           <lf start="13" span="1" word="String" lemma="string" pos="VBG" chunk="I-NP" entity="O" cat="(S[ng]\NP)/NP" />
           <rule type="ba" cat="NP">
            <rule type="fa" cat="NP[nb]">
             <lf start="14" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
             <rule type="fa" cat="N">
              <lf start="15" span="1" word="type_character_" lemma="type_character_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
              <lf start="16" span="1" word="chrs_a" lemma="chrs_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
             </rule>
            </rule>
            <rule type="fa" cat="NP\NP">
             <lf start="17" span="1" word="that" lemma="that" pos="WDT" chunk="B-NP" entity="O" cat="(NP\NP)/(S[dcl]\NP)" />
             <rule type="fa" cat="S[dcl]\NP">
              <lf start="18" span="1" word="forms" lemma="form" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
              <rule type="ba" cat="NP">
               <rule type="fa" cat="NP[nb]">
                <lf start="19" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
                <rule type="fa" cat="N">
                 <rule type="fa" cat="N/N">
                  <lf start="20" span="1" word="well" lemma="well" pos="RB" chunk="I-NP" entity="O" cat="(N/N)/(N/N)" />
                  <lf start="21" span="1" word="formed" lemma="form" pos="VBN" chunk="I-NP" entity="O" cat="N/N" />
                 </rule>
                 <lf start="22" span="1" word="sequence" lemma="sequence" pos="NN" chunk="I-NP" entity="O" cat="N" />
                </rule>
               </rule>
               <rule type="fa" cat="NP\NP">
                <lf start="23" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
                <rule type="ba" cat="NP">
                 <rule type="lex" cat="NP">
                  <lf start="24" span="1" word="opening" lemma="opening" pos="NN" chunk="I-NP" entity="O" cat="N" />
                 </rule>
                 <rule type="conj" cat="NP\NP">
                  <lf start="25" span="1" word="and" lemma="and" pos="CC" chunk="I-NP" entity="O" cat="conj" />
                  <rule type="lex" cat="NP">
                   <rule type="fa" cat="N">
                    <lf start="26" span="1" word="closing" lemma="closing" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
                    <lf start="27" span="1" word="brackets" lemma="bracket" pos="NNS" chunk="I-NP" entity="O" cat="N" />
                   </rule>
                  </rule>
                 </rule>
                </rule>
               </rule>
              </rule>
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="28" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
