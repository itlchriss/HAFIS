<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[pt]">
  <rule type="ba" cat="S[pt]">
   <rule type="ba" cat="NP">
    <rule type="fa" cat="NP[nb]">
     <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <rule type="fa" cat="N">
      <lf start="1" span="1" word="int" lemma="int" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="2" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
    <rule type="lex" cat="NP\NP">
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="3" span="1" word="equals" lemma="equal" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
      <rule type="ba" cat="NP">
       <rule type="lex" cat="NP">
        <lf start="4" span="1" word="0" lemma="0" pos="CD" chunk="I-NP" entity="O" cat="N" />
       </rule>
       <rule type="fa" cat="NP\NP">
        <lf start="5" span="1" word="when" lemma="when" pos="WRB" chunk="I-ADVP" entity="O" cat="(NP\NP)/S[dcl]" />
        <rule type="fa" cat="S[dcl]">
         <rule type="tr" cat="S[X]/(S[X]/NP)">
          <rule type="fa" cat="NP[nb]">
           <lf start="6" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
           <lf start="7" span="1" word="String" lemma="string" pos="VBG" chunk="I-NP" entity="O" cat="N" />
          </rule>
         </rule>
         <rule type="fc" cat="S[dcl]/NP">
          <rule type="tr" cat="S[X]/(S[X]\NP)">
           <rule type="fa" cat="NP[nb]">
            <lf start="8" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
            <rule type="fa" cat="N">
             <lf start="9" span="1" word="type_character_" lemma="type_character_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
             <lf start="10" span="1" word="chrs_a" lemma="chrs_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
            </rule>
           </rule>
          </rule>
          <rule type="bx" cat="(S[dcl]\NP)/NP">
           <lf start="11" span="1" word="contains" lemma="contain" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
           <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
            <lf start="12" span="1" word="no" lemma="no" pos="DT" chunk="I-NP" entity="O" cat="((S\NP)\(S\NP))/((S\NP)\(S\NP))" />
            <lf start="13" span="1" word="well" lemma="well" pos="RB" chunk="I-NP" entity="O" cat="(S\NP)\(S\NP)" />
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
   <rule type="fa" cat="S[pt]\NP">
    <lf start="14" span="1" word="formed" lemma="form" pos="VBN" chunk="I-NP" entity="O" cat="(S[pt]\NP)/NP" />
    <rule type="lex" cat="NP">
     <rule type="fa" cat="N">
      <lf start="15" span="1" word="bracket" lemma="bracket" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="16" span="1" word="substring" lemma="substring" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="17" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
