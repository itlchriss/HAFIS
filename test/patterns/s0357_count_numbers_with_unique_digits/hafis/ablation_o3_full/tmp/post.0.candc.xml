<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <rule type="fa" cat="N">
     <lf start="0" span="1" word="int" lemma="int" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="1" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="ba" cat="NP">
     <rule type="fa" cat="NP[nb]">
      <lf start="3" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="4" span="1" word="count" lemma="count" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="5" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
      <rule type="ba" cat="NP">
       <rule type="lex" cat="NP">
        <lf start="6" span="1" word="integers" lemma="integer" pos="NNS" chunk="I-NP" entity="O" cat="N" />
       </rule>
       <rule type="lex" cat="NP\NP">
        <rule type="fa" cat="S[dcl]\NP">
         <lf start="7" span="1" word="x" lemma="x" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/S[em]" />
         <rule type="fa" cat="S[em]">
          <lf start="8" span="1" word="that" lemma="that" pos="IN" chunk="I-SBAR" entity="O" cat="S[em]/S[dcl]" />
          <rule type="ba" cat="S[dcl]">
           <rule type="ba" cat="S[dcl]">
            <rule type="ba" cat="NP">
             <rule type="lex" cat="NP">
              <lf start="9" span="1" word="0" lemma="0" pos="CD" chunk="I-NP" entity="O" cat="N" />
             </rule>
             <rule type="lex" cat="NP\NP">
              <rule type="fa" cat="S[dcl]\NP">
               <lf start="10" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
               <rule type="fa" cat="S[adj]\NP">
                <lf start="11" span="1" word="less_than_or_equal" lemma="less_than_or_equal" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/(S[to]\NP)" />
                <rule type="fa" cat="S[to]\NP">
                 <lf start="12" span="1" word="to" lemma="to" pos="TO" chunk="I-VP" entity="O" cat="(S[to]\NP)/(S[b]\NP)" />
                 <rule type="ba" cat="S[b]\NP">
                  <lf start="13" span="1" word="x" lemma="x" pos="VB" chunk="I-VP" entity="O" cat="S[b]\NP" />
                  <rule type="conj" cat="(S[b]\NP)\(S[b]\NP)">
                   <lf start="14" span="1" word="and" lemma="and" pos="CC" chunk="I-VP" entity="O" cat="conj" />
                   <lf start="15" span="1" word="x" lemma="x" pos="VB" chunk="I-VP" entity="O" cat="S[b]\NP" />
                  </rule>
                 </rule>
                </rule>
               </rule>
              </rule>
             </rule>
            </rule>
            <rule type="fa" cat="S[dcl]\NP">
             <lf start="16" span="1" word="is" lemma="be" pos="VBZ" chunk="B-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
             <rule type="fa" cat="S[adj]\NP">
              <lf start="17" span="1" word="less_than" lemma="less_than" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
              <rule type="fa" cat="PP">
               <lf start="18" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
               <rule type="ba" cat="NP">
                <rule type="lex" cat="NP">
                 <lf start="19" span="1" word="10" lemma="10" pos="CD" chunk="I-NP" entity="O" cat="N" />
                </rule>
                <rule type="lex" cat="NP\NP">
                 <rule type="ba" cat="S[pss]\NP">
                  <lf start="20" span="1" word="raised" lemma="raise" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
                  <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
                   <lf start="21" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
                   <rule type="ba" cat="NP">
                    <rule type="fa" cat="NP[nb]">
                     <lf start="22" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
                     <lf start="23" span="1" word="power" lemma="power" pos="NN" chunk="I-NP" entity="O" cat="N" />
                    </rule>
                    <rule type="fa" cat="NP\NP">
                     <lf start="24" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
                     <rule type="lex" cat="NP">
                      <rule type="fa" cat="N">
                       <lf start="25" span="1" word="int" lemma="int" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
                       <lf start="26" span="1" word="stry_a" lemma="stry_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
                      </rule>
                     </rule>
                    </rule>
                   </rule>
                  </rule>
                 </rule>
                </rule>
               </rule>
              </rule>
             </rule>
            </rule>
           </rule>
           <rule type="conj" cat="S[dcl]\S[dcl]">
            <lf start="27" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
            <rule type="ba" cat="S[dcl]">
             <rule type="ba" cat="NP">
              <rule type="fa" cat="NP[nb]">
               <lf start="28" span="1" word="every" lemma="every" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
               <rule type="fa" cat="N">
                <lf start="29" span="1" word="decimal" lemma="decimal" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
                <lf start="30" span="1" word="digit" lemma="digit" pos="NN" chunk="I-NP" entity="O" cat="N" />
               </rule>
              </rule>
              <rule type="fa" cat="NP\NP">
               <lf start="31" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
               <rule type="lex" cat="NP">
                <lf start="32" span="1" word="x" lemma="x" pos="NN" chunk="I-NP" entity="O" cat="N" />
               </rule>
              </rule>
             </rule>
             <rule type="ba" cat="S[dcl]\NP">
              <lf start="33" span="1" word="appears" lemma="appear" pos="VBZ" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
              <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
               <lf start="34" span="1" word="at" lemma="at" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
               <rule type="ba" cat="NP">
                <rule type="lex" cat="NP">
                 <lf start="35" span="1" word="most" lemma="most" pos="JJS" chunk="I-ADVP" entity="O" cat="N" />
                </rule>
                <lf start="36" span="1" word="once" lemma="once" pos="RB" chunk="I-ADVP" entity="O" cat="NP\NP" />
               </rule>
              </rule>
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="37" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
