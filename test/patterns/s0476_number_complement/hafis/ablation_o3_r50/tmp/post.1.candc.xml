<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="S[X]/S[X]">
  <lf start="0" span="1" word="For" lemma="for" pos="IN" chunk="I-PP" entity="O" cat="(S/S)/NP" />
  <rule type="ba" cat="NP">
   <rule type="fa" cat="NP[nb]">
    <lf start="1" span="1" word="every" lemma="every" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="2" span="1" word="bit" lemma="bit" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="3" span="1" word="position" lemma="position" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="lex" cat="NP\NP">
    <rule type="ba" cat="S[pss]\NP">
     <rule type="fa" cat="S[pss]\NP">
      <lf start="4" span="1" word="set" lemma="set" pos="VBN" chunk="I-VP" entity="O" cat="(S[pss]\NP)/PP" />
      <rule type="fa" cat="PP">
       <lf start="5" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
       <rule type="lex" cat="NP">
        <lf start="6" span="1" word="1" lemma="1" pos="CD" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="7" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
      <rule type="ba" cat="NP">
       <rule type="fa" cat="NP[nb]">
        <lf start="8" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="9" span="1" word="type_integer_" lemma="type_integer_" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
       <rule type="lex" cat="NP\NP">
        <rule type="fa" cat="S[dcl]\NP">
         <lf start="10" span="1" word="param_num_" lemma="param_num_" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
         <rule type="ba" cat="NP">
          <rule type="fa" cat="NP[nb]">
           <lf start="11" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
           <rule type="fa" cat="N">
            <lf start="12" span="1" word="corresponding" lemma="corresponding" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
            <rule type="fa" cat="N">
             <lf start="13" span="1" word="bit" lemma="bit" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
             <lf start="14" span="1" word="position" lemma="position" pos="NN" chunk="I-NP" entity="O" cat="N" />
            </rule>
           </rule>
          </rule>
          <rule type="lex" cat="NP\NP">
           <rule type="rp" cat="S[dcl]\NP">
            <rule type="fa" cat="S[dcl]\NP">
             <lf start="15" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pss]\NP)" />
             <rule type="ba" cat="S[pss]\NP">
              <rule type="fa" cat="S[pss]\NP">
               <lf start="16" span="1" word="set" lemma="set" pos="VBN" chunk="I-VP" entity="O" cat="(S[pss]\NP)/PP" />
               <rule type="fa" cat="PP">
                <lf start="17" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
                <rule type="lex" cat="NP">
                 <lf start="18" span="1" word="0" lemma="0" pos="CD" chunk="I-NP" entity="O" cat="N" />
                </rule>
               </rule>
              </rule>
              <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
               <lf start="19" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
               <rule type="fa" cat="NP[nb]">
                <lf start="20" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
                <rule type="fa" cat="N">
                 <lf start="21" span="1" word="integer" lemma="integer" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
                 <lf start="22" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
                </rule>
               </rule>
              </rule>
             </rule>
            </rule>
            <lf start="23" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
 </rule>
</ccg>
</candc>
