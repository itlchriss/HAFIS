<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <rule type="fa" cat="N">
     <lf start="0" span="1" word="boolean" lemma="boolean" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="1" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="ba" cat="S[dcl]\NP">
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="2" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
     <rule type="fa" cat="S[adj]\NP">
      <lf start="3" span="1" word="equal" lemma="equal" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
      <rule type="fa" cat="PP">
       <lf start="4" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
       <rule type="fa" cat="NP[nb]">
        <lf start="5" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <rule type="fa" cat="N">
         <lf start="6" span="1" word="true" lemma="true" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="7" span="1" word="literal" lemma="literal" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
    <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
     <lf start="8" span="1" word="if" lemma="if" pos="IN" chunk="I-SBAR" entity="O" cat="((S\NP)\(S\NP))/S[dcl]" />
     <rule type="ba" cat="S[dcl]">
      <lf start="9" span="1" word="there" lemma="there" pos="EX" chunk="I-NP" entity="O" cat="NP[thr]" />
      <rule type="fa" cat="S[dcl]\NP[thr]">
       <lf start="10" span="1" word="exists" lemma="exist" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP[thr])/NP" />
       <rule type="ba" cat="NP">
        <rule type="fa" cat="NP[nb]">
         <lf start="11" span="1" word="an" lemma="an" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <rule type="fa" cat="N">
          <lf start="12" span="1" word="integer" lemma="integer" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="13" span="1" word="x" lemma="x" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
        <rule type="lex" cat="NP\NP">
         <rule type="fa" cat="S[adj]\NP">
          <lf start="14" span="1" word="such" lemma="such" pos="JJ" chunk="I-PP" entity="O" cat="(S[adj]\NP)/S[em]" />
          <rule type="fa" cat="S[em]">
           <lf start="15" span="1" word="that" lemma="that" pos="IN" chunk="I-NP" entity="O" cat="S[em]/S[dcl]" />
           <rule type="ba" cat="S[dcl]">
            <rule type="ba" cat="NP">
             <rule type="lex" cat="NP">
              <lf start="16" span="1" word="3" lemma="3" pos="CD" chunk="I-NP" entity="O" cat="N" />
             </rule>
             <rule type="lex" cat="NP\NP">
              <rule type="ba" cat="S[pss]\NP">
               <lf start="17" span="1" word="raised" lemma="raise" pos="VBD" chunk="I-VP" entity="O" cat="S[pss]\NP" />
               <rule type="lex" cat="(S[X]\NP)\(S[X]\NP)">
                <rule type="fa" cat="S[to]\NP">
                 <lf start="18" span="1" word="to" lemma="to" pos="TO" chunk="I-VP" entity="O" cat="(S[to]\NP)/(S[b]\NP)" />
                 <rule type="fa" cat="S[b]\NP">
                  <lf start="19" span="1" word="x" lemma="x" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/NP" />
                  <rule type="lex" cat="NP">
                   <lf start="20" span="1" word="equals" lemma="equal" pos="NNS" chunk="B-VP" entity="O" cat="N" />
                  </rule>
                 </rule>
                </rule>
               </rule>
              </rule>
             </rule>
            </rule>
            <rule type="fa" cat="S[dcl]\NP">
             <lf start="21" span="1" word="int" lemma="int" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
             <rule type="fa" cat="NP[nb]">
              <lf start="22" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
              <lf start="23" span="1" word="chry_a" lemma="chry_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="24" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
