<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="ba" cat="NP">
    <rule type="lex" cat="NP">
     <lf start="0" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
    <lf start="1" span="1" word="int" lemma="int" pos="NN" chunk="I-NP" entity="O" cat="NP\NP" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pss]\NP)" />
    <rule type="ba" cat="S[pss]\NP">
     <lf start="3" span="1" word="guaranteed" lemma="guarantee" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
     <rule type="lex" cat="(S[X]\NP)\(S[X]\NP)">
      <rule type="fa" cat="S[to]\NP">
       <lf start="4" span="1" word="to" lemma="to" pos="TO" chunk="I-VP" entity="O" cat="(S[to]\NP)/(S[b]\NP)" />
       <rule type="fa" cat="S[b]\NP">
        <lf start="5" span="1" word="be" lemma="be" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/PP" />
        <rule type="fa" cat="PP">
         <lf start="6" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
         <rule type="fa" cat="NP[nb]">
          <lf start="7" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
          <rule type="fa" cat="N">
           <lf start="8" span="1" word="32" lemma="32" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
           <rule type="fa" cat="N">
            <lf start="9" span="1" word="bit" lemma="bit" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
            <rule type="fa" cat="N">
             <lf start="10" span="1" word="signed" lemma="signed" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
             <rule type="fa" cat="N">
              <lf start="11" span="1" word="integer" lemma="integer" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
              <lf start="12" span="1" word="range" lemma="range" pos="NN" chunk="I-NP" entity="O" cat="N" />
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="13" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
