<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="S[X]/S[X]">
  <lf start="0" span="1" word="After" lemma="after" pos="IN" chunk="I-PP" entity="O" cat="(S/S)/S[dcl]" />
  <rule type="rp" cat="S[dcl]">
   <rule type="ba" cat="S[dcl]">
    <rule type="ba" cat="NP">
     <rule type="lex" cat="NP">
      <rule type="fa" cat="N">
       <lf start="1" span="1" word="successful" lemma="successful" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
       <rule type="fa" cat="N">
        <lf start="2" span="1" word="execution" lemma="execution" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="3" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="4" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
      <rule type="lex" cat="NP">
       <rule type="fa" cat="N">
        <lf start="5" span="1" word="data" lemma="datum" pos="NNS" chunk="I-NP" entity="O" cat="N/N" />
        <rule type="fa" cat="N">
         <lf start="6" span="1" word="type" lemma="type" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="7" span="1" word="int" lemma="int" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="8" span="1" word="equals" lemma="equal" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
     <rule type="ba" cat="NP">
      <rule type="ba" cat="NP">
       <rule type="fa" cat="NP[nb]">
        <lf start="9" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <rule type="fa" cat="N">
         <lf start="10" span="1" word="total" lemma="total" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="11" span="1" word="number" lemma="number" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
       <rule type="fa" cat="NP\NP">
        <lf start="12" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
        <rule type="lex" cat="NP">
         <rule type="fa" cat="N">
          <lf start="13" span="1" word="continuous" lemma="continuous" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="14" span="1" word="subarrays" lemma="subarray" pos="NNS" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
      <rule type="fa" cat="NP\NP">
       <lf start="15" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
       <rule type="ba" cat="NP">
        <rule type="lex" cat="NP">
         <rule type="fa" cat="N">
          <lf start="16" span="1" word="param_the_" lemma="param_the_" pos="NN" chunk="I-NP" entity="I-LOC" cat="N/N" />
          <lf start="17" span="1" word="chry_a" lemma="chry_a" pos="NN" chunk="I-NP" entity="I-LOC" cat="N" />
         </rule>
        </rule>
        <rule type="fa" cat="NP\NP">
         <rule type="fa" cat="(NP\NP)/(S[dcl]\NP)">
          <lf start="18" span="1" word="whose" lemma="whose" pos="WP$" chunk="B-NP" entity="O" cat="((NP\NP)/(S[dcl]\NP))/N" />
          <rule type="fa" cat="N">
           <lf start="19" span="1" word="element" lemma="element" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
           <lf start="20" span="1" word="sum" lemma="sum" pos="NN" chunk="I-NP" entity="O" cat="N" />
          </rule>
         </rule>
         <rule type="fa" cat="S[dcl]\NP">
          <lf start="21" span="1" word="equals" lemma="equal" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
          <rule type="lex" cat="NP">
           <rule type="fa" cat="N">
            <lf start="22" span="1" word="param_the_" lemma="param_the_" pos="NN" chunk="I-NP" entity="I-ORG" cat="N/N" />
            <lf start="23" span="1" word="chry_b" lemma="chry_b" pos="NN" chunk="I-NP" entity="I-ORG" cat="N" />
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
   <lf start="24" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
</candc>
