<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="S[dcl]">
  <rule type="fa" cat="S[X]/S[X]">
   <lf start="0" span="1" word="If" lemma="if" pos="IN" chunk="I-SBAR" entity="O" cat="(S/S)/S[dcl]" />
   <rule type="fa" cat="S[dcl]">
    <rule type="tr" cat="S[X]/(S[X]/NP)">
     <rule type="lex" cat="NP">
      <lf start="1" span="1" word="int" lemma="int" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
    <rule type="fc" cat="S[dcl]/NP">
     <rule type="tr" cat="S[X]/(S[X]\NP)">
      <rule type="fa" cat="NP[nb]">
       <lf start="2" span="1" word="the" lemma="the" pos="DT" chunk="B-NP" entity="O" cat="NP[nb]/N" />
       <rule type="fa" cat="N">
        <lf start="3" span="1" word="type_string_" lemma="type_string_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="4" span="1" word="strs_a" lemma="strs_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
     <lf start="5" span="1" word="has" lemma="have" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    </rule>
   </rule>
  </rule>
  <rule type="rp" cat="S[dcl]">
   <rule type="ba" cat="S[dcl]">
    <rule type="lex" cat="NP">
     <rule type="fa" cat="N">
      <rule type="fa" cat="N/N">
       <rule type="ba" cat="(N/N)/(N/N)">
        <lf start="6" span="1" word="more" lemma="more" pos="JJR" chunk="I-NP" entity="O" cat="S[adj]\NP" />
        <lf start="7" span="1" word="than" lemma="than" pos="IN" chunk="I-NP" entity="O" cat="((N/N)/(N/N))\(S[adj]\NP)" />
       </rule>
       <lf start="8" span="1" word="1" lemma="1" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
      </rule>
      <rule type="fa" cat="N">
       <lf start="9" span="1" word="decimal" lemma="decimal" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
       <rule type="fa" cat="N">
        <lf start="10" span="1" word="digit" lemma="digit" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
        <rule type="fa" cat="N">
         <rule type="fa" cat="N/N">
          <lf start="11" span="1" word="then" lemma="then" pos="RB" chunk="I-NP" entity="O" cat="(N/N)/(N/N)" />
          <lf start="12" span="1" word="int" lemma="int" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
         </rule>
         <lf start="13" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="14" span="1" word="equals" lemma="equal" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
     <rule type="ba" cat="NP">
      <rule type="fa" cat="NP[nb]">
       <lf start="15" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="16" span="1" word="single_digit" lemma="single_digit" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
      <rule type="lex" cat="NP\NP">
       <rule type="ba" cat="S[pss]\NP">
        <lf start="17" span="1" word="produced" lemma="produce" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
        <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
         <lf start="18" span="1" word="by" lemma="by" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/(S[ng]\NP)" />
         <rule type="fa" cat="S[ng]\NP">
          <lf start="19" span="1" word="repeatedly" lemma="repeatedly" pos="RB" chunk="I-ADVP" entity="O" cat="(S\NP)/(S\NP)" />
          <rule type="ba" cat="S[ng]\NP">
           <rule type="fa" cat="S[ng]\NP">
            <lf start="20" span="1" word="summing" lemma="sum" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
            <rule type="ba" cat="NP">
             <rule type="fa" cat="NP[nb]">
              <lf start="21" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
              <rule type="fa" cat="N">
               <lf start="22" span="1" word="decimal" lemma="decimal" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
               <lf start="23" span="1" word="digits" lemma="digit" pos="NNS" chunk="I-NP" entity="O" cat="N" />
              </rule>
             </rule>
             <rule type="fa" cat="NP\NP">
              <lf start="24" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
              <rule type="fa" cat="NP[nb]">
               <lf start="25" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
               <rule type="fa" cat="N">
                <lf start="26" span="1" word="type_string_" lemma="type_string_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
                <lf start="27" span="1" word="strs_b" lemma="strs_b" pos="NN" chunk="I-NP" entity="O" cat="N" />
               </rule>
              </rule>
             </rule>
            </rule>
           </rule>
           <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
            <lf start="28" span="1" word="until" lemma="until" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/S[dcl]" />
            <rule type="ba" cat="S[dcl]">
             <rule type="lex" cat="NP">
              <rule type="fa" cat="N">
               <rule type="fa" cat="N/N">
                <lf start="29" span="1" word="only" lemma="only" pos="RB" chunk="I-NP" entity="O" cat="(N/N)/(N/N)" />
                <lf start="30" span="1" word="1" lemma="1" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
               </rule>
               <lf start="31" span="1" word="digit" lemma="digit" pos="NN" chunk="I-NP" entity="O" cat="N" />
              </rule>
             </rule>
             <lf start="32" span="1" word="remains" lemma="remain" pos="VBZ" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
   <lf start="33" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
</candc>
