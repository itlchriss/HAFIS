<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="ba" cat="NP">
    <rule type="fa" cat="NP[nb]">
     <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="1" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
    <rule type="fa" cat="NP\NP">
     <lf start="2" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
     <rule type="lex" cat="NP">
      <rule type="fa" cat="N">
       <lf start="3" span="1" word="type" lemma="type" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="4" span="1" word="int" lemma="int" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
   <rule type="ba" cat="S[dcl]\NP">
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="5" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
     <rule type="fa" cat="S[adj]\NP">
      <lf start="6" span="1" word="never" lemma="never" pos="RB" chunk="I-ADVP" entity="O" cat="(S[adj]\NP)/(S[adj]\NP)" />
      <lf start="7" span="1" word="negative" lemma="negative" pos="JJ" chunk="I-ADJP" entity="O" cat="S[adj]\NP" />
     </rule>
    </rule>
    <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
     <lf start="8" span="1" word="for" lemma="for" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
     <rule type="ba" cat="NP">
      <rule type="fa" cat="NP[nb]">
       <lf start="9" span="1" word="any" lemma="any" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <rule type="fa" cat="N">
        <lf start="10" span="1" word="valid" lemma="valid" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="11" span="1" word="stry_a" lemma="stry_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
      <rule type="fa" cat="NP\NP">
       <lf start="12" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
       <rule type="lex" cat="NP">
        <rule type="fa" cat="N">
         <lf start="13" span="1" word="type" lemma="type" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
         <rule type="fa" cat="N">
          <lf start="14" span="1" word="int" lemma="int" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
          <rule type="fa" cat="N">
           <lf start="15" span="1" word="[" lemma="[" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
           <lf start="16" span="1" word="]" lemma="]" pos="NN" chunk="I-NP" entity="O" cat="N" />
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="17" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
