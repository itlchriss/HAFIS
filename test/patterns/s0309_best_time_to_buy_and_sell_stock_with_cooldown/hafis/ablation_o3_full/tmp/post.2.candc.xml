<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="ba" cat="NP">
    <rule type="fa" cat="NP[nb]">
     <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="1" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
    <rule type="fa" cat="NP\NP">
     <lf start="2" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
     <rule type="lex" cat="NP">
      <rule type="fa" cat="N">
       <lf start="3" span="1" word="type" lemma="type" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="4" span="1" word="int" lemma="int" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="5" span="1" word="equals" lemma="equal" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="ba" cat="NP">
     <rule type="ba" cat="NP">
      <rule type="fa" cat="NP[nb]">
       <lf start="6" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <rule type="fa" cat="N">
        <lf start="7" span="1" word="maximum" lemma="maximum" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="8" span="1" word="profit" lemma="profit" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
      <rule type="fa" cat="NP\NP">
       <lf start="9" span="1" word="that" lemma="that" pos="WDT" chunk="B-NP" entity="O" cat="(NP\NP)/(S[dcl]\NP)" />
       <rule type="fa" cat="S[dcl]\NP">
        <lf start="10" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pss]\NP)" />
        <rule type="fa" cat="S[pss]\NP">
         <lf start="11" span="1" word="obtained" lemma="obtain" pos="VBN" chunk="I-VP" entity="O" cat="(S[pss]\NP)/PP" />
         <rule type="fa" cat="PP">
          <lf start="12" span="1" word="from" lemma="from" pos="IN" chunk="I-PP" entity="O" cat="PP/(S[ng]\NP)" />
          <rule type="fa" cat="S[ng]\NP">
           <lf start="13" span="1" word="performing" lemma="perform" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
           <rule type="ba" cat="NP">
            <rule type="fa" cat="NP[nb]">
             <lf start="14" span="1" word="any" lemma="any" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
             <lf start="15" span="1" word="number" lemma="number" pos="NN" chunk="I-NP" entity="O" cat="N" />
            </rule>
            <rule type="fa" cat="NP\NP">
             <lf start="16" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
             <rule type="ba" cat="NP">
              <rule type="lex" cat="NP">
               <lf start="17" span="1" word="buy" lemma="buy" pos="NN" chunk="I-NP" entity="O" cat="N" />
              </rule>
              <rule type="conj" cat="NP\NP">
               <lf start="18" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
               <rule type="lex" cat="NP">
                <rule type="fa" cat="N">
                 <lf start="19" span="1" word="sell" lemma="sell" pos="VB" chunk="I-VP" entity="O" cat="N/N" />
                 <lf start="20" span="1" word="transactions" lemma="transaction" pos="NNS" chunk="I-NP" entity="O" cat="N" />
                </rule>
               </rule>
              </rule>
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="21" span="1" word="on" lemma="on" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
      <rule type="ba" cat="NP">
       <rule type="ba" cat="NP">
        <rule type="fa" cat="NP[nb]">
         <lf start="22" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <rule type="fa" cat="N">
          <lf start="23" span="1" word="daily" lemma="daily" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="24" span="1" word="values" lemma="value" pos="NNS" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
        <rule type="fa" cat="NP\NP">
         <lf start="25" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
         <rule type="lex" cat="NP">
          <lf start="26" span="1" word="stry_a" lemma="stry_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
       <rule type="fa" cat="NP\NP">
        <lf start="27" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
        <rule type="ba" cat="NP">
         <rule type="lex" cat="NP">
          <rule type="fa" cat="N">
           <lf start="28" span="1" word="type" lemma="type" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
           <rule type="fa" cat="N">
            <lf start="29" span="1" word="int" lemma="int" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
            <rule type="fa" cat="N">
             <lf start="30" span="1" word="[" lemma="[" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
             <rule type="fa" cat="N">
              <lf start="31" span="1" word="]" lemma="]" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
              <lf start="32" span="1" word="subject" lemma="subject" pos="NN" chunk="I-NP" entity="O" cat="N" />
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
         <rule type="fa" cat="NP\NP">
          <lf start="33" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
          <rule type="fa" cat="NP[nb]">
           <lf start="34" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
           <rule type="fa" cat="N">
            <lf start="35" span="1" word="rule" lemma="rule" pos="NN" chunk="I-NP" entity="O" cat="N/S[em]" />
            <rule type="fa" cat="S[em]">
             <lf start="36" span="1" word="that" lemma="that" pos="IN" chunk="I-SBAR" entity="O" cat="S[em]/S[dcl]" />
             <rule type="ba" cat="S[dcl]">
              <rule type="ba" cat="S[dcl]">
               <rule type="fa" cat="NP[nb]">
                <lf start="37" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
                <rule type="fa" cat="N">
                 <lf start="38" span="1" word="buy" lemma="buy" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
                 <lf start="39" span="1" word="action" lemma="action" pos="NN" chunk="I-NP" entity="O" cat="N" />
                </rule>
               </rule>
               <rule type="fa" cat="S[dcl]\NP">
                <rule type="bx" cat="(S[dcl]\NP)/(S[b]\NP)">
                 <lf start="40" span="1" word="can" lemma="can" pos="MD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[b]\NP)" />
                 <lf start="41" span="1" word="not" lemma="not" pos="RB" chunk="I-VP" entity="O" cat="(S\NP)\(S\NP)" />
                </rule>
                <rule type="ba" cat="S[b]\NP">
                 <rule type="ba" cat="S[b]\NP">
                  <lf start="42" span="1" word="occur" lemma="occur" pos="VB" chunk="I-VP" entity="O" cat="S[b]\NP" />
                  <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
                   <lf start="43" span="1" word="on" lemma="on" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
                   <rule type="fa" cat="NP[nb]">
                    <lf start="44" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
                    <lf start="45" span="1" word="day" lemma="day" pos="NN" chunk="I-NP" entity="O" cat="N" />
                   </rule>
                  </rule>
                 </rule>
                 <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
                  <lf start="46" span="1" word="after" lemma="after" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
                  <rule type="fa" cat="NP[nb]">
                   <lf start="47" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
                   <rule type="fa" cat="N">
                    <lf start="48" span="1" word="sell" lemma="sell" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
                    <lf start="49" span="1" word="action" lemma="action" pos="NN" chunk="I-NP" entity="O" cat="N" />
                   </rule>
                  </rule>
                 </rule>
                </rule>
               </rule>
              </rule>
              <rule type="conj" cat="S[dcl]\S[dcl]">
               <lf start="50" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
               <rule type="ba" cat="S[dcl]">
                <rule type="lex" cat="NP">
                 <rule type="fa" cat="N">
                  <rule type="fa" cat="N/N">
                   <lf start="51" span="1" word="only" lemma="only" pos="RB" chunk="I-NP" entity="O" cat="(N/N)/(N/N)" />
                   <lf start="52" span="1" word="1" lemma="1" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
                  </rule>
                  <lf start="53" span="1" word="share" lemma="share" pos="NN" chunk="I-NP" entity="O" cat="N" />
                 </rule>
                </rule>
                <rule type="fa" cat="S[dcl]\NP">
                 <lf start="54" span="1" word="may" lemma="may" pos="MD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[b]\NP)" />
                 <rule type="fa" cat="S[b]\NP">
                  <lf start="55" span="1" word="be" lemma="be" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/(S[pss]\NP)" />
                  <rule type="ba" cat="S[pss]\NP">
                   <lf start="56" span="1" word="held" lemma="hold" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
                   <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
                    <lf start="57" span="1" word="at" lemma="at" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
                    <rule type="fa" cat="NP[nb]">
                     <lf start="58" span="1" word="any" lemma="any" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
                     <rule type="fa" cat="N">
                      <lf start="59" span="1" word="given" lemma="give" pos="VBN" chunk="I-NP" entity="O" cat="N/N" />
                      <lf start="60" span="1" word="time" lemma="time" pos="NN" chunk="I-NP" entity="O" cat="N" />
                     </rule>
                    </rule>
                   </rule>
                  </rule>
                 </rule>
                </rule>
               </rule>
              </rule>
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="61" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
