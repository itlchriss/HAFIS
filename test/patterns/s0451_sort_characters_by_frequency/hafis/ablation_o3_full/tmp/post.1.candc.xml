<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="ba" cat="S[dcl]">
  <rule type="ba" cat="NP">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="lex" cat="NP\NP">
    <lf start="1" span="1" word="String" lemma="string" pos="VBG" chunk="I-VP" entity="O" cat="S[ng]\NP" />
   </rule>
  </rule>
  <rule type="fa" cat="S[dcl]\NP">
   <lf start="2" span="1" word="must" lemma="must" pos="MD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[b]\NP)" />
   <rule type="fa" cat="S[b]\NP">
    <lf start="3" span="1" word="arrange" lemma="arrange" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/NP" />
    <rule type="ba" cat="NP">
     <rule type="lex" cat="NP">
      <lf start="4" span="1" word="characters" lemma="character" pos="NNS" chunk="I-NP" entity="O" cat="N" />
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="5" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
      <rule type="ba" cat="NP">
       <rule type="lex" cat="NP">
        <rule type="fa" cat="N">
         <lf start="6" span="1" word="contiguous" lemma="contiguous" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="7" span="1" word="groups" lemma="group" pos="NNS" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
       <rule type="fa" cat="NP\NP">
        <lf start="8" span="1" word="that" lemma="that" pos="WDT" chunk="B-NP" entity="O" cat="(NP\NP)/S[dcl]" />
        <rule type="rp" cat="S[dcl]">
         <rule type="ba" cat="S[dcl]">
          <rule type="fa" cat="NP[nb]">
           <lf start="9" span="1" word="each" lemma="each" pos="DT" chunk="B-NP" entity="O" cat="NP[nb]/N" />
           <lf start="10" span="1" word="group" lemma="group" pos="NN" chunk="I-NP" entity="O" cat="N" />
          </rule>
          <rule type="fa" cat="S[dcl]\NP">
           <lf start="11" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pss]\NP)" />
           <rule type="fa" cat="S[pss]\NP">
            <lf start="12" span="1" word="composed" lemma="compose" pos="VBN" chunk="I-VP" entity="O" cat="(S[pss]\NP)/PP" />
            <rule type="fa" cat="PP">
             <lf start="13" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
             <rule type="lex" cat="NP">
              <rule type="fa" cat="N">
               <lf start="14" span="1" word="identical" lemma="identical" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
               <lf start="15" span="1" word="characters" lemma="character" pos="NNS" chunk="I-NP" entity="O" cat="N" />
              </rule>
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
         <lf start="16" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
 </rule>
</ccg>
</candc>
