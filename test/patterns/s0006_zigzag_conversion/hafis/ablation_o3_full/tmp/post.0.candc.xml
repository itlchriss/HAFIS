<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <rule type="fa" cat="N">
     <lf start="0" span="1" word="String" lemma="string" pos="VBG" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="1" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="contains" lemma="contain" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/PP" />
    <rule type="fa" cat="PP">
     <lf start="3" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
     <rule type="ba" cat="NP">
      <rule type="fa" cat="NP[nb]">
       <lf start="4" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <rule type="fa" cat="N">
        <lf start="5" span="1" word="type_character_" lemma="type_character_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="6" span="1" word="chrs_a" lemma="chrs_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
      <rule type="lex" cat="NP\NP">
       <rule type="ba" cat="S[pss]\NP">
        <lf start="7" span="1" word="arranged" lemma="arrange" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
        <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
         <lf start="8" span="1" word="by" lemma="by" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/(S[ng]\NP)" />
         <rule type="ba" cat="S[ng]\NP">
          <rule type="fa" cat="S[ng]\NP">
           <lf start="9" span="1" word="writing" lemma="write" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
           <rule type="ba" cat="NP">
            <rule type="fa" cat="NP[nb]">
             <lf start="10" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
             <rule type="fa" cat="N">
              <lf start="11" span="1" word="type_character_" lemma="type_character_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
              <lf start="12" span="1" word="chrs_b" lemma="chrs_b" pos="NN" chunk="I-NP" entity="O" cat="N" />
             </rule>
            </rule>
            <rule type="fa" cat="NP\NP">
             <lf start="13" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
             <rule type="ba" cat="NP">
              <rule type="fa" cat="NP[nb]">
               <lf start="14" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
               <rule type="fa" cat="N">
                <lf start="15" span="1" word="zigzag" lemma="zigzag" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
                <lf start="16" span="1" word="pattern" lemma="pattern" pos="NN" chunk="I-NP" entity="O" cat="N" />
               </rule>
              </rule>
              <rule type="fa" cat="NP\NP">
               <lf start="17" span="1" word="across" lemma="across" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
               <rule type="fa" cat="NP[nb]">
                <lf start="18" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
                <rule type="fa" cat="N">
                 <lf start="19" span="1" word="type_string_" lemma="type_string_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
                 <rule type="fa" cat="N">
                  <lf start="20" span="1" word="strs_c" lemma="strs_c" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
                  <lf start="21" span="1" word="rows" lemma="row" pos="NNS" chunk="I-NP" entity="O" cat="N" />
                 </rule>
                </rule>
               </rule>
              </rule>
             </rule>
            </rule>
           </rule>
          </rule>
          <rule type="conj" cat="(S[ng]\NP)\(S[ng]\NP)">
           <lf start="22" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
           <rule type="fa" cat="S[ng]\NP">
            <lf start="23" span="1" word="then" lemma="then" pos="RB" chunk="I-VP" entity="O" cat="(S\NP)/(S\NP)" />
            <rule type="fa" cat="S[ng]\NP">
             <rule type="fa" cat="(S[ng]\NP)/PP">
              <lf start="24" span="1" word="concatenating" lemma="concatenate" pos="VBG" chunk="I-VP" entity="O" cat="((S[ng]\NP)/PP)/NP" />
              <rule type="ba" cat="NP">
               <rule type="fa" cat="NP[nb]">
                <lf start="25" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
                <lf start="26" span="1" word="rows" lemma="row" pos="NNS" chunk="I-NP" entity="O" cat="N" />
               </rule>
               <rule type="fa" cat="NP\NP">
                <lf start="27" span="1" word="from" lemma="from" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/(S[adj]\NP)" />
                <lf start="28" span="1" word="top" lemma="top" pos="JJ" chunk="I-ADJP" entity="O" cat="S[adj]\NP" />
               </rule>
              </rule>
             </rule>
             <rule type="fa" cat="PP">
              <lf start="29" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
              <rule type="lex" cat="NP">
               <lf start="30" span="1" word="bottom" lemma="bottom" pos="NN" chunk="I-NP" entity="O" cat="N" />
              </rule>
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="31" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
