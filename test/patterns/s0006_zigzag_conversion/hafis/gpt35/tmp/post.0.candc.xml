<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="type_string_" lemma="type_string_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pss]\NP)" />
    <rule type="ba" cat="S[pss]\NP">
     <lf start="4" span="1" word="formed" lemma="form" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="5" span="1" word="by" lemma="by" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/(S[ng]\NP)" />
      <rule type="fa" cat="S[ng]\NP">
       <rule type="fa" cat="(S[ng]\NP)/PP">
        <lf start="6" span="1" word="converting" lemma="convert" pos="VBG" chunk="I-VP" entity="O" cat="((S[ng]\NP)/PP)/NP" />
        <rule type="fa" cat="NP[nb]">
         <lf start="7" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <rule type="fa" cat="N">
          <lf start="8" span="1" word="type_string_" lemma="type_string_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="9" span="1" word="param_s_" lemma="param_s_" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
       <rule type="fa" cat="PP">
        <lf start="10" span="1" word="into" lemma="into" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
        <rule type="ba" cat="NP">
         <rule type="fa" cat="NP[nb]">
          <lf start="11" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
          <rule type="fa" cat="N">
           <lf start="12" span="1" word="zigzag" lemma="zigzag" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
           <lf start="13" span="1" word="pattern" lemma="pattern" pos="NN" chunk="I-NP" entity="O" cat="N" />
          </rule>
         </rule>
         <rule type="fa" cat="NP\NP">
          <lf start="14" span="1" word="with" lemma="with" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
          <rule type="ba" cat="NP">
           <rule type="fa" cat="NP[nb]">
            <lf start="15" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
            <rule type="fa" cat="N">
             <lf start="16" span="1" word="given" lemma="give" pos="VBN" chunk="I-NP" entity="O" cat="N/N" />
             <lf start="17" span="1" word="number" lemma="number" pos="NN" chunk="I-NP" entity="O" cat="N" />
            </rule>
           </rule>
           <rule type="fa" cat="NP\NP">
            <lf start="18" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
            <rule type="lex" cat="NP">
             <lf start="19" span="1" word="rows" lemma="row" pos="NNS" chunk="I-NP" entity="O" cat="N" />
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="20" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
