<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="type_string_" lemma="type_string_" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <rule type="bx" cat="(S[dcl]\NP)/(S[b]\NP)">
     <lf start="3" span="1" word="should" lemma="should" pos="MD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[b]\NP)" />
     <lf start="4" span="1" word="only" lemma="only" pos="RB" chunk="I-VP" entity="O" cat="(S\NP)\(S\NP)" />
    </rule>
    <rule type="fa" cat="S[b]\NP">
     <lf start="5" span="1" word="have" lemma="have" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/NP" />
     <rule type="ba" cat="NP">
      <rule type="fa" cat="NP[nb]">
       <lf start="6" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <rule type="fa" cat="N">
        <lf start="7" span="1" word="single" lemma="single" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="8" span="1" word="space" lemma="space" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
      <rule type="lex" cat="NP\NP">
       <rule type="fa" cat="S[ng]\NP">
        <lf start="9" span="1" word="separating" lemma="separate" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
        <rule type="fa" cat="NP[nb]">
         <lf start="10" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <lf start="11" span="1" word="words" lemma="word" pos="NNS" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="12" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
