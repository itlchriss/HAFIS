<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="String" lemma="string" pos="VBG" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="shall" lemma="shall" pos="MD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[b]\NP)" />
    <rule type="ba" cat="S[b]\NP">
     <lf start="4" span="1" word="collapse" lemma="collapse" pos="VB" chunk="I-VP" entity="O" cat="S[b]\NP" />
     <rule type="ba" cat="(S[X]\NP)\(S[X]\NP)">
      <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
       <lf start="5" span="1" word="every" lemma="every" pos="DT" chunk="I-NP" entity="O" cat="((S\NP)\(S\NP))/N" />
       <lf start="6" span="1" word="series" lemma="series" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
      <rule type="fa" cat="((S[X]\NP)\(S[X]\NP))\((S[X]\NP)\(S[X]\NP))">
       <lf start="7" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(((S\NP)\(S\NP))\((S\NP)\(S\NP)))/NP" />
       <rule type="ba" cat="NP">
        <rule type="lex" cat="NP">
         <rule type="fa" cat="N">
          <rule type="ba" cat="N/N">
           <lf start="8" span="1" word="2" lemma="2" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
           <rule type="conj" cat="(N/N)\(N/N)">
            <lf start="9" span="1" word="or" lemma="or" pos="CC" chunk="I-NP" entity="O" cat="conj" />
            <lf start="10" span="1" word="more" lemma="more" pos="JJR" chunk="I-NP" entity="O" cat="N/N" />
           </rule>
          </rule>
          <rule type="fa" cat="N">
           <lf start="11" span="1" word="consecutive" lemma="consecutive" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
           <rule type="fa" cat="N">
            <lf start="12" span="1" word="space" lemma="space" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
            <lf start="13" span="1" word="characters" lemma="character" pos="NNS" chunk="I-NP" entity="O" cat="N" />
           </rule>
          </rule>
         </rule>
        </rule>
        <rule type="fa" cat="NP\NP">
         <lf start="14" span="1" word="that" lemma="that" pos="WDT" chunk="B-NP" entity="O" cat="(NP\NP)/(S[dcl]\NP)" />
         <rule type="ba" cat="S[dcl]\NP">
          <lf start="15" span="1" word="occur" lemma="occur" pos="VBP" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
          <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
           <lf start="16" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
           <rule type="ba" cat="NP">
            <rule type="fa" cat="NP[nb]">
             <lf start="17" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
             <rule type="fa" cat="N">
              <lf start="18" span="1" word="type_character_" lemma="type_character_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
              <lf start="19" span="1" word="chrs_a" lemma="chrs_a" pos="NN" chunk="I-NP" entity="O" cat="N" />
             </rule>
            </rule>
            <rule type="fa" cat="NP\NP">
             <lf start="20" span="1" word="into" lemma="into" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
             <rule type="ba" cat="NP">
              <rule type="fa" cat="NP[nb]">
               <lf start="21" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
               <rule type="fa" cat="N">
                <lf start="22" span="1" word="single" lemma="single" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
                <rule type="fa" cat="N">
                 <lf start="23" span="1" word="space" lemma="space" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
                 <lf start="24" span="1" word="character" lemma="character" pos="NN" chunk="I-NP" entity="O" cat="N" />
                </rule>
               </rule>
              </rule>
              <rule type="lex" cat="NP\NP">
               <rule type="fa" cat="S[pss]\NP">
                <lf start="25" span="1" word="used" lemma="use" pos="VBN" chunk="I-VP" entity="O" cat="(S[pss]\NP)/PP" />
                <rule type="fa" cat="PP">
                 <lf start="26" span="1" word="as" lemma="as" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
                 <rule type="ba" cat="NP">
                  <rule type="fa" cat="NP[nb]">
                   <lf start="27" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
                   <lf start="28" span="1" word="separator" lemma="separator" pos="NN" chunk="I-NP" entity="O" cat="N" />
                  </rule>
                  <rule type="fa" cat="NP\NP">
                   <lf start="29" span="1" word="between" lemma="between" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
                   <rule type="lex" cat="NP">
                    <rule type="fa" cat="N">
                     <lf start="30" span="1" word="adjacent" lemma="adjacent" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
                     <lf start="31" span="1" word="words" lemma="word" pos="NNS" chunk="I-NP" entity="O" cat="N" />
                    </rule>
                   </rule>
                  </rule>
                 </rule>
                </rule>
               </rule>
              </rule>
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="32" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
