<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="type_string_" lemma="type_string_" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="param_s_" lemma="param_s_" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="only" lemma="only" pos="RB" chunk="I-ADVP" entity="O" cat="(S\NP)/(S\NP)" />
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="4" span="1" word="contains" lemma="contain" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
     <rule type="ba" cat="NP">
      <rule type="lex" cat="NP">
       <rule type="fa" cat="N">
        <lf start="5" span="1" word="alphabets" lemma="alphabet" pos="NNS" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="6" span="1" word="digits" lemma="digit" pos="NNS" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
      <rule type="conj" cat="NP\NP">
       <lf start="7" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
       <rule type="lex" cat="NP">
        <rule type="fa" cat="N">
         <lf start="8" span="1" word="space" lemma="space" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="9" span="1" word="characters" lemma="character" pos="NNS" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="10" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
