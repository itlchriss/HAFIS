<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <rule type="fa" cat="N">
     <lf start="0" span="1" word="boolean" lemma="boolean" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="1" span="1" word="result" lemma="result" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
    <rule type="fa" cat="S[adj]\NP">
     <lf start="3" span="1" word="equal" lemma="equal" pos="JJ" chunk="I-ADJP" entity="O" cat="(S[adj]\NP)/PP" />
     <rule type="fa" cat="PP">
      <lf start="4" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
      <rule type="ba" cat="NP">
       <rule type="fa" cat="NP[nb]">
        <lf start="5" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <rule type="fa" cat="N">
         <lf start="6" span="1" word="false" lemma="false" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="7" span="1" word="literal" lemma="literal" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
       <rule type="fa" cat="NP\NP">
        <lf start="8" span="1" word="when" lemma="when" pos="WRB" chunk="I-ADVP" entity="O" cat="(NP\NP)/S[dcl]" />
        <rule type="ba" cat="S[dcl]">
         <rule type="ba" cat="NP">
          <lf start="9" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP" />
          <rule type="lex" cat="NP\NP">
           <rule type="fa" cat="S[dcl]\NP">
            <lf start="10" span="1" word="described" lemma="describe" pos="VBN" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
            <rule type="lex" cat="NP">
             <rule type="fa" cat="N">
              <lf start="11" span="1" word="replacement" lemma="replacement" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
              <lf start="12" span="1" word="process" lemma="process" pos="NN" chunk="I-NP" entity="O" cat="N" />
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
         <rule type="fa" cat="S[dcl]\NP">
          <lf start="13" span="1" word="enters" lemma="enter" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
          <rule type="ba" cat="NP">
           <rule type="fa" cat="NP[nb]">
            <lf start="14" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
            <rule type="fa" cat="N">
             <lf start="15" span="1" word="recurring" lemma="recur" pos="VBG" chunk="I-NP" entity="O" cat="N/N" />
             <lf start="16" span="1" word="cycle" lemma="cycle" pos="NN" chunk="I-NP" entity="O" cat="N" />
            </rule>
           </rule>
           <rule type="fa" cat="NP\NP">
            <lf start="17" span="1" word="that" lemma="that" pos="WDT" chunk="B-NP" entity="O" cat="(NP\NP)/(S[dcl]\NP)" />
            <rule type="fa" cat="S[dcl]\NP">
             <lf start="18" span="1" word="never" lemma="never" pos="RB" chunk="I-ADVP" entity="O" cat="(S\NP)/(S\NP)" />
             <rule type="fa" cat="S[dcl]\NP">
              <lf start="19" span="1" word="produces" lemma="produce" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
              <rule type="fa" cat="NP[nb]">
               <lf start="20" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
               <rule type="ba" cat="N">
                <lf start="21" span="1" word="value" lemma="value" pos="NN" chunk="I-NP" entity="O" cat="N" />
                <lf start="22" span="1" word="1" lemma="1" pos="CD" chunk="I-NP" entity="I-DAT" cat="N\N" />
               </rule>
              </rule>
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="23" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
